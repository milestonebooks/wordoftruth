[{"id":1,"text":"\u7576\u4eba\u5728\u4e16\u4e0a\u591a\u8d77\u4f86\u3001\u53c8\u751f\u5973\u5152\u7684\u6642\u5019\u3001"},{"id":2,"text":"\u795e\u7684\u5152\u5b50\u5011\u770b\u898b\u4eba\u7684\u5973\u5b50\u7f8e\u8c8c\u3001\u5c31\u96a8\u610f\u6311\u9078\u3001\u5a36\u4f86\u70ba\u59bb\u3002"},{"id":3,"text":"\u8036\u548c\u83ef\u8aaa\u3001\u4eba\u65e2\u5c6c\u4e4e\u8840\u6c23\u3001\u6211\u7684\u9748\u5c31\u4e0d\u6c38\u9060\u4f4f\u5728\u4ed6\u88e1\u9762\u3001\u7136\u800c\u4ed6\u7684\u65e5\u5b50\u9084\u53ef\u5230\u4e00\u767e\u4e8c\u5341\u5e74\u3002"},{"id":4,"text":"\u90a3\u6642\u5019\u6709\u5049\u4eba\u5728\u5730\u4e0a\u3001\u5f8c\u4f86\u3000\u795e\u7684\u5152\u5b50\u5011\u3001\u548c\u4eba\u7684\u5973\u5b50\u5011\u4ea4\u5408\u751f\u5b50\u3001\u90a3\u5c31\u662f\u4e0a\u53e4\u82f1\u6b66\u6709\u540d\u7684\u4eba\u3002"},{"id":5,"text":"\u8036\u548c\u83ef\u898b\u4eba\u5728\u5730\u4e0a\u7f6a\u60e1\u5f88\u5927\u3001\u7d42\u65e5\u6240\u601d\u60f3\u7684\u76e1\u90fd\u662f\u60e1\uff0e"},{"id":6,"text":"\u8036\u548c\u83ef\u5c31\u5f8c\u6094\u9020\u4eba\u5728\u5730\u4e0a\u3001\u5fc3\u4e2d\u6182\u50b7\u3002"},{"id":7,"text":"\u8036\u548c\u83ef\u8aaa\u3001\u6211\u8981\u5c07\u6240\u9020\u7684\u4eba\u3001\u548c\u8d70\u7378\u3001\u4e26\u6606\u87f2\u3001\u4ee5\u53ca\u7a7a\u4e2d\u7684\u98db\u9ce5\u3001\u90fd\u5f9e\u5730\u4e0a\u9664\u6ec5\u3001\u56e0\u70ba\u6211\u9020\u4ed6\u5011\u5f8c\u6094\u4e86\u3002"},{"id":8,"text":"\u60df\u6709\u632a\u4e9e\u5728\u8036\u548c\u83ef\u773c\u524d\u8499\u6069\u3002"},{"id":9,"text":"\u632a\u4e9e\u7684\u5f8c\u4ee3\u3001\u8a18\u5728\u4e0b\u9762\u3002\u632a\u4e9e\u662f\u500b\u7fa9\u4eba\u3001\u5728\u7576\u6642\u7684\u4e16\u4ee3\u662f\u500b\u5b8c\u5168\u4eba\uff0e\u632a\u4e9e\u8207\u3000\u795e\u540c\u884c\u3002"},{"id":10,"text":"\u632a\u4e9e\u751f\u4e86\u4e09\u500b\u5152\u5b50\u3001\u5c31\u662f\u9583\u3001\u542b\u3001\u96c5\u5f17\u3002"},{"id":11,"text":"\u4e16\u754c\u5728\u3000\u795e\u9762\u524d\u6557\u58de\uff0e\u5730\u4e0a\u6eff\u4e86\u5f37\u66b4\u3002"},{"id":12,"text":"\u795e\u89c0\u770b\u4e16\u754c\u3001\u898b\u662f\u6557\u58de\u4e86\uff0e\u51e1\u6709\u8840\u6c23\u7684\u4eba\u3001\u5728\u5730\u4e0a\u90fd\u6557\u58de\u4e86\u884c\u70ba\u3002"},{"id":13,"text":"\u795e\u5c31\u5c0d\u632a\u4e9e\u8aaa\u3001\u51e1\u6709\u8840\u6c23\u7684\u4eba\u3001\u4ed6\u7684\u76e1\u982d\u5df2\u7d93\u4f86\u5230\u6211\u9762\u524d\u3001\u56e0\u70ba\u5730\u4e0a\u6eff\u4e86\u4ed6\u5011\u7684\u5f37\u66b4\u3001\u6211\u8981\u628a\u4ed6\u5011\u548c\u5730\u4e00\u4f75\u6bc0\u6ec5\u3002"},{"id":14,"text":"\u4f60\u8981\u7528\u6b4c\u6590\u6728\u9020\u4e00\u96bb\u65b9\u821f\u3001\u5206\u4e00\u9593\u4e00\u9593\u7684\u9020\u3001\u88e1\u5916\u62b9\u4e0a\u677e\u9999\u3002"},{"id":15,"text":"\u65b9\u821f\u7684\u9020\u6cd5\u4e43\u662f\u9019\u6a23\u3001\u8981\u9577\u4e09\u767e\u8098\u3001\u5bec\u4e94\u5341\u8098\u3001\u9ad8\u4e09\u5341\u8098\u3002"},{"id":16,"text":"\u65b9\u821f\u4e0a\u908a\u8981\u7559\u900f\u5149\u8655\u3001\u9ad8\u4e00\u8098\uff0e\u65b9\u821f\u7684\u9580\u8981\u958b\u5728\u65c1\u908a\uff0e\u65b9\u821f\u8981\u5206\u4e0a\u3001\u4e2d\u3001\u4e0b\u4e09\u5c64\u3002"},{"id":17,"text":"\u770b\u54ea\u3001\u6211\u8981\u4f7f\u6d2a\u6c34\u6c3e\u6feb\u5728\u5730\u4e0a\u3001\u6bc0\u6ec5\u5929\u4e0b\u3001\u51e1\u5730\u4e0a\u6709\u8840\u8089\u3001\u6709\u6c23\u606f\u7684\u6d3b\u7269\u3001\u7121\u4e00\u4e0d\u6b7b\u3002"},{"id":18,"text":"\u6211\u537b\u8981\u8207\u4f60\u7acb\u7d04\u3001\u4f60\u540c\u4f60\u7684\u59bb\u3001\u8207\u5152\u5b50\u3001\u5152\u5a66\u3001\u90fd\u8981\u9032\u5165\u65b9\u821f\u3002"},{"id":19,"text":"\u51e1\u6709\u8840\u8089\u7684\u6d3b\u7269\u3001\u6bcf\u6a23\u5169\u500b\u3001\u4e00\u516c\u4e00\u6bcd\u3001\u4f60\u8981\u5e36\u9032\u65b9\u821f\u3001\u597d\u5728\u4f60\u90a3\u88e1\u4fdd\u5168\u751f\u547d\u3002"},{"id":20,"text":"\u98db\u9ce5\u5404\u5f9e\u5176\u985e\u3001\u7272\u755c\u5404\u5f9e\u5176\u985e\u3001\u5730\u4e0a\u7684\u6606\u87f2\u5404\u5f9e\u5176\u985e\u3001\u6bcf\u6a23\u5169\u500b\u3001\u8981\u5230\u4f60\u90a3\u88e1\u3001\u597d\u4fdd\u5168\u751f\u547d\u3002"},{"id":21,"text":"\u4f60\u8981\u62ff\u5404\u6a23\u98df\u7269\u7a4d\u84c4\u8d77\u4f86\u3001\u597d\u4f5c\u4f60\u548c\u4ed6\u5011\u7684\u98df\u7269\u3002"},{"id":22,"text":"\u632a\u4e9e\u5c31\u9019\u6a23\u884c\uff0e\u51e1\u3000\u795e\u6240\u5429\u5490\u7684\u3001\u4ed6\u90fd\u7167\u6a23\u884c\u4e86\u3002"}]