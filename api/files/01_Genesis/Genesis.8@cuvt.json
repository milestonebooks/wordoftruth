[{"id":1,"text":"\u795e\u8a18\u5ff5\u632a\u4e9e\u3001\u548c\u632a\u4e9e\u65b9\u821f\u88e1\u7684\u4e00\u5207\u8d70\u7378\u7272\u755c\uff0e\u3000\u795e\u53eb\u98a8\u5439\u5730\u3001\u6c34\u52e2\u6f38\u843d\u3002"},{"id":2,"text":"\u6df5\u6e90\u548c\u5929\u4e0a\u7684\u7a97\u6236\u3001\u90fd\u9589\u585e\u4e86\u3001\u5929\u4e0a\u7684\u5927\u96e8\u4e5f\u6b62\u4f4f\u4e86\u3002"},{"id":3,"text":"\u6c34\u5f9e\u5730\u4e0a\u6f38\u9000\uff0e\u904e\u4e86\u4e00\u767e\u4e94\u5341\u5929\u3001\u6c34\u5c31\u6f38\u6d88\u3002"},{"id":4,"text":"\u4e03\u6708\u5341\u4e03\u65e5\u3001\u65b9\u821f\u505c\u5728\u4e9e\u62c9\u81d8\u5c71\u4e0a\u3002"},{"id":5,"text":"\u6c34\u53c8\u6f38\u6d88\u3001\u5230\u5341\u6708\u521d\u4e00\u65e5\u3001\u5c71\u9802\u90fd\u73fe\u51fa\u4f86\u4e86\u3002"},{"id":6,"text":"\u904e\u4e86\u56db\u5341\u5929\u3001\u632a\u4e9e\u958b\u4e86\u65b9\u821f\u7684\u7a97\u6236\u3001"},{"id":7,"text":"\u653e\u51fa\u4e00\u96bb\u70cf\u9d09\u53bb\u3001\u90a3\u70cf\u9d09\u98db\u4f86\u98db\u53bb\u3001\u76f4\u5230\u5730\u4e0a\u7684\u6c34\u90fd\u4e7e\u4e86\u3002"},{"id":8,"text":"\u4ed6\u53c8\u653e\u51fa\u4e00\u96bb\u9d3f\u5b50\u53bb\u3001\u8981\u770b\u770b\u6c34\u5f9e\u5730\u4e0a\u9000\u4e86\u6c92\u6709\u3002"},{"id":9,"text":"\u4f46\u904d\u5730\u4e0a\u90fd\u662f\u6c34\u3001\u9d3f\u5b50\u627e\u4e0d\u8457\u843d\u8173\u4e4b\u5730\u3001\u5c31\u56de\u5230\u65b9\u821f\u632a\u4e9e\u90a3\u88e1\u3001\u632a\u4e9e\u4f38\u624b\u628a\u9d3f\u5b50\u63a5\u9032\u65b9\u821f\u4f86\u3002"},{"id":10,"text":"\u4ed6\u53c8\u7b49\u4e86\u4e03\u5929\u3001\u518d\u628a\u9d3f\u5b50\u5f9e\u65b9\u821f\u653e\u51fa\u53bb\uff0e"},{"id":11,"text":"\u5230\u4e86\u665a\u4e0a\u3001\u9d3f\u5b50\u56de\u5230\u4ed6\u90a3\u88e1\u3001\u5634\u88e1\u53fc\u8457\u4e00\u500b\u65b0\u64f0\u4e0b\u4f86\u7684\u6a44\u6b16\u8449\u5b50\u3001\u632a\u4e9e\u5c31\u77e5\u9053\u5730\u4e0a\u7684\u6c34\u9000\u4e86\u3002"},{"id":12,"text":"\u4ed6\u53c8\u7b49\u4e86\u4e03\u5929\u3001\u653e\u51fa\u9d3f\u5b50\u53bb\u3001\u9d3f\u5b50\u5c31\u4e0d\u518d\u56de\u4f86\u4e86\u3002"},{"id":13,"text":"\u5230\u632a\u4e9e\u516d\u767e\u96f6\u4e00\u6b72\u3001\u6b63\u6708\u521d\u4e00\u65e5\u3001\u5730\u4e0a\u7684\u6c34\u90fd\u4e7e\u4e86\uff0e\u632a\u4e9e\u64a4\u53bb\u65b9\u821f\u7684\u84cb\u89c0\u770b\u3001\u4fbf\u898b\u5730\u9762\u4e0a\u4e7e\u4e86\u3002"},{"id":14,"text":"\u5230\u4e86\u4e8c\u6708\u4e8c\u5341\u4e03\u65e5\u3001\u5730\u5c31\u90fd\u4e7e\u4e86\u3002"},{"id":15,"text":"\u795e\u5c0d\u632a\u4e9e\u8aaa\u3001"},{"id":16,"text":"\u4f60\u548c\u4f60\u7684\u59bb\u5b50\u3001\u5152\u5b50\u3001\u5152\u5a66\u3001\u90fd\u53ef\u4ee5\u51fa\u65b9\u821f\u3002"},{"id":17,"text":"\u5728\u4f60\u90a3\u88e1\u51e1\u6709\u8840\u8089\u7684\u6d3b\u7269\u3001\u5c31\u662f\u98db\u9ce5\u3001\u7272\u755c\u3001\u548c\u4e00\u5207\u722c\u5728\u5730\u4e0a\u7684\u6606\u87f2\u3001\u90fd\u8981\u5e36\u51fa\u4f86\u3001\u53eb\u4ed6\u5728\u5730\u4e0a\u591a\u591a\u6ecb\u751f\u3001\u5927\u5927\u8208\u65fa\u3002"},{"id":18,"text":"\u65bc\u662f\u632a\u4e9e\u548c\u4ed6\u7684\u59bb\u5b50\u3001\u5152\u5b50\u3001\u5152\u5a66\u3001\u90fd\u51fa\u4f86\u4e86\u3002"},{"id":19,"text":"\u4e00\u5207\u8d70\u7378\u3001\u6606\u87f2\u3001\u98db\u9ce5\u3001\u548c\u5730\u4e0a\u6240\u6709\u7684\u52d5\u7269\u3001\u5404\u5f9e\u5176\u985e\u3001\u4e5f\u90fd\u51fa\u4e86\u65b9\u821f\u3002"},{"id":20,"text":"\u632a\u4e9e\u70ba\u8036\u548c\u83ef\u7bc9\u4e86\u4e00\u5ea7\u58c7\u3001\u62ff\u5404\u985e\u6f54\u6de8\u7684\u7272\u755c\u3001\u98db\u9ce5\u3001\u737b\u5728\u58c7\u4e0a\u70ba\u71d4\u796d\u3002"},{"id":21,"text":"\u8036\u548c\u83ef\u805e\u90a3\u99a8\u9999\u4e4b\u6c23\u3001\u5c31\u5fc3\u88e1\u8aaa\u3001\u6211\u4e0d\u518d\u56e0\u4eba\u7684\u7de3\u6545\u5492\u8a5b\u5730\u3001\uff08\u4eba\u5f9e\u5c0f\u6642\u5fc3\u88e1\u61f7\u8457\u60e1\u5ff5\uff09\u4e5f\u4e0d\u518d\u6309\u8457\u6211\u7e94\u884c\u7684\u3001\u6ec5\u5404\u7a2e\u7684\u6d3b\u7269\u4e86\u3002"},{"id":22,"text":"\u5730\u9084\u5b58\u7559\u7684\u6642\u5019\u3001\u7a3c\u7a61\u3001\u5bd2\u6691\u3001\u51ac\u590f\u3001\u665d\u591c\u3001\u5c31\u6c38\u4e0d\u505c\u606f\u4e86\u3002"}]