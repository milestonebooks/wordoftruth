[{"id":1,"text":"\u5929\u5730\u842c\u7269\u90fd\u9020\u9f4a\u4e86\u3002"},{"id":2,"text":"\u5230\u7b2c\u4e03\u65e5\u3001\u3000\u795e\u9020\u7269\u7684\u5de5\u5df2\u7d93\u5b8c\u7562\u3001\u5c31\u5728\u7b2c\u4e03\u65e5\u6b47\u4e86\u4ed6\u4e00\u5207\u7684\u5de5\u3001\u5b89\u606f\u4e86\u3002"},{"id":3,"text":"\u795e\u8cdc\u798f\u7d66\u7b2c\u4e03\u65e5\u3001\u5b9a\u70ba\u8056\u65e5\u3001\u56e0\u70ba\u5728\u9019\u65e5\u3000\u795e\u6b47\u4e86\u4ed6\u4e00\u5207\u5275\u9020\u7684\u5de5\u3001\u5c31\u5b89\u606f\u4e86\u3002"},{"id":4,"text":"\u5275\u9020\u5929\u5730\u7684\u4f86\u6b77\u3001\u5728\u8036\u548c\u83ef\u3000\u795e\u9020\u5929\u5730\u7684\u65e5\u5b50\u3001\u4e43\u662f\u9019\u6a23\uff0e"},{"id":5,"text":"\u91ce\u5730\u9084\u6c92\u6709\u8349\u6728\u3001\u7530\u9593\u7684\u83dc\u852c\u9084\u6c92\u6709\u9577\u8d77\u4f86\u3001\u56e0\u70ba\u8036\u548c\u83ef\u3000\u795e\u9084\u6c92\u6709\u964d\u96e8\u5728\u5730\u4e0a\u3001\u4e5f\u6c92\u6709\u4eba\u8015\u5730\u3002"},{"id":6,"text":"\u4f46\u6709\u9727\u6c23\u5f9e\u5730\u4e0a\u9a30\u3001\u6ecb\u6f64\u904d\u5730\u3002"},{"id":7,"text":"\u8036\u548c\u83ef\u3000\u795e\u7528\u5730\u4e0a\u7684\u5875\u571f\u9020\u4eba\u3001\u5c07\u751f\u6c23\u5439\u5728\u4ed6\u9f3b\u5b54\u88e1\u3001\u4ed6\u5c31\u6210\u4e86\u6709\u9748\u7684\u6d3b\u4eba\u3001\u540d\u53eb\u4e9e\u7576\u3002"},{"id":8,"text":"\u8036\u548c\u83ef\u3000\u795e\u5728\u6771\u65b9\u7684\u4f0a\u7538\u7acb\u4e86\u4e00\u500b\u5712\u5b50\u3001\u628a\u6240\u9020\u7684\u4eba\u5b89\u7f6e\u5728\u90a3\u88e1\u3002"},{"id":9,"text":"\u8036\u548c\u83ef\u3000\u795e\u4f7f\u5404\u6a23\u7684\u6a39\u5f9e\u5730\u88e1\u9577\u51fa\u4f86\u3001\u53ef\u4ee5\u6085\u4eba\u7684\u773c\u76ee\u3001\u5176\u4e0a\u7684\u679c\u5b50\u597d\u4f5c\u98df\u7269\uff0e\u5712\u5b50\u7576\u4e2d\u53c8\u6709\u751f\u547d\u6a39\u3001\u548c\u5206\u5225\u5584\u60e1\u7684\u6a39\u3002"},{"id":10,"text":"\u6709\u6cb3\u5f9e\u4f0a\u7538\u6d41\u51fa\u4f86\u6ecb\u6f64\u90a3\u5712\u5b50\u3001\u5f9e\u90a3\u88e1\u5206\u70ba\u56db\u9053\u3002"},{"id":11,"text":"\u7b2c\u4e00\u9053\u540d\u53eb\u6bd4\u905c\u3001\u5c31\u662f\u74b0\u7e5e\u54c8\u8153\u62c9\u5168\u5730\u7684\uff0e\u5728\u90a3\u88e1\u6709\u91d1\u5b50\u3001"},{"id":12,"text":"\u4e26\u4e14\u90a3\u5730\u7684\u91d1\u5b50\u662f\u597d\u7684\uff0e\u5728\u90a3\u88e1\u53c8\u6709\u73cd\u73e0\u548c\u7d05\u746a\u7459\u3002"},{"id":13,"text":"\u7b2c\u4e8c\u9053\u6cb3\u540d\u53eb\u57fa\u8a13\u3001\u5c31\u662f\u74b0\u7e5e\u53e4\u5be6\u5168\u5730\u7684\u3002"},{"id":14,"text":"\u7b2c\u4e09\u9053\u6cb3\u540d\u53eb\u5e0c\u5e95\u7d50\u3001\u6d41\u5728\u4e9e\u8ff0\u7684\u6771\u908a\u3002\u7b2c\u56db\u9053\u6cb3\u5c31\u662f\u4f2f\u62c9\u6cb3\u3002"},{"id":15,"text":"\u8036\u548c\u83ef\u3000\u795e\u5c07\u90a3\u4eba\u5b89\u7f6e\u5728\u4f0a\u7538\u5712\u3001\u4f7f\u4ed6\u4fee\u7406\u770b\u5b88\u3002"},{"id":16,"text":"\u8036\u548c\u83ef\u3000\u795e\u5429\u5490\u4ed6\u8aaa\u3001\u5712\u4e2d\u5404\u6a23\u6a39\u4e0a\u7684\u679c\u5b50\u3001\u4f60\u53ef\u4ee5\u96a8\u610f\u55ab\uff0e"},{"id":17,"text":"\u53ea\u662f\u5206\u5225\u5584\u60e1\u6a39\u4e0a\u7684\u679c\u5b50\u3001\u4f60\u4e0d\u53ef\u55ab\u3001\u56e0\u70ba\u4f60\u55ab\u7684\u65e5\u5b50\u5fc5\u5b9a\u6b7b\u3002"},{"id":18,"text":"\u8036\u548c\u83ef\u3000\u795e\u8aaa\u3001\u90a3\u4eba\u7368\u5c45\u4e0d\u597d\u3001\u6211\u8981\u70ba\u4ed6\u9020\u4e00\u500b\u914d\u5076\u5e6b\u52a9\u4ed6\u3002"},{"id":19,"text":"\u8036\u548c\u83ef\u3000\u795e\u7528\u571f\u6240\u9020\u6210\u7684\u91ce\u5730\u5404\u6a23\u8d70\u7378\u3001\u548c\u7a7a\u4e2d\u5404\u6a23\u98db\u9ce5\u3001\u90fd\u5e36\u5230\u90a3\u4eba\u9762\u524d\u770b\u4ed6\u53eb\u751a\u9ebc\uff0e\u90a3\u4eba\u600e\u6a23\u53eb\u5404\u6a23\u7684\u6d3b\u7269\u3001\u90a3\u5c31\u662f\u4ed6\u7684\u540d\u5b57\u3002"},{"id":20,"text":"\u90a3\u4eba\u4fbf\u7d66\u4e00\u5207\u7272\u755c\u3001\u548c\u7a7a\u4e2d\u98db\u9ce5\u3001\u91ce\u5730\u8d70\u7378\u90fd\u8d77\u4e86\u540d\uff0e\u53ea\u662f\u90a3\u4eba\u6c92\u6709\u9047\u898b\u914d\u5076\u5e6b\u52a9\u4ed6\u3002"},{"id":21,"text":"\u8036\u548c\u83ef\u3000\u795e\u4f7f\u4ed6\u6c89\u7761\u3001\u4ed6\u5c31\u7761\u4e86\uff0e\u65bc\u662f\u53d6\u4e0b\u4ed6\u7684\u4e00\u689d\u808b\u9aa8\u3001\u53c8\u628a\u8089\u5408\u8d77\u4f86\u3002"},{"id":22,"text":"\u8036\u548c\u83ef\u3000\u795e\u5c31\u7528\u90a3\u4eba\u8eab\u4e0a\u6240\u53d6\u7684\u808b\u9aa8\u3001\u9020\u6210\u4e00\u500b\u5973\u4eba\u3001\u9818\u4ed6\u5230\u90a3\u4eba\u8ddf\u524d\u3002"},{"id":23,"text":"\u90a3\u4eba\u8aaa\u3001\u9019\u662f\u6211\u9aa8\u4e2d\u7684\u9aa8\u3001\u8089\u4e2d\u7684\u8089\u3001\u53ef\u4ee5\u7a31\u4ed6\u70ba\u5973\u4eba\u3001\u56e0\u70ba\u4ed6\u662f\u5f9e\u7537\u4eba\u8eab\u4e0a\u53d6\u51fa\u4f86\u7684\u3002"},{"id":24,"text":"\u56e0\u6b64\u3001\u4eba\u8981\u96e2\u958b\u7236\u6bcd\u3001\u8207\u59bb\u5b50\u9023\u5408\u3001\u4e8c\u4eba\u6210\u70ba\u4e00\u9ad4\u3002"},{"id":25,"text":"\u7576\u6642\u592b\u59bb\u4e8c\u4eba\u3001\u8d64\u8eab\u9732\u9ad4\u3001\u4e26\u4e0d\u7f9e\u6065\u3002"}]