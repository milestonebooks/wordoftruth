[{"id":1,"text":"\u5929\u5730\u4e07\u7269\u90fd\u9020\u9f50\u4e86\u3002"},{"id":2,"text":"\u5230\u7b2c\u4e03\u65e5\uff0c\u795e\u9020\u7269\u7684\u5de5\u5df2\u7ecf\u5b8c\u6bd5\uff0c\u5c31\u5728\u7b2c\u4e03\u65e5\u6b47\u4e86\u4ed6\u4e00\u5207\u7684\u5de5\uff0c\u5b89\u606f\u4e86\u3002"},{"id":3,"text":"\u795e\u8d50\u798f\u7ed9\u7b2c\u4e03\u65e5\uff0c\u5b9a\u4e3a\u5723\u65e5\uff0c\u56e0\u4e3a\u5728\u8fd9\u65e5\u795e\u6b47\u4e86\u4ed6\u4e00\u5207\u521b\u9020\u7684\u5de5\uff0c\u5c31\u5b89\u606f\u4e86\u3002"},{"id":4,"text":"\u521b\u9020\u5929\u5730\u7684\u6765\u5386\uff0c\u5728\u8036\u548c\u534e\u795e\u9020\u5929\u5730\u7684\u65e5\u5b50\uff0c\u4e43\u662f\u8fd9\u6837\u3002"},{"id":5,"text":"\u91ce\u5730\u8fd8\u6ca1\u6709\u8349\u6728\uff0c\u7530\u95f4\u7684\u83dc\u852c\u8fd8\u6ca1\u6709\u957f\u8d77\u6765\uff0c\u56e0\u4e3a\u8036\u548c\u534e\u795e\u8fd8\u6ca1\u6709\u964d\u96e8\u5728\u5730\u4e0a\uff0c\u4e5f\u6ca1\u6709\u4eba\u8015\u5730\u3002"},{"id":6,"text":"\u4f46\u6709\u96fe\u6c14\u4ece\u5730\u4e0a\u817e\uff0c\u6ecb\u6da6\u904d\u5730\u3002"},{"id":7,"text":"\u8036\u548c\u534e\u795e\u7528\u5730\u4e0a\u7684\u5c18\u571f\u9020\u4eba\uff0c\u5c06\u751f\u6c14\u5439\u5728\u4ed6\u9f3b\u5b54\u91cc\uff0c\u4ed6\u5c31\u6210\u4e86\u6709\u7075\u7684\u6d3b\u4eba\uff0c\u540d\u53eb\u4e9a\u5f53\u3002"},{"id":8,"text":"\u8036\u548c\u534e\u795e\u5728\u4e1c\u65b9\u7684\u4f0a\u7538\u7acb\u4e86\u4e00\u4e2a\u56ed\u5b50\uff0c\u628a\u6240\u9020\u7684\u4eba\u5b89\u7f6e\u5728\u90a3\u91cc\u3002"},{"id":9,"text":"\u8036\u548c\u534e\u795e\u4f7f\u5404\u6837\u7684\u6811\u4ece\u5730\u91cc\u957f\u51fa\u6765\uff0c\u53ef\u4ee5\u60a6\u4eba\u7684\u773c\u76ee\uff0c\u5176\u4e0a\u7684\u679c\u5b50\u597d\u4f5c\u98df\u7269\u3002\u56ed\u5b50\u5f53\u4e2d\u53c8\u6709\u751f\u547d\u6811\u548c\u5206\u522b\u5584\u6076\u7684\u6811\u3002"},{"id":10,"text":"\u6709\u6cb3\u4ece\u4f0a\u7538\u6d41\u51fa\u6765\uff0c\u6ecb\u6da6\u90a3\u56ed\u5b50\uff0c\u4ece\u90a3\u91cc\u5206\u4e3a\u56db\u9053\u3002"},{"id":11,"text":"\u7b2c\u4e00\u9053\u540d\u53eb\u6bd4\u900a\uff0c\u5c31\u662f\u73af\u7ed5\u54c8\u8153\u62c9\u5168\u5730\u7684\u3002\u5728\u90a3\u91cc\u6709\u91d1\u5b50\uff0c"},{"id":12,"text":"\u5e76\u4e14\u90a3\u5730\u7684\u91d1\u5b50\u662f\u597d\u7684\u3002\u5728\u90a3\u91cc\u53c8\u6709\u73cd\u73e0\u548c\u7ea2\u739b\u7459\u3002"},{"id":13,"text":"\u7b2c\u4e8c\u9053\u6cb3\u540d\u53eb\u57fa\u8bad\uff0c\u5c31\u662f\u73af\u7ed5\u53e4\u5b9e\u5168\u5730\u7684\u3002"},{"id":14,"text":"\u7b2c\u4e09\u9053\u6cb3\u540d\u53eb\u897f\u5e95\u7ed3\uff0c\u6d41\u5728\u4e9a\u8ff0\u7684\u4e1c\u8fb9\u3002\u7b2c\u56db\u9053\u6cb3\u5c31\u662f\u4f2f\u62c9\u6cb3\u3002"},{"id":15,"text":"\u8036\u548c\u534e\u795e\u5c06\u90a3\u4eba\u5b89\u7f6e\u5728\u4f0a\u7538\u56ed\uff0c\u4f7f\u4ed6\u4fee\u7406\u770b\u5b88\u3002"},{"id":16,"text":"\u8036\u548c\u534e\u795e\u5429\u5490\u4ed6\u8bf4\uff0c\u56ed\u4e2d\u5404\u6837\u6811\u4e0a\u7684\u679c\u5b50\uff0c\u4f60\u53ef\u4ee5\u968f\u610f\u5403\u3002"},{"id":17,"text":"\u53ea\u662f\u5206\u522b\u5584\u6076\u6811\u4e0a\u7684\u679c\u5b50\uff0c\u4f60\u4e0d\u53ef\u5403\uff0c\u56e0\u4e3a\u4f60\u5403\u7684\u65e5\u5b50\u5fc5\u5b9a\u6b7b\u3002"},{"id":18,"text":"\u8036\u548c\u534e\u795e\u8bf4\uff0c\u90a3\u4eba\u72ec\u5c45\u4e0d\u597d\uff0c\u6211\u8981\u4e3a\u4ed6\u9020\u4e00\u4e2a\u914d\u5076\u5e2e\u52a9\u4ed6\u3002"},{"id":19,"text":"\u8036\u548c\u534e\u795e\u7528\u571f\u6240\u9020\u6210\u7684\u91ce\u5730\u5404\u6837\u8d70\u517d\u548c\u7a7a\u4e2d\u5404\u6837\u98de\u9e1f\u90fd\u5e26\u5230\u90a3\u4eba\u9762\u524d\uff0c\u770b\u4ed6\u53eb\u4ec0\u4e48\u3002\u90a3\u4eba\u600e\u6837\u53eb\u5404\u6837\u7684\u6d3b\u7269\uff0c\u90a3\u5c31\u9972\u5b83\u7684\u540d\u5b57\u3002"},{"id":20,"text":"\u90a3\u4eba\u4fbf\u7ed9\u4e00\u5207\u7272\u755c\u548c\u7a7a\u4e2d\u98de\u9e1f\uff0c\u91ce\u5730\u8d70\u517d\u90fd\u8d77\u4e86\u540d\u3002\u53ea\u662f\u90a3\u4eba\u6ca1\u6709\u9047\u89c1\u914d\u5076\u5e2e\u52a9\u4ed6\u3002"},{"id":21,"text":"\u8036\u548c\u534e\u795e\u4f7f\u4ed6\u6c89\u7761\uff0c\u4ed6\u5c31\u7761\u4e86\u3002\u4e8e\u662f\u53d6\u4e0b\u4ed6\u7684\u4e00\u6761\u808b\u9aa8\uff0c\u53c8\u628a\u8089\u5408\u8d77\u6765\u3002"},{"id":22,"text":"\u8036\u548c\u534e\u795e\u5c31\u7528\u90a3\u4eba\u8eab\u4e0a\u6240\u53d6\u7684\u808b\u9aa8\uff0c\u9020\u6210\u4e00\u4e2a\u5973\u4eba\uff0c\u9886\u5979\u5230\u90a3\u4eba\u8ddf\u524d\u3002"},{"id":23,"text":"\u90a3\u4eba\u8bf4\uff0c\u8fd9\u662f\u6211\u9aa8\u4e2d\u7684\u9aa8\uff0c\u8089\u4e2d\u7684\u8089\uff0c\u53ef\u4ee5\u79f0\u5979\u4e3a\u5973\u4eba\uff0c\u56e0\u4e3a\u5979\u662f\u4ece\u7537\u4eba\u8eab\u4e0a\u53d6\u51fa\u6765\u7684\u3002"},{"id":24,"text":"\u56e0\u6b64\uff0c\u4eba\u8981\u79bb\u5f00\u7236\u6bcd\u4e0e\u59bb\u5b50\u8fde\u5408\uff0c\u4e8c\u4eba\u6210\u4e3a\u4e00\u4f53\u3002"},{"id":25,"text":"\u5f53\u65f6\u592b\u59bb\u4e8c\u4eba\u8d64\u8eab\u9732\u4f53\uff0c\u5e76\u4e0d\u7f9e\u803b\u3002"}]