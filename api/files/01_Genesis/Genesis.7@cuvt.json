[{"id":1,"text":"\u8036\u548c\u83ef\u5c0d\u632a\u4e9e\u8aaa\u3001\u4f60\u548c\u4f60\u7684\u5168\u5bb6\u90fd\u8981\u9032\u5165\u65b9\u821f\u3001\u56e0\u70ba\u5728\u9019\u4e16\u4ee3\u4e2d\u3001\u6211\u898b\u4f60\u5728\u6211\u9762\u524d\u662f\u7fa9\u4eba\u3002"},{"id":2,"text":"\u51e1\u6f54\u6de8\u7684\u755c\u985e\u3001\u4f60\u8981\u5e36\u4e03\u516c\u4e03\u6bcd\uff0e\u4e0d\u6f54\u6de8\u7684\u755c\u985e\u3001\u4f60\u8981\u5e36\u4e00\u516c\u4e00\u6bcd\uff0e"},{"id":3,"text":"\u7a7a\u4e2d\u7684\u98db\u9ce5\u3001\u4e5f\u8981\u5e36\u4e03\u516c\u4e03\u6bcd\u3001\u53ef\u4ee5\u7559\u7a2e\u3001\u6d3b\u5728\u5168\u5730\u4e0a\u3001"},{"id":4,"text":"\u56e0\u70ba\u518d\u904e\u4e03\u5929\u3001\u6211\u8981\u964d\u96e8\u5728\u5730\u4e0a\u56db\u5341\u665d\u591c\u3001\u628a\u6211\u6240\u9020\u7684\u5404\u7a2e\u6d3b\u7269\u3001\u90fd\u5f9e\u5730\u4e0a\u9664\u6ec5\u3002"},{"id":5,"text":"\u632a\u4e9e\u5c31\u9075\u8457\u8036\u548c\u83ef\u6240\u5429\u5490\u7684\u884c\u4e86\u3002"},{"id":6,"text":"\u7576\u6d2a\u6c34\u6c3e\u6feb\u5728\u5730\u4e0a\u7684\u6642\u5019\u3001\u632a\u4e9e\u6574\u516d\u767e\u6b72\u3002"},{"id":7,"text":"\u632a\u4e9e\u5c31\u540c\u4ed6\u7684\u59bb\u3001\u548c\u5152\u5b50\u3001\u5152\u5a66\u3001\u90fd\u9032\u5165\u65b9\u821f\u3001\u8eb2\u907f\u6d2a\u6c34\u3002"},{"id":8,"text":"\u6f54\u6de8\u7684\u755c\u985e\u3001\u548c\u4e0d\u6f54\u6de8\u7684\u755c\u985e\u3001\u98db\u9ce5\u4e26\u5730\u4e0a\u4e00\u5207\u7684\u6606\u87f2\u3001"},{"id":9,"text":"\u90fd\u662f\u4e00\u5c0d\u4e00\u5c0d\u7684\u3001\u6709\u516c\u6709\u6bcd\u3001\u5230\u632a\u4e9e\u90a3\u88e1\u9032\u5165\u65b9\u821f\u3001\u6b63\u5982\u3000\u795e\u6240\u5429\u5490\u632a\u4e9e\u7684\u3002"},{"id":10,"text":"\u904e\u4e86\u90a3\u4e03\u5929\u3001\u6d2a\u6c34\u6c3e\u6feb\u5728\u5730\u4e0a\u3002"},{"id":11,"text":"\u7576\u632a\u4e9e\u516d\u767e\u6b72\u3001\u4e8c\u6708\u5341\u4e03\u65e5\u90a3\u4e00\u5929\u3001\u5927\u6df5\u7684\u6cc9\u6e90\u3001\u90fd\u88c2\u958b\u4e86\u3001\u5929\u4e0a\u7684\u7a97\u6236\u3001\u4e5f\u655e\u958b\u4e86\u3002"},{"id":12,"text":"\u56db\u5341\u665d\u591c\u964d\u5927\u96e8\u5728\u5730\u4e0a\u3002"},{"id":13,"text":"\u6b63\u7576\u90a3\u65e5\u3001\u632a\u4e9e\u548c\u4ed6\u4e09\u500b\u5152\u5b50\u3001\u9583\u3001\u542b\u3001\u96c5\u5f17\u3001\u4e26\u632a\u4e9e\u7684\u59bb\u5b50\u3001\u548c\u4e09\u500b\u5152\u5a66\u3001\u90fd\u9032\u5165\u65b9\u821f\u3002"},{"id":14,"text":"\u4ed6\u5011\u548c\u767e\u7378\u3001\u5404\u5f9e\u5176\u985e\u3001\u4e00\u5207\u7272\u755c\u3001\u5404\u5f9e\u5176\u985e\u3001\u722c\u5728\u5730\u4e0a\u7684\u6606\u87f2\u3001\u5404\u5f9e\u5176\u985e\u3001\u4e00\u5207\u79bd\u9ce5\u3001\u5404\u5f9e\u5176\u985e\u3001\u90fd\u9032\u5165\u65b9\u821f\u3002"},{"id":15,"text":"\u51e1\u6709\u8840\u8089\u3001\u6709\u6c23\u606f\u7684\u6d3b\u7269\u3001\u90fd\u4e00\u5c0d\u4e00\u5c0d\u7684\u5230\u632a\u4e9e\u90a3\u88e1\u3001\u9032\u5165\u65b9\u821f\u3002"},{"id":16,"text":"\u51e1\u6709\u8840\u8089\u9032\u5165\u65b9\u821f\u7684\u3001\u90fd\u662f\u6709\u516c\u6709\u6bcd\u3001\u6b63\u5982\u3000\u795e\u6240\u5429\u5490\u632a\u4e9e\u7684\uff0e\u8036\u548c\u83ef\u5c31\u628a\u4ed6\u95dc\u5728\u65b9\u821f\u88e1\u982d\u3002"},{"id":17,"text":"\u6d2a\u6c34\u6c3e\u6feb\u5728\u5730\u4e0a\u56db\u5341\u5929\u3001\u6c34\u5f80\u4e0a\u9577\u3001\u628a\u65b9\u821f\u5f9e\u5730\u4e0a\u6f02\u8d77\u3002"},{"id":18,"text":"\u6c34\u52e2\u6d69\u5927\u3001\u5728\u5730\u4e0a\u5927\u5927\u7684\u5f80\u4e0a\u9577\u3001\u65b9\u821f\u5728\u6c34\u9762\u4e0a\u6f02\u4f86\u6f02\u53bb\u3002"},{"id":19,"text":"\u6c34\u52e2\u5728\u5730\u4e0a\u6975\u5176\u6d69\u5927\u3001\u5929\u4e0b\u7684\u9ad8\u5c71\u90fd\u6df9\u6c92\u4e86\u3002"},{"id":20,"text":"\u6c34\u52e2\u6bd4\u5c71\u9ad8\u904e\u5341\u4e94\u8098\u3001\u5c71\u5dba\u90fd\u6df9\u6c92\u4e86\u3002"},{"id":21,"text":"\u51e1\u5728\u5730\u4e0a\u6709\u8840\u8089\u7684\u52d5\u7269\u3001\u5c31\u662f\u98db\u9ce5\u3001\u7272\u755c\u3001\u8d70\u7378\u3001\u548c\u722c\u5728\u5730\u4e0a\u7684\u6606\u87f2\u3001\u4ee5\u53ca\u6240\u6709\u7684\u4eba\u90fd\u6b7b\u4e86\u3002"},{"id":22,"text":"\u51e1\u5728\u65f1\u5730\u4e0a\u3001\u9f3b\u5b54\u6709\u6c23\u606f\u7684\u751f\u9748\u90fd\u6b7b\u4e86\u3002"},{"id":23,"text":"\u51e1\u5730\u4e0a\u5404\u985e\u7684\u6d3b\u7269\u3001\u9023\u4eba\u5e36\u7272\u755c\u3001\u6606\u87f2\u3001\u4ee5\u53ca\u7a7a\u4e2d\u7684\u98db\u9ce5\u3001\u90fd\u5f9e\u5730\u4e0a\u9664\u6ec5\u4e86\u3001\u53ea\u7559\u4e0b\u632a\u4e9e\u548c\u90a3\u4e9b\u8207\u4ed6\u540c\u5728\u65b9\u821f\u88e1\u7684\u3002"},{"id":24,"text":"\u6c34\u52e2\u6d69\u5927\u3001\u5728\u5730\u4e0a\u5171\u4e00\u767e\u4e94\u5341\u5929\u3002"}]