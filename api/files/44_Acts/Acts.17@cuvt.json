[{"id":1,"text":"\u4fdd\u7f85\u548c\u897f\u62c9\u3001\u7d93\u904e\u6697\u5983\u6ce2\u91cc\u3001\u4e9e\u6ce2\u7f85\u5c3c\u4e9e\u3001\u4f86\u5230\u5e16\u6492\u7f85\u5c3c\u8fe6\u3001\u5728\u90a3\u88e1\u6709\u7336\u592a\u4eba\u7684\u6703\u5802\u3002"},{"id":2,"text":"\u4fdd\u7f85\u7167\u4ed6\u7d20\u5e38\u7684\u898f\u77e9\u9032\u53bb\u3001\u4e00\u9023\u4e09\u500b\u5b89\u606f\u65e5\u3001\u672c\u8457\u8056\u7d93\u8207\u4ed6\u5011\u8faf\u8ad6\u3001"},{"id":3,"text":"\u8b1b\u89e3\u9673\u660e\u57fa\u7763\u5fc5\u9808\u53d7\u5bb3\u3001\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\uff0e\u53c8\u8aaa\u3001\u6211\u6240\u50b3\u8207\u4f60\u5011\u7684\u9019\u7acb\u8036\u7a4c\u3001\u5c31\u662f\u57fa\u7763\u3002"},{"id":4,"text":"\u4ed6\u5011\u4e2d\u9593\u6709\u4e9b\u4eba\u807d\u4e86\u52f8\u3001\u5c31\u9644\u5f9e\u4fdd\u7f85\u548c\u897f\u62c9\uff0e\u4e26\u6709\u8a31\u591a\u8654\u656c\u7684\u5e0c\u5229\u5c3c\u4eba\u3001\u5c0a\u8cb4\u7684\u5a66\u5973\u4e5f\u4e0d\u5c11\u3002"},{"id":5,"text":"\u4f46\u90a3\u4e0d\u4fe1\u7684\u7336\u592a\u4eba\u5fc3\u88e1\u5ac9\u5992\u3001\u62db\u805a\u4e86\u4e9b\u5e02\u4e95\u532a\u985e\u3001\u642d\u5925\u6210\u7fa4\u3001\u8073\u52d5\u5408\u57ce\u7684\u4eba\u3001\u95d6\u9032\u8036\u5b6b\u7684\u5bb6\u3001\u8981\u5c07\u4fdd\u7f85\u897f\u62c9\u5e36\u5230\u767e\u59d3\u90a3\u88e1\u3002"},{"id":6,"text":"\u627e\u4e0d\u8457\u4ed6\u5011\u3001\u5c31\u628a\u8036\u5b6b\u3001\u548c\u5e7e\u500b\u5f1f\u5144\u3001\u62c9\u5230\u5730\u65b9\u5b98\u90a3\u88e1\u3001\u558a\u53eb\u8aaa\u3001\u90a3\u652a\u4e82\u5929\u4e0b\u7684\u3001\u4e5f\u5230\u9019\u88e1\u4f86\u4e86\uff0e"},{"id":7,"text":"\u8036\u5b6b\u6536\u7559\u4ed6\u5011\uff0e\u9019\u4e9b\u4eba\u90fd\u9055\u80cc\u8a72\u6492\u7684\u547d\u4ee4\u3001\u8aaa\u53e6\u6709\u4e00\u500b\u738b\u8036\u7a4c\u3002"},{"id":8,"text":"\u773e\u4eba\u548c\u5730\u65b9\u5b98\u3001\u807d\u898b\u9019\u8a71\u3001\u5c31\u9a5a\u614c\u4e86\u3002"},{"id":9,"text":"\u65bc\u662f\u53d6\u4e86\u8036\u5b6b\u548c\u5176\u9918\u4e4b\u4eba\u7684\u4fdd\u72c0\u3001\u5c31\u91cb\u653e\u4e86\u4ed6\u5011\u3002"},{"id":10,"text":"\u5f1f\u5144\u5011\u3001\u96a8\u5373\u5728\u591c\u9593\u6253\u767c\u4fdd\u7f85\u548c\u897f\u62c9\u5f80\u5e87\u54e9\u4e9e\u53bb\uff0e\u4e8c\u4eba\u5230\u4e86\u3001\u5c31\u9032\u5165\u7336\u592a\u4eba\u7684\u6703\u5802\u3002"},{"id":11,"text":"\u9019\u5730\u65b9\u7684\u4eba\u3001\u8ce2\u65bc\u5e16\u6492\u7f85\u5c3c\u8fe6\u7684\u4eba\u3001\u7518\u5fc3\u9818\u53d7\u9019\u9053\u3001\u5929\u5929\u8003\u67e5\u8056\u7d93\u3001\u8981\u66c9\u5f97\u9019\u9053\u662f\u8207\u4e0d\u662f\u3002"},{"id":12,"text":"\u6240\u4ee5\u4ed6\u5011\u4e2d\u9593\u591a\u6709\u76f8\u4fe1\u7684\uff0e\u53c8\u6709\u5e0c\u5229\u5c3c\u5c0a\u8cb4\u7684\u5a66\u5973\u3001\u7537\u5b50\u4e5f\u4e0d\u5c11\u3002"},{"id":13,"text":"\u4f46\u5e16\u6492\u7f85\u5c3c\u8fe6\u7684\u7336\u592a\u4eba\u3001\u77e5\u9053\u4fdd\u7f85\u53c8\u5e87\u54e9\u4e9e\u50b3\u3000\u795e\u7684\u9053\u3001\u4e5f\u5c31\u5f80\u90a3\u88e1\u53bb\u3001\u8073\u52d5\u652a\u64fe\u773e\u4eba\u3002"},{"id":14,"text":"\u7576\u6642\u5f1f\u5144\u5011\u4fbf\u6253\u767c\u4fdd\u7f85\u5f80\u6d77\u908a\u53bb\uff0e\u897f\u62c9\u548c\u63d0\u6469\u592a\u4ecd\u4f4f\u5728\u5e87\u54e9\u4e9e\u3002"},{"id":15,"text":"\u9001\u4fdd\u7f85\u7684\u4eba\u5e36\u4ed6\u5230\u4e86\u96c5\u5178\uff0e\u65e2\u9818\u4e86\u4fdd\u7f85\u7684\u547d\u3001\u53eb\u897f\u62c9\u548c\u63d0\u6469\u592a\u901f\u901f\u5230\u4ed6\u9019\u88e1\u4f86\u3001\u5c31\u56de\u53bb\u4e86\u3002"},{"id":16,"text":"\u4fdd\u7f85\u5728\u96c5\u5178\u7b49\u5019\u4ed6\u5011\u7684\u6642\u5019\u3001\u770b\u898b\u6eff\u57ce\u90fd\u662f\u5076\u50cf\u3001\u5c31\u5fc3\u88e1\u8457\u6025\u3002"},{"id":17,"text":"\u65bc\u662f\u5728\u6703\u5802\u88e1\u3001\u8207\u7336\u592a\u4eba\u3001\u548c\u8654\u656c\u7684\u4eba\u3001\u4e26\u6bcf\u65e5\u5728\u5e02\u4e0a\u6240\u9047\u898b\u7684\u4eba\u8faf\u8ad6\u3002"},{"id":18,"text":"\u9084\u6709\u4ee5\u5f7c\u53e4\u7f85\u548c\u65af\u591a\u4e9e\u5169\u9580\u7684\u5b78\u58eb\u3001\u8207\u4ed6\u722d\u8ad6\uff0e\u6709\u7684\u8aaa\u3001\u9019\u80e1\u8a00\u4e82\u8a9e\u7684\u8981\u8aaa\u751a\u9ebc\uff0e\u6709\u7684\u8aaa\u3001\u4ed6\u4f3c\u4e4e\u662f\u50b3\u8aaa\u5916\u90a6\u9b3c\u795e\u7684\uff0e\u9019\u8a71\u662f\u56e0\u4fdd\u7f85\u50b3\u8b1b\u8036\u7a4c\u3001\u8207\u5fa9\u6d3b\u7684\u9053\u3002"},{"id":19,"text":"\u4ed6\u5011\u5c31\u628a\u4ed6\u5e36\u5230\u4e9e\u7565\u5df4\u53e4\u8aaa\u3001\u4f60\u6240\u8b1b\u7684\u9019\u65b0\u9053\u3001\u6211\u5011\u4e5f\u53ef\u4ee5\u77e5\u9053\u9ebc\uff0e"},{"id":20,"text":"\u56e0\u70ba\u4f60\u6709\u4e9b\u5947\u602a\u7684\u4e8b\u3001\u50b3\u5230\u6211\u5011\u8033\u4e2d\uff0e\u6211\u5011\u9858\u610f\u77e5\u9053\u9019\u4e9b\u4e8b\u662f\u751a\u9ebc\u610f\u601d\u3002"},{"id":21,"text":"\uff08\u96c5\u5178\u4eba\u3001\u548c\u4f4f\u5728\u90a3\u88e1\u7684\u5ba2\u4eba\u3001\u90fd\u4e0d\u9867\u5225\u7684\u4e8b\u3001\u53ea\u5c07\u65b0\u805e\u8aaa\u8aaa\u807d\u807d\u3002\uff09"},{"id":22,"text":"\u4fdd\u7f85\u7ad9\u5728\u4e9e\u7565\u5df4\u53e4\u7576\u4e2d\u3001\u8aaa\u3001\u773e\u4f4d\u96c5\u5178\u4eba\u54ea\u3001\u6211\u770b\u4f60\u5011\u51e1\u4e8b\u5f88\u656c\u754f\u9b3c\u795e\u3002"},{"id":23,"text":"\u6211\u904a\u884c\u7684\u6642\u5019\u3001\u89c0\u770b\u4f60\u5011\u6240\u656c\u62dc\u7684\u3001\u9047\u898b\u4e00\u5ea7\u58c7\u3001\u4e0a\u9762\u5beb\u8457\u672a\u8b58\u4e4b\u795e\uff0e\u4f60\u5011\u6240\u4e0d\u8a8d\u8b58\u800c\u656c\u62dc\u7684\u3001\u6211\u73fe\u5728\u544a\u8a34\u4f60\u5011\u3002"},{"id":24,"text":"\u5275\u9020\u5b87\u5b99\u548c\u5176\u4e2d\u842c\u7269\u7684\u3000\u795e\u3001\u65e2\u662f\u5929\u5730\u7684\u4e3b\u3001\u5c31\u4e0d\u4f4f\u4eba\u624b\u6240\u9020\u7684\u6bbf\uff0e"},{"id":25,"text":"\u4e5f\u4e0d\u7528\u4eba\u624b\u670d\u4e8b\u3001\u597d\u50cf\u7f3a\u5c11\u751a\u9ebc\u3001\u81ea\u5df1\u5012\u5c07\u751f\u547d\u3001\u6c23\u606f\u3001\u842c\u7269\u3001\u8cdc\u7d66\u842c\u4eba\u3002"},{"id":26,"text":"\u4ed6\u5f9e\u4e00\u672c\u9020\u51fa\u842c\u65cf\u7684\u4eba\u3001\u3014\u672c\u6709\u53e4\u5377\u4f5c\u8840\u8108\u3015\u4f4f\u5728\u5168\u5730\u4e0a\u3001\u4e26\u4e14\u8c6b\u5148\u5b9a\u6e96\u4ed6\u5011\u7684\u5e74\u9650\u3001\u548c\u6240\u4f4f\u7684\u7586\u754c\uff0e"},{"id":27,"text":"\u8981\u53eb\u4ed6\u5011\u5c0b\u6c42\u3000\u795e\u3001\u6216\u8005\u53ef\u4ee5\u63e3\u6469\u800c\u5f97\u3001\u5176\u5be6\u4ed6\u96e2\u6211\u5011\u5404\u4eba\u4e0d\u9060\uff0e"},{"id":28,"text":"\u6211\u5011\u751f\u6d3b\u3001\u52d5\u4f5c\u3001\u5b58\u7559\u3001\u90fd\u5728\u4e4e\u4ed6\uff0e\u5c31\u5982\u4f60\u5011\u4f5c\u8a69\u7684\u3001\u6709\u4eba\u8aaa\u3001\u6211\u5011\u4e5f\u662f\u4ed6\u6240\u751f\u7684\u3002"},{"id":29,"text":"\u6211\u5011\u65e2\u662f\u3000\u795e\u6240\u751f\u7684\u3001\u5c31\u4e0d\u7576\u4ee5\u70ba\u3000\u795e\u7684\u795e\u6027\u50cf\u4eba\u7528\u624b\u85dd\u3001\u5fc3\u601d\u3001\u6240\u96d5\u523b\u7684\u91d1\u3001\u9280\u3001\u77f3\u3002"},{"id":30,"text":"\u4e16\u4eba\u8499\u6627\u7121\u77e5\u7684\u6642\u5019\u3001\u3000\u795e\u4e26\u4e0d\u76e3\u5bdf\u3001\u5982\u4eca\u537b\u5429\u5490\u5404\u8655\u7684\u4eba\u90fd\u8981\u6094\u6539\u3002"},{"id":31,"text":"\u56e0\u70ba\u4ed6\u5df2\u7d93\u5b9a\u4e86\u65e5\u5b50\u3001\u8981\u85c9\u8457\u4ed6\u6240\u8a2d\u7acb\u7684\u4eba\u3001\u6309\u516c\u7fa9\u5be9\u5224\u5929\u4e0b\uff0e\u4e26\u4e14\u53eb\u4ed6\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u3001\u7d66\u842c\u4eba\u4f5c\u53ef\u4fe1\u7684\u6191\u64da\u3002"},{"id":32,"text":"\u773e\u4eba\u807d\u898b\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u7684\u8a71\u3001\u5c31\u6709\u8b4f\u8a9a\u4ed6\u7684\uff0e\u53c8\u6709\u4eba\u8aaa\u3001\u6211\u5011\u518d\u807d\u4f60\u8b1b\u9019\u500b\u7f77\u3002"},{"id":33,"text":"\u65bc\u662f\u4fdd\u7f85\u5f9e\u4ed6\u5011\u7576\u4e2d\u51fa\u53bb\u4e86\u3002"},{"id":34,"text":"\u4f46\u6709\u5e7e\u500b\u4eba\u8cbc\u8fd1\u4ed6\u3001\u4fe1\u4e86\u4e3b\u3001\u5176\u4e2d\u6709\u4e9e\u7565\u5df4\u53e4\u7684\u5b98\u4e1f\u5c3c\u4fee\u3001\u4e26\u4e00\u500b\u5a66\u4eba\u3001\u540d\u53eb\u5927\u99ac\u54e9\u3001\u9084\u6709\u5225\u4eba\u4e00\u540c\u4fe1\u5f9e\u3002"}]