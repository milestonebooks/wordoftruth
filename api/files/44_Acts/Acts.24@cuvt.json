[{"id":1,"text":"\u904e\u4e86\u4e94\u5929\u3001\u5927\u796d\u53f8\u4e9e\u62ff\u5c3c\u4e9e\u3001\u540c\u5e7e\u500b\u9577\u8001\u3001\u548c\u4e00\u500b\u8faf\u58eb\u5e16\u58eb\u7f85\u3001\u4e0b\u4f86\u3001\u5411\u5de1\u64ab\u63a7\u544a\u4fdd\u7f85\uff0e"},{"id":2,"text":"\u4fdd\u7f85\u88ab\u63d0\u4e86\u4f86\u3001\u5e16\u58eb\u7f85\u5c31\u544a\u4ed6\u8aaa\u3001"},{"id":3,"text":"\u8153\u529b\u65af\u5927\u4eba\u3001\u6211\u5011\u56e0\u4f60\u5f97\u4ee5\u5927\u4eab\u592a\u5e73\u3001\u4e26\u4e14\u9019\u4e00\u570b\u7684\u5f0a\u75c5\u3001\u56e0\u8457\u4f60\u7684\u5148\u898b\u3001\u5f97\u4ee5\u66f4\u6b63\u4e86\uff0e\u6211\u5011\u96a8\u6642\u96a8\u5730\u3001\u6eff\u5fc3\u611f\u8b1d\u4e0d\u76e1\u3002"},{"id":4,"text":"\u60df\u6050\u591a\u8aaa\u3001\u4f60\u5acc\u7169\u7d6e\u3001\u53ea\u6c42\u4f60\u5bec\u5bb9\u807d\u6211\u5011\u8aaa\u5e7e\u53e5\u8a71\u3002"},{"id":5,"text":"\u6211\u5011\u770b\u9019\u500b\u4eba\u3001\u5982\u540c\u761f\u75ab\u4e00\u822c\u3001\u662f\u9f13\u52d5\u666e\u5929\u4e0b\u773e\u7336\u592a\u4eba\u751f\u4e82\u7684\u3001\u53c8\u662f\u62ff\u6492\u52d2\u6559\u9ee8\u88e1\u7684\u4e00\u500b\u982d\u76ee\uff0e"},{"id":6,"text":"\u9023\u8056\u6bbf\u4ed6\u4e5f\u60f3\u8981\u6c61\u7a62\uff0e\u6211\u5011\u628a\u4ed6\u6349\u4f4f\u4e86\uff0e\u3014\u6709\u53e4\u5377\u5728\u6b64\u6709\u8981\u6309\u6211\u5011\u7684\u5f8b\u6cd5\u5be9\u554f\u3015"},{"id":7,"text":"\u3014\u4e0d\u6599\u5343\u592b\u9577\u5442\u897f\u4e9e\u524d\u4f86\u751a\u662f\u5f37\u6a6b\u5f9e\u6211\u5011\u624b\u4e2d\u628a\u4ed6\u596a\u53bb\u5429\u5490\u544a\u4ed6\u7684\u4eba\u5230\u4f60\u9019\u88e1\u4f86\u3015"},{"id":8,"text":"\u4f60\u81ea\u5df1\u7a76\u554f\u4ed6\u3001\u5c31\u53ef\u4ee5\u77e5\u9053\u6211\u5011\u544a\u4ed6\u7684\u4e00\u5207\u4e8b\u4e86\u3002"},{"id":9,"text":"\u773e\u7336\u592a\u4eba\u4e5f\u96a8\u8457\u544a\u4ed6\u8aaa\u3001\u4e8b\u60c5\u8aa0\u7136\u662f\u9019\u6a23\u3002"},{"id":10,"text":"\u5de1\u64ab\u9ede\u982d\u53eb\u4fdd\u7f85\u8aaa\u8a71\u3001\u4ed6\u5c31\u8aaa\u3001\u6211\u77e5\u9053\u4f60\u5728\u9019\u570b\u88e1\u65b7\u4e8b\u591a\u5e74\u3001\u6240\u4ee5\u6211\u6a02\u610f\u70ba\u81ea\u5df1\u5206\u8a34\u3002"},{"id":11,"text":"\u4f60\u67e5\u554f\u5c31\u53ef\u4ee5\u77e5\u9053\u3001\u5f9e\u6211\u4e0a\u8036\u8def\u6492\u51b7\u79ae\u62dc\u3001\u5230\u4eca\u65e5\u3001\u4e0d\u904e\u6709\u5341\u4e8c\u5929\uff0e"},{"id":12,"text":"\u4ed6\u5011\u4e26\u6c92\u6709\u770b\u898b\u6211\u5728\u6bbf\u88e1\u3001\u6216\u662f\u5728\u6703\u5802\u88e1\u3001\u6216\u662f\u5728\u57ce\u88e1\u3001\u548c\u4eba\u8faf\u8ad6\u3001\u8073\u52d5\u773e\u4eba\u3002"},{"id":13,"text":"\u4ed6\u5011\u73fe\u5728\u6240\u544a\u6211\u7684\u4e8b\u3001\u4e26\u4e0d\u80fd\u5c0d\u4f60\u8b49\u5be6\u4e86\u3002"},{"id":14,"text":"\u4f46\u6709\u4e00\u4ef6\u4e8b\u3001\u6211\u5411\u4f60\u627f\u8a8d\u3001\u5c31\u662f\u4ed6\u5011\u6240\u7a31\u70ba\u7570\u7aef\u7684\u9053\u3001\u6211\u6b63\u6309\u8457\u90a3\u9053\u4e8b\u5949\u6211\u7956\u5b97\u7684\u3000\u795e\u3001\u53c8\u4fe1\u5408\u4e4e\u5f8b\u6cd5\u7684\u3001\u548c\u5148\u77e5\u66f8\u4e0a\u4e00\u5207\u6240\u8a18\u8f09\u7684\uff0e"},{"id":15,"text":"\u4e26\u4e14\u9760\u8457\u3000\u795e\u3001\u76fc\u671b\u6b7b\u4eba\u3001\u7121\u8ad6\u5584\u60e1\u3001\u90fd\u8981\u5fa9\u6d3b\u3001\u5c31\u662f\u4ed6\u5011\u81ea\u5df1\u4e5f\u6709\u9019\u500b\u76fc\u671b\u3002"},{"id":16,"text":"\u6211\u56e0\u6b64\u81ea\u5df1\u52c9\u52f5\u3001\u5c0d\u3000\u795e\u3001\u5c0d\u4eba\u3001\u5e38\u5b58\u7121\u8667\u7684\u826f\u5fc3\u3002"},{"id":17,"text":"\u904e\u4e86\u5e7e\u5e74\u3001\u6211\u5e36\u8457\u8cd9\u6fdf\u672c\u570b\u7684\u6350\u9805\u548c\u4f9b\u737b\u7684\u7269\u4e0a\u53bb\u3002"},{"id":18,"text":"\u6b63\u737b\u7684\u6642\u5019\u3001\u4ed6\u5011\u770b\u898b\u6211\u5728\u6bbf\u88e1\u5df2\u7d93\u6f54\u6de8\u4e86\u3001\u4e26\u6c92\u6709\u805a\u773e\u3001\u4e5f\u6c92\u6709\u5435\u56b7\uff0e\u60df\u6709\u5e7e\u500b\u5f9e\u4e9e\u897f\u4e9e\u4f86\u7684\u7336\u592a\u4eba\uff0e"},{"id":19,"text":"\u4ed6\u5011\u82e5\u6709\u544a\u6211\u7684\u4e8b\u3001\u5c31\u61c9\u7576\u5230\u4f60\u9762\u524d\u4f86\u544a\u6211\u3002"},{"id":20,"text":"\u5373\u6216\u4e0d\u7136\u3001\u9019\u4e9b\u4eba\u3001\u82e5\u770b\u51fa\u6211\u7ad9\u5728\u516c\u6703\u524d\u3001\u6709\u5984\u70ba\u7684\u5730\u65b9\u3001\u4ed6\u5011\u81ea\u5df1\u4e5f\u53ef\u4ee5\u8aaa\u660e\u3002"},{"id":21,"text":"\u7e31\u7136\u6709\u3001\u4e5f\u4e0d\u904e\u4e00\u53e5\u8a71\u3001\u5c31\u662f\u6211\u7ad9\u5728\u4ed6\u5011\u4e2d\u9593\u5927\u8072\u8aaa\u3001\u6211\u4eca\u65e5\u5728\u4f60\u5011\u9762\u524d\u53d7\u5be9\u3001\u662f\u70ba\u6b7b\u4eba\u5fa9\u6d3b\u7684\u9053\u7406\u3002"},{"id":22,"text":"\u8153\u529b\u65af\u672c\u662f\u8a73\u7d30\u66c9\u5f97\u9019\u9053\u3001\u5c31\u652f\u543e\u4ed6\u5011\u8aaa\u3001\u4e14\u7b49\u5343\u592b\u9577\u5442\u897f\u4e9e\u4e0b\u4f86\u3001\u6211\u8981\u5be9\u65b7\u4f60\u5011\u7684\u4e8b\u3002"},{"id":23,"text":"\u65bc\u662f\u5429\u5490\u767e\u592b\u9577\u770b\u5b88\u4fdd\u7f85\u3001\u4e26\u4e14\u5bec\u5f85\u4ed6\u3001\u4e5f\u4e0d\u6514\u963b\u4ed6\u7684\u89aa\u53cb\u4f86\u4f9b\u7d66\u4ed6\u3002"},{"id":24,"text":"\u904e\u4e86\u5e7e\u5929\u3001\u8153\u529b\u65af\u548c\u4ed6\u592b\u4eba\u7336\u592a\u7684\u5973\u5b50\u571f\u897f\u62c9\u3001\u4e00\u540c\u4f86\u5230\u3001\u5c31\u53eb\u4e86\u4fdd\u7f85\u4f86\u3001\u807d\u4ed6\u8b1b\u8ad6\u4fe1\u57fa\u7763\u8036\u7a4c\u7684\u9053\u3002"},{"id":25,"text":"\u4fdd\u7f85\u8b1b\u8ad6\u516c\u7fa9\u3001\u7bc0\u5236\u3001\u548c\u5c07\u4f86\u7684\u5be9\u5224\u3001\u8153\u529b\u65af\u751a\u89ba\u6050\u61fc\u3001\u8aaa\u3001\u4f60\u66ab\u4e14\u53bb\u7f77\u3001\u7b49\u6211\u5f97\u4fbf\u518d\u53eb\u4f60\u4f86\u3002"},{"id":26,"text":"\u8153\u529b\u65af\u53c8\u6307\u671b\u4fdd\u7f85\u9001\u4ed6\u9280\u9322\u3001\u6240\u4ee5\u5c62\u6b21\u53eb\u4ed6\u4f86\u3001\u548c\u4ed6\u8ac7\u8ad6\u3002"},{"id":27,"text":"\u904e\u4e86\u5169\u5e74\u3001\u6ce2\u6c42\u975e\u65af\u90fd\u63a5\u4e86\u8153\u529b\u65af\u7684\u4efb\uff0e\u8153\u529b\u65af\u8981\u8a0e\u7336\u592a\u4eba\u7684\u559c\u6b61\u3001\u5c31\u7559\u4fdd\u7f85\u5728\u76e3\u88e1\u3002"}]