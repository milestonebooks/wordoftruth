[{"id":1,"text":"\u8af8\u4f4d\u7236\u5144\u8acb\u807d\u3001\u6211\u73fe\u5728\u5c0d\u4f60\u5011\u5206\u8a34\u3002"},{"id":2,"text":"\u773e\u4eba\u807d\u4ed6\u8aaa\u7684\u662f\u5e0c\u4f2f\u4f86\u8a71\u3001\u5c31\u66f4\u52a0\u5b89\u975c\u4e86\u3002"},{"id":3,"text":"\u4fdd\u7f85\u8aaa\u3001\u6211\u539f\u662f\u7336\u592a\u4eba\u3001\u751f\u5728\u57fa\u5229\u5bb6\u7684\u5927\u6578\u3001\u9577\u5728\u9019\u57ce\u88e1\u3001\u5728\u8fe6\u746a\u5217\u9580\u4e0b\u3001\u6309\u8457\u6211\u5011\u7956\u5b97\u56b4\u7dca\u7684\u5f8b\u6cd5\u53d7\u6559\u3001\u71b1\u5fc3\u4e8b\u5949\u3000\u795e\u3001\u50cf\u4f60\u5011\u773e\u4eba\u4eca\u65e5\u4e00\u6a23\u3002"},{"id":4,"text":"\u6211\u4e5f\u66fe\u903c\u8feb\u5949\u9019\u9053\u7684\u4eba\u3001\u76f4\u5230\u6b7b\u5730\u3001\u7121\u8ad6\u7537\u5973\u90fd\u9396\u62ff\u4e0b\u76e3\u3002"},{"id":5,"text":"\u9019\u662f\u5927\u796d\u53f8\u548c\u773e\u9577\u8001\u90fd\u53ef\u4ee5\u7d66\u6211\u4f5c\u898b\u8b49\u7684\uff0e\u6211\u53c8\u9818\u4e86\u4ed6\u5011\u9054\u8207\u5f1f\u5144\u7684\u66f8\u4fe1\u3001\u5f80\u5927\u99ac\u8272\u53bb\u3001\u8981\u628a\u5728\u90a3\u88e1\u5949\u9019\u9053\u7684\u4eba\u9396\u62ff\u3001\u5e36\u5230\u8036\u8def\u6492\u51b7\u53d7\u5211\u3002"},{"id":6,"text":"\u6211\u5c07\u5230\u5927\u99ac\u8272\u3001\u6b63\u8d70\u7684\u6642\u5019\u3001\u7d04\u5728\u664c\u5348\u3001\u5ffd\u7136\u5f9e\u5929\u4e0a\u767c\u5927\u5149\u3001\u56db\u9762\u7167\u8457\u6211\u3002"},{"id":7,"text":"\u6211\u5c31\u4ec6\u5012\u5728\u5730\u3001\u807d\u898b\u6709\u8072\u97f3\u5c0d\u6211\u8aaa\u3001\u6383\u7f85\u3001\u6383\u7f85\u3001\u4f60\u70ba\u751a\u9ebc\u903c\u8feb\u6211\u3002"},{"id":8,"text":"\u6211\u56de\u7b54\u8aaa\u3001\u4e3b\u963f\u3001\u4f60\u662f\u8ab0\u3002\u4ed6\u8aaa\u3001\u6211\u5c31\u662f\u4f60\u6240\u903c\u8feb\u7684\u62ff\u6492\u52d2\u4eba\u8036\u7a4c\u3002"},{"id":9,"text":"\u8207\u6211\u540c\u884c\u7684\u4eba\u3001\u770b\u898b\u4e86\u90a3\u5149\u3001\u537b\u6c92\u6709\u807d\u660e\u90a3\u4f4d\u5c0d\u6211\u8aaa\u8a71\u7684\u8072\u97f3\u3002"},{"id":10,"text":"\u6211\u8aaa\u3001\u4e3b\u963f\u3001\u6211\u7576\u4f5c\u751a\u9ebc\u3002\u4e3b\u8aaa\u3001\u8d77\u4f86\u3001\u9032\u5927\u99ac\u8272\u53bb\u3001\u5728\u90a3\u88e1\u8981\u5c07\u6240\u6d3e\u4f60\u4f5c\u7684\u4e00\u5207\u4e8b\u3001\u544a\u8a34\u4f60\u3002"},{"id":11,"text":"\u6211\u56e0\u90a3\u5149\u7684\u69ae\u8000\u3001\u4e0d\u80fd\u770b\u898b\u3001\u540c\u884c\u7684\u4eba\u3001\u5c31\u62c9\u8457\u6211\u624b\u9032\u4e86\u5927\u99ac\u8272\u3002"},{"id":12,"text":"\u90a3\u88e1\u6709\u4e00\u500b\u4eba\u3001\u540d\u53eb\u4e9e\u62ff\u5c3c\u4e9e\u3001\u6309\u8457\u5f8b\u6cd5\u662f\u8654\u8aa0\u4eba\u3001\u70ba\u4e00\u5207\u4f4f\u5728\u90a3\u88e1\u7684\u7336\u592a\u4eba\u6240\u7a31\u8b9a\u3002"},{"id":13,"text":"\u4ed6\u4f86\u898b\u6211\u3001\u7ad9\u5728\u65c1\u908a\u3001\u5c0d\u6211\u8aaa\u3001\u5144\u5f1f\u6383\u7f85\u3001\u4f60\u53ef\u4ee5\u770b\u898b\uff0e\u6211\u7576\u6642\u5f80\u4e0a\u4e00\u770b\u3001\u5c31\u770b\u898b\u4e86\u4ed6\u3002"},{"id":14,"text":"\u4ed6\u53c8\u8aaa\u3001\u6211\u5011\u7956\u5b97\u7684\u3000\u795e\u3001\u63c0\u9078\u4e86\u4f60\u3001\u53eb\u4f60\u660e\u767d\u4ed6\u7684\u65e8\u610f\u3001\u53c8\u5f97\u898b\u90a3\u7fa9\u8005\u3001\u807d\u4ed6\u53e3\u4e2d\u6240\u51fa\u7684\u8072\u97f3\u3002"},{"id":15,"text":"\u56e0\u70ba\u4f60\u8981\u5c07\u6240\u770b\u898b\u7684\u3001\u6240\u807d\u898b\u7684\u3001\u5c0d\u8457\u842c\u4eba\u70ba\u4ed6\u4f5c\u898b\u8b49\u3002"},{"id":16,"text":"\u73fe\u5728\u4f60\u70ba\u751a\u9ebc\u803d\u5ef6\u5462\u3001\u8d77\u4f86\u3001\u6c42\u544a\u4ed6\u7684\u540d\u53d7\u6d17\u3001\u6d17\u53bb\u4f60\u7684\u7f6a\u3002"},{"id":17,"text":"\u5f8c\u4f86\u6211\u56de\u5230\u8036\u8def\u6492\u51b7\u3001\u5728\u6bbf\u88e1\u79b1\u544a\u7684\u6642\u5019\u3001\u9b42\u904a\u8c61\u5916\u3001"},{"id":18,"text":"\u770b\u898b\u4e3b\u5411\u6211\u8aaa\u3001\u4f60\u8d95\u7dca\u7684\u96e2\u958b\u8036\u8def\u6492\u51b7\u3001\u4e0d\u53ef\u9072\u5ef6\u3001\u56e0\u4f60\u70ba\u6211\u4f5c\u7684\u898b\u8b49\u3001\u9019\u88e1\u7684\u4eba\u3001\u5fc5\u4e0d\u9818\u53d7\u3002"},{"id":19,"text":"\u6211\u5c31\u8aaa\u3001\u4e3b\u963f\u3001\u4ed6\u5011\u77e5\u9053\u6211\u5f9e\u524d\u628a\u4fe1\u4f60\u7684\u4eba\u3001\u6536\u5728\u76e3\u88e1\u3001\u53c8\u5728\u5404\u6703\u5802\u88e1\u97ad\u6253\u4ed6\u5011\uff0e"},{"id":20,"text":"\u4e26\u4e14\u4f60\u7684\u898b\u8b49\u4eba\u53f8\u63d0\u53cd\u3001\u88ab\u5bb3\u6d41\u8840\u7684\u6642\u5019\u3001\u6211\u4e5f\u7ad9\u5728\u65c1\u908a\u6b61\u559c\uff0e\u53c8\u770b\u5b88\u5bb3\u6b7b\u4ed6\u4e4b\u4eba\u7684\u8863\u88f3\u3002"},{"id":21,"text":"\u4e3b\u5411\u6211\u8aaa\u3001\u4f60\u53bb\u7f77\uff0e\u6211\u8981\u5dee\u4f60\u9060\u9060\u7684\u5f80\u5916\u90a6\u4eba\u90a3\u88e1\u53bb\u3002"},{"id":22,"text":"\u773e\u4eba\u807d\u4ed6\u8aaa\u5230\u9019\u53e5\u8a71\u3001\u5c31\u9ad8\u8072\u8aaa\u3001\u9019\u6a23\u7684\u4eba\u3001\u5f9e\u4e16\u4e0a\u9664\u6389\u4ed6\u7f77\uff0e\u4ed6\u662f\u4e0d\u7576\u6d3b\u8457\u7684\u3002"},{"id":23,"text":"\u773e\u4eba\u55a7\u56b7\u3001\u6454\u6389\u8863\u88f3\u3001\u628a\u5875\u571f\u5411\u7a7a\u4e2d\u63da\u8d77\u4f86\u3002"},{"id":24,"text":"\u5343\u592b\u9577\u5c31\u5429\u5490\u5c07\u4fdd\u7f85\u5e36\u9032\u71df\u6a13\u53bb\u3001\u53eb\u4eba\u7528\u97ad\u5b50\u62f7\u554f\u4ed6\u3001\u8981\u77e5\u9053\u4ed6\u5011\u5411\u4ed6\u9019\u6a23\u55a7\u56b7\u3001\u662f\u70ba\u751a\u9ebc\u7de3\u6545\u3002"},{"id":25,"text":"\u525b\u7528\u76ae\u689d\u6346\u4e0a\u3001\u4fdd\u7f85\u5c0d\u65c1\u908a\u7ad9\u8457\u7684\u767e\u592b\u9577\u8aaa\u3001\u4eba\u662f\u7f85\u99ac\u4eba\u3001\u53c8\u6c92\u6709\u5b9a\u7f6a\u3001\u4f60\u5011\u5c31\u97ad\u6253\u4ed6\u3001\u6709\u9019\u500b\u4f8b\u3002"},{"id":26,"text":"\u767e\u592b\u9577\u807d\u898b\u9019\u8a71\u3001\u5c31\u53bb\u898b\u5343\u592b\u9577\u3001\u544a\u8a34\u4ed6\u8aaa\u3001\u4f60\u8981\u4f5c\u751a\u9ebc\uff0e\u9019\u4eba\u662f\u7f85\u99ac\u4eba\u3002"},{"id":27,"text":"\u5343\u592b\u9577\u5c31\u4f86\u554f\u4fdd\u7f85\u8aaa\u3001\u4f60\u544a\u8a34\u6211\u3001\u4f60\u662f\u7f85\u99ac\u4eba\u9ebc\u3002\u4fdd\u7f85\u8aaa\u3001\u662f\u3002"},{"id":28,"text":"\u5343\u592b\u9577\u8aaa\u3001\u6211\u7528\u8a31\u591a\u9280\u5b50\u3001\u7e94\u5165\u4e86\u7f85\u99ac\u7684\u6c11\u85c9\u3002\u4fdd\u7f85\u8aaa\u3001\u6211\u751f\u4f86\u5c31\u662f\u3002"},{"id":29,"text":"\u65bc\u662f\u90a3\u4e9b\u8981\u62f7\u554f\u4fdd\u7f85\u7684\u4eba\u3001\u5c31\u96e2\u958b\u4ed6\u53bb\u4e86\u3002\u5343\u592b\u9577\u65e2\u77e5\u9053\u4ed6\u662f\u7f85\u99ac\u4eba\u3001\u53c8\u56e0\u70ba\u6346\u7d81\u4e86\u4ed6\u3001\u4e5f\u5bb3\u6015\u4e86\u3002"},{"id":30,"text":"\u7b2c\u4e8c\u5929\u3001\u5343\u592b\u9577\u70ba\u8981\u77e5\u9053\u7336\u592a\u4eba\u63a7\u544a\u4fdd\u7f85\u7684\u5be6\u60c5\u3001\u4fbf\u89e3\u958b\u4ed6\u3001\u5429\u5490\u796d\u53f8\u9577\u548c\u5168\u516c\u6703\u7684\u4eba\u3001\u90fd\u805a\u96c6\u3001\u5c07\u4fdd\u7f85\u5e36\u4e0b\u4f86\u3001\u53eb\u4ed6\u7ad9\u5728\u4ed6\u5011\u9762\u524d\u3002"}]