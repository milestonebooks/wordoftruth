[{"id":1,"text":"\u4e9e\u57fa\u5e15\u5c0d\u4fdd\u7f85\u8aaa\u3001\u51c6\u4f60\u70ba\u81ea\u5df1\u8faf\u660e\u3002\u65bc\u662f\u4fdd\u7f85\u4f38\u624b\u5206\u8a34\u8aaa\u3001"},{"id":2,"text":"\u4e9e\u57fa\u5e15\u738b\u963f\u3001\u7336\u592a\u4eba\u6240\u544a\u6211\u7684\u4e00\u5207\u4e8b\u3001\u4eca\u65e5\u5f97\u5728\u4f60\u9762\u524d\u5206\u8a34\u3001\u5be6\u70ba\u842c\u5e78\uff0e"},{"id":3,"text":"\u66f4\u53ef\u5e78\u7684\u3001\u662f\u4f60\u719f\u6089\u7336\u592a\u4eba\u7684\u898f\u77e9\u3001\u548c\u4ed6\u5011\u7684\u8faf\u8ad6\uff0e\u6240\u4ee5\u6c42\u4f60\u8010\u5fc3\u807d\u6211\u3002"},{"id":4,"text":"\u6211\u5f9e\u8d77\u521d\u5728\u672c\u570b\u7684\u6c11\u4e2d\u3001\u4e26\u5728\u8036\u8def\u6492\u51b7\u3001\u81ea\u5e7c\u70ba\u4eba\u5982\u4f55\u3001\u7336\u592a\u4eba\u90fd\u77e5\u9053\uff0e"},{"id":5,"text":"\u4ed6\u5011\u82e5\u80af\u4f5c\u898b\u8b49\u3001\u5c31\u66c9\u5f97\u6211\u5f9e\u8d77\u521d\u3001\u662f\u6309\u8457\u6211\u5011\u6559\u4e2d\u6700\u56b4\u7dca\u7684\u6559\u9580\u3001\u4f5c\u4e86\u6cd5\u5229\u8cfd\u4eba\u3002"},{"id":6,"text":"\u73fe\u5728\u6211\u7ad9\u5728\u9019\u88e1\u53d7\u5be9\u3001\u662f\u56e0\u70ba\u6307\u671b\u3000\u795e\u5411\u6211\u5011\u7956\u5b97\u6240\u61c9\u8a31\u7684\uff0e"},{"id":7,"text":"\u9019\u61c9\u8a31\u3001\u6211\u5011\u5341\u4e8c\u500b\u652f\u6d3e\u3001\u665d\u591c\u5207\u5207\u7684\u4e8b\u5949\u3000\u795e\u3001\u90fd\u6307\u671b\u5f97\u8457\uff0e\u738b\u963f\u3001\u6211\u88ab\u7336\u592a\u4eba\u63a7\u544a\u3001\u5c31\u662f\u56e0\u9019\u6307\u671b\u3002"},{"id":8,"text":"\u795e\u53eb\u6b7b\u4eba\u5fa9\u6d3b\u3001\u4f60\u5011\u70ba\u751a\u9ebc\u770b\u4f5c\u4e0d\u53ef\u4fe1\u7684\u5462\u3002"},{"id":9,"text":"\u5f9e\u524d\u6211\u81ea\u5df1\u4ee5\u70ba\u61c9\u7576\u591a\u65b9\u653b\u64ca\u62ff\u6492\u52d2\u4eba\u8036\u7a4c\u7684\u540d\u3002"},{"id":10,"text":"\u6211\u5728\u8036\u8def\u6492\u51b7\u4e5f\u66fe\u9019\u6a23\u884c\u4e86\uff0e\u65e2\u5f9e\u796d\u53f8\u9577\u5f97\u4e86\u6b0a\u67c4\u3001\u6211\u5c31\u628a\u8a31\u591a\u8056\u5f92\u56da\u5728\u76e3\u88e1\uff0e\u4ed6\u5011\u88ab\u6bba\u3001\u6211\u4e5f\u51fa\u540d\u5b9a\u6848\u3002"},{"id":11,"text":"\u5728\u5404\u6703\u5802\u3001\u6211\u5c62\u6b21\u7528\u5211\u3001\u5f37\u903c\u4ed6\u5011\u8aaa\u893b\u7006\u7684\u8a71\uff0e\u53c8\u5206\u5916\u60f1\u6068\u4ed6\u5011\u3001\u751a\u81f3\u8ffd\u903c\u4ed6\u5011\u76f4\u5230\u5916\u90a6\u7684\u57ce\u9091\u3002"},{"id":12,"text":"\u90a3\u6642\u3001\u6211\u9818\u4e86\u796d\u53f8\u9577\u7684\u6b0a\u67c4\u548c\u547d\u4ee4\u3001\u5f80\u5927\u99ac\u8272\u53bb\u3002"},{"id":13,"text":"\u738b\u963f\u3001\u6211\u5728\u8def\u4e0a\u3001\u664c\u5348\u7684\u6642\u5019\u3001\u770b\u898b\u5f9e\u5929\u767c\u5149\u3001\u6bd4\u65e5\u982d\u9084\u4eae\u3001\u56db\u9762\u7167\u8457\u6211\u3001\u4e26\u8207\u6211\u540c\u884c\u7684\u4eba\u3002"},{"id":14,"text":"\u6211\u5011\u90fd\u4ec6\u5012\u5728\u5730\u3001\u6211\u5c31\u807d\u898b\u6709\u8072\u97f3\u3001\u7528\u5e0c\u4f2f\u4f86\u8a71\u3001\u5411\u6211\u8aaa\u3001\u6383\u7f85\u3001\u6383\u7f85\u3001\u70ba\u751a\u9ebc\u903c\u8feb\u6211\uff0e\u4f60\u7528\u8173\u8e22\u523a\u662f\u96e3\u7684\u3002"},{"id":15,"text":"\u6211\u8aaa\u3001\u4e3b\u963f\u3001\u4f60\u662f\u8ab0\u3002\u4e3b\u8aaa\u3001\u6211\u5c31\u662f\u4f60\u6240\u903c\u8feb\u7684\u8036\u7a4c\u3002"},{"id":16,"text":"\u4f60\u8d77\u4f86\u7ad9\u8457\u3001\u6211\u7279\u610f\u5411\u4f60\u986f\u73fe\u3001\u8981\u6d3e\u4f60\u4f5c\u57f7\u4e8b\u4f5c\u898b\u8b49\u3001\u5c07\u4f60\u6240\u770b\u898b\u7684\u4e8b\u3001\u548c\u6211\u5c07\u8981\u6307\u793a\u4f60\u7684\u4e8b\u3001\u8b49\u660e\u51fa\u4f86\uff0e"},{"id":17,"text":"\u6211\u4e5f\u8981\u6551\u4f60\u812b\u96e2\u767e\u59d3\u548c\u5916\u90a6\u4eba\u7684\u624b\u3002"},{"id":18,"text":"\u6211\u5dee\u4f60\u5230\u4ed6\u5011\u90a3\u88e1\u53bb\u3001\u8981\u53eb\u4ed6\u5011\u7684\u773c\u775b\u5f97\u958b\u3001\u5f9e\u9ed1\u6697\u4e2d\u6b78\u5411\u5149\u660e\u3001\u5f9e\u6492\u4f46\u6b0a\u4e0b\u6b78\u5411\u3000\u795e\uff0e\u53c8\u56e0\u4fe1\u6211\u3001\u5f97\u8499\u8d66\u7f6a\u3001\u548c\u4e00\u5207\u6210\u8056\u7684\u4eba\u540c\u5f97\u57fa\u696d\u3002"},{"id":19,"text":"\u4e9e\u57fa\u5e15\u738b\u963f\u3001\u6211\u6545\u6b64\u6c92\u6709\u9055\u80cc\u90a3\u5f9e\u5929\u4e0a\u4f86\u7684\u7570\u8c61\uff0e"},{"id":20,"text":"\u5148\u5728\u5927\u99ac\u8272\u3001\u5f8c\u5728\u8036\u8def\u6492\u51b7\u3001\u548c\u7336\u592a\u5168\u5730\u3001\u4ee5\u53ca\u5916\u90a6\u3001\u52f8\u52c9\u4ed6\u5011\u61c9\u7576\u6094\u6539\u6b78\u5411\u3000\u795e\u3001\u884c\u4e8b\u8207\u6094\u6539\u7684\u5fc3\u76f8\u7a31\u3002"},{"id":21,"text":"\u56e0\u6b64\u3001\u7336\u592a\u4eba\u5728\u6bbf\u88e1\u62ff\u4f4f\u6211\u3001\u60f3\u8981\u6bba\u6211\u3002"},{"id":22,"text":"\u7136\u800c\u6211\u8499\u3000\u795e\u7684\u5e6b\u52a9\u3001\u76f4\u5230\u4eca\u65e5\u9084\u7ad9\u5f97\u4f4f\u3001\u5c0d\u8457\u5c0a\u8cb4\u5351\u8ce4\u8001\u5e7c\u4f5c\u898b\u8b49\uff0e\u6240\u8b1b\u7684\u3001\u4e26\u4e0d\u5916\u4e4e\u773e\u5148\u77e5\u548c\u6469\u897f\u6240\u8aaa\u3001\u5c07\u4f86\u5fc5\u6210\u7684\u4e8b\uff0e"},{"id":23,"text":"\u5c31\u662f\u57fa\u7763\u5fc5\u9808\u53d7\u5bb3\u3001\u4e26\u4e14\u56e0\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u3001\u8981\u9996\u5148\u628a\u5149\u660e\u7684\u9053\u3001\u50b3\u7d66\u767e\u59d3\u548c\u5916\u90a6\u4eba\u3002"},{"id":24,"text":"\u4fdd\u7f85\u9019\u6a23\u5206\u8a34\u3001\u975e\u65af\u90fd\u5927\u8072\u8aaa\u3001\u4fdd\u7f85\u3001\u4f60\u7672\u72c2\u4e86\u7f77\uff0e\u4f60\u7684\u5b78\u554f\u592a\u5927\u3001\u53cd\u53eb\u4f60\u7672\u72c2\u4e86\u3002"},{"id":25,"text":"\u4fdd\u7f85\u8aaa\u3001\u975e\u65af\u90fd\u5927\u4eba\u3001\u6211\u4e0d\u662f\u7672\u72c2\u3001\u6211\u8aaa\u7684\u4e43\u662f\u771f\u5be6\u660e\u767d\u8a71\u3002"},{"id":26,"text":"\u738b\u4e5f\u66c9\u5f97\u9019\u4e9b\u4e8b\u3001\u6240\u4ee5\u6211\u5411\u738b\u653e\u81bd\u76f4\u8a00\u3001\u6211\u6df1\u4fe1\u9019\u4e9b\u4e8b\u6c92\u6709\u4e00\u4ef6\u5411\u738b\u96b1\u85cf\u7684\uff0e\u56e0\u90fd\u4e0d\u662f\u5728\u80cc\u5730\u88e1\u4f5c\u7684\u3002"},{"id":27,"text":"\u4e9e\u57fa\u5e15\u738b\u963f\u3001\u4f60\u4fe1\u5148\u77e5\u9ebc\u3001\u6211\u77e5\u9053\u4f60\u662f\u4fe1\u7684\u3002"},{"id":28,"text":"\u4e9e\u57fa\u5e15\u5c0d\u4fdd\u7f85\u8aaa\u3001\u4f60\u60f3\u5c11\u5fae\u4e00\u52f8\u3001\u4fbf\u53eb\u6211\u4f5c\u57fa\u7763\u5f92\u963f\u3002\u3014\u6216\u4f5c\u4f60\u9019\u6a23\u52f8\u6211\u5e7e\u4e4e\u53eb\u6211\u4f5c\u57fa\u7763\u5f92\u4e86\u3015"},{"id":29,"text":"\u4fdd\u7f85\u8aaa\u3001\u7121\u8ad6\u662f\u5c11\u52f8\u3001\u662f\u591a\u52f8\u3001\u6211\u5411\u3000\u795e\u6240\u6c42\u7684\u3001\u4e0d\u4f46\u4f60\u4e00\u500b\u4eba\u3001\u5c31\u662f\u4eca\u5929\u4e00\u5207\u807d\u6211\u7684\u3001\u90fd\u8981\u50cf\u6211\u4e00\u6a23\u3001\u53ea\u662f\u4e0d\u8981\u50cf\u6211\u6709\u9019\u4e9b\u9396\u934a\u3002"},{"id":30,"text":"\u65bc\u662f\u738b\u3001\u548c\u5de1\u64ab\u3001\u4e26\u767e\u5c3c\u57fa\u3001\u8207\u540c\u5750\u7684\u4eba\u3001\u90fd\u8d77\u4f86\u3001"},{"id":31,"text":"\u9000\u5230\u88e1\u9762\u3001\u5f7c\u6b64\u8ac7\u8ad6\u8aaa\u3001\u9019\u4eba\u4e26\u6c92\u6709\u72af\u751a\u9ebc\u8a72\u6b7b\u8a72\u7d81\u7684\u7f6a\u3002"},{"id":32,"text":"\u4e9e\u57fa\u5e15\u53c8\u5c0d\u975e\u65af\u90fd\u8aaa\u3001\u9019\u4eba\u82e5\u6c92\u6709\u4e0a\u544a\u65bc\u8a72\u6492\u3001\u5c31\u53ef\u4ee5\u91cb\u653e\u4e86\u3002"}]