[{"id":1,"text":"\u6211\u5011\u65e2\u5df2\u5f97\u6551\u3001\u7e94\u77e5\u9053\u90a3\u5cf6\u540d\u53eb\u7c73\u5229\u5927\u3002"},{"id":2,"text":"\u571f\u4eba\u770b\u5f85\u6211\u5011\u3001\u6709\u975e\u5e38\u7684\u60c5\u5206\u3001\u56e0\u70ba\u7576\u6642\u4e0b\u96e8\u3001\u5929\u6c23\u53c8\u51b7\u3001\u5c31\u751f\u706b\u3001\u63a5\u5f85\u6211\u5011\u773e\u4eba\u3002"},{"id":3,"text":"\u90a3\u6642\u3001\u4fdd\u7f85\u62fe\u8d77\u4e00\u6346\u67f4\u3001\u653e\u5728\u706b\u4e0a\u3001\u6709\u4e00\u689d\u6bd2\u86c7\u3001\u56e0\u70ba\u71b1\u4e86\u51fa\u4f86\u3001\u54ac\u4f4f\u4ed6\u7684\u624b\u3002"},{"id":4,"text":"\u571f\u4eba\u770b\u898b\u90a3\u6bd2\u86c7\u3001\u61f8\u5728\u4ed6\u624b\u4e0a\u3001\u5c31\u5f7c\u6b64\u8aaa\u3001\u9019\u4eba\u5fc5\u662f\u500b\u5147\u624b\u3001\u96d6\u7136\u5f9e\u6d77\u88e1\u6551\u4e0a\u4f86\u3001\u5929\u7406\u9084\u4e0d\u5bb9\u4ed6\u6d3b\u8457\u3002"},{"id":5,"text":"\u4fdd\u7f85\u7adf\u628a\u90a3\u6bd2\u86c7\u7529\u5728\u706b\u88e1\u3001\u4e26\u6c92\u6709\u53d7\u50b7\u3002"},{"id":6,"text":"\u571f\u4eba\u60f3\u4ed6\u5fc5\u8981\u816b\u8d77\u4f86\u3001\u6216\u662f\u5ffd\u7136\u4ec6\u5012\u6b7b\u4e86\uff0e\u770b\u4e86\u591a\u6642\u3001\u898b\u4ed6\u7121\u5bb3\u3001\u5c31\u8f49\u5ff5\u8aaa\u3001\u4ed6\u662f\u500b\u795e\u3002"},{"id":7,"text":"\u96e2\u90a3\u5730\u65b9\u4e0d\u9060\u3001\u6709\u7530\u7522\u662f\u5cf6\u9577\u90e8\u767e\u6d41\u7684\uff0e\u4ed6\u63a5\u7d0d\u6211\u5011\u3001\u76e1\u60c5\u6b3e\u5f85\u4e09\u65e5\u3002"},{"id":8,"text":"\u7576\u6642\u3001\u90e8\u767e\u6d41\u7684\u7236\u89aa\u3001\u60a3\u71b1\u75c5\u548c\u75e2\u75be\u8eba\u8457\uff0e\u4fdd\u7f85\u9032\u53bb\u3001\u70ba\u4ed6\u79b1\u544a\u3001\u6309\u624b\u5728\u4ed6\u8eab\u4e0a\u3001\u6cbb\u597d\u4e86\u4ed6\u3002"},{"id":9,"text":"\u5f9e\u6b64\u3001\u5cf6\u4e0a\u5176\u9918\u7684\u75c5\u4eba\u3001\u4e5f\u4f86\u5f97\u4e86\u91ab\u6cbb\u3002"},{"id":10,"text":"\u4ed6\u5011\u53c8\u591a\u65b9\u7684\u5c0a\u656c\u6211\u5011\uff0e\u5230\u4e86\u958b\u8239\u7684\u6642\u5019\u3001\u4e5f\u628a\u6211\u5011\u6240\u9700\u7528\u7684\u9001\u5230\u8239\u4e0a\u3002"},{"id":11,"text":"\u904e\u4e86\u4e09\u500b\u6708\u3001\u6211\u5011\u4e0a\u4e86\u4e9e\u529b\u5c71\u592a\u7684\u8239\u3001\u5f80\u524d\u884c\uff0e\u9019\u8239\u4ee5\u4e1f\u65af\u96d9\u5b50\u70ba\u8a18\u3001\u662f\u5728\u90a3\u6d77\u5cf6\u904e\u4e86\u51ac\u7684\u3002"},{"id":12,"text":"\u5230\u4e86\u6558\u62c9\u53e4\u3001\u6211\u5011\u505c\u6cca\u4e09\u65e5\u3002"},{"id":13,"text":"\u53c8\u5f9e\u90a3\u88e1\u7e5e\u884c\u3001\u4f86\u5230\u5229\u57fa\u7fc1\uff0e\u904e\u4e86\u4e00\u5929\u3001\u8d77\u4e86\u5357\u98a8\u3001\u7b2c\u4e8c\u5929\u5c31\u4f86\u5230\u90e8\u4e1f\u5229\u3002"},{"id":14,"text":"\u5728\u90a3\u88e1\u9047\u898b\u5f1f\u5144\u5011\u3001\u8acb\u6211\u5011\u8207\u4ed6\u5011\u540c\u4f4f\u4e86\u4e03\u5929\uff0e\u9019\u6a23\u3001\u6211\u5011\u4f86\u5230\u7f85\u99ac\u3002"},{"id":15,"text":"\u90a3\u88e1\u7684\u5f1f\u5144\u5011\u3001\u4e00\u807d\u898b\u6211\u5011\u7684\u4fe1\u606f\u3001\u5c31\u51fa\u4f86\u5230\u4e9e\u6bd4\u70cf\u5dff\u3001\u548c\u4e09\u9928\u5730\u65b9\u8fce\u63a5\u6211\u5011\uff0e\u4fdd\u7f85\u898b\u4e86\u4ed6\u5011\u3001\u5c31\u611f\u8b1d\u3000\u795e\u3001\u653e\u5fc3\u58ef\u81bd\u3002"},{"id":16,"text":"\u9032\u4e86\u7f85\u99ac\u57ce\u3001\u3014\u6709\u53e4\u5377\u5728\u6b64\u6709\u767e\u592b\u9577\u628a\u773e\u56da\u72af\u4ea4\u7d66\u5fa1\u71df\u7684\u7d71\u9818\u60df\u6709\u3015\u4fdd\u7f85\u8499\u51c6\u3001\u548c\u4e00\u500b\u770b\u5b88\u4ed6\u7684\u5175\u3001\u53e6\u4f4f\u5728\u4e00\u8655\u3002"},{"id":17,"text":"\u904e\u4e86\u4e09\u5929\u3001\u4fdd\u7f85\u8acb\u7336\u592a\u4eba\u7684\u9996\u9818\u4f86\uff0e\u4ed6\u5011\u4f86\u4e86\u3001\u5c31\u5c0d\u4ed6\u5011\u8aaa\u3001\u5f1f\u5144\u5011\u3001\u6211\u96d6\u6c92\u6709\u4f5c\u751a\u9ebc\u4e8b\u5e72\u72af\u672c\u570b\u7684\u767e\u59d3\u3001\u548c\u6211\u5011\u7956\u5b97\u7684\u898f\u689d\u3001\u537b\u88ab\u9396\u7d81\u3001\u5f9e\u8036\u8def\u6492\u51b7\u89e3\u5728\u7f85\u99ac\u4eba\u7684\u624b\u88e1\u3002"},{"id":18,"text":"\u4ed6\u5011\u5be9\u554f\u4e86\u6211\u3001\u5c31\u9858\u610f\u91cb\u653e\u6211\uff0e\u56e0\u70ba\u5728\u6211\u8eab\u4e0a\u3001\u4e26\u6c92\u6709\u8a72\u6b7b\u7684\u7f6a\u3002"},{"id":19,"text":"\u7121\u5948\u7336\u592a\u4eba\u4e0d\u670d\u3001\u6211\u4e0d\u5f97\u5df2\u3001\u53ea\u597d\u4e0a\u544a\u65bc\u8a72\u6492\uff0e\u4e26\u975e\u6709\u751a\u9ebc\u4e8b\u3001\u8981\u63a7\u544a\u6211\u672c\u570b\u7684\u767e\u59d3\u3002"},{"id":20,"text":"\u56e0\u6b64\u3001\u6211\u8acb\u4f60\u5011\u4f86\u898b\u9762\u8aaa\u8a71\uff0e\u6211\u539f\u70ba\u4ee5\u8272\u5217\u4eba\u6240\u6307\u671b\u7684\u3001\u88ab\u9019\u934a\u5b50\u6346\u9396\u3002"},{"id":21,"text":"\u4ed6\u5011\u8aaa\u3001\u6211\u5011\u4e26\u6c92\u6709\u63a5\u8457\u5f9e\u7336\u592a\u4f86\u8ad6\u4f60\u7684\u4fe1\u3001\u4e5f\u6c92\u6709\u5f1f\u5144\u5230\u9019\u88e1\u4f86\u3001\u5831\u7d66\u6211\u5011\u8aaa\u3001\u4f60\u6709\u751a\u9ebc\u4e0d\u597d\u8655\u3002"},{"id":22,"text":"\u4f46\u6211\u5011\u9858\u610f\u807d\u4f60\u7684\u610f\u898b\u5982\u4f55\uff0e\u56e0\u70ba\u9019\u6559\u9580\u3001\u6211\u5011\u66c9\u5f97\u662f\u5230\u8655\u88ab\u6bc0\u8b17\u7684\u3002"},{"id":23,"text":"\u4ed6\u5011\u548c\u4fdd\u7f85\u7d04\u5b9a\u4e86\u65e5\u5b50\u3001\u5c31\u6709\u8a31\u591a\u4eba\u5230\u4ed6\u7684\u5bd3\u8655\u4f86\u3001\u4fdd\u7f85\u5f9e\u65e9\u5230\u665a\u3001\u5c0d\u4ed6\u5011\u8b1b\u8ad6\u9019\u4e8b\u3001\u8b49\u660e\u3000\u795e\u570b\u7684\u9053\u3001\u5f15\u6469\u897f\u7684\u5f8b\u6cd5\u548c\u5148\u77e5\u7684\u66f8\u3001\u4ee5\u8036\u7a4c\u7684\u4e8b\u3001\u52f8\u52c9\u4ed6\u5011\u3002"},{"id":24,"text":"\u4ed6\u6240\u8aaa\u7684\u8a71\u3001\u6709\u4fe1\u7684\u3001\u6709\u4e0d\u4fe1\u7684\u3002"},{"id":25,"text":"\u4ed6\u5011\u5f7c\u6b64\u4e0d\u5408\u3001\u5c31\u6563\u4e86\uff0e\u672a\u6563\u4ee5\u5148\u3001\u4fdd\u7f85\u8aaa\u4e86\u4e00\u53e5\u8a71\u3001\u8aaa\u3001\u8056\u9748\u85c9\u5148\u77e5\u4ee5\u8cfd\u4e9e\u3001\u5411\u4f60\u5011\u7956\u5b97\u6240\u8aaa\u7684\u8a71\u3001\u662f\u4e0d\u932f\u7684\u3002"},{"id":26,"text":"\u4ed6\u8aaa\u3001\u300e\u4f60\u53bb\u544a\u8a34\u9019\u767e\u59d3\u8aaa\u3001\u4f60\u5011\u807d\u662f\u8981\u807d\u898b\u3001\u537b\u4e0d\u660e\u767d\uff0e\u770b\u662f\u8981\u770b\u898b\u3001\u537b\u4e0d\u66c9\u5f97\uff0e"},{"id":27,"text":"\u56e0\u70ba\u9019\u767e\u59d3\u3001\u6cb9\u8499\u4e86\u5fc3\u3001\u8033\u6735\u767c\u6c89\u3001\u773c\u775b\u9589\u8457\uff0e\u6050\u6015\u773c\u775b\u770b\u898b\u3001\u8033\u6735\u807d\u898b\u3001\u5fc3\u88e1\u660e\u767d\u3001\u56de\u8f49\u904e\u4f86\u3001\u6211\u5c31\u91ab\u6cbb\u4ed6\u5011\u3002\u300f"},{"id":28,"text":"\u6240\u4ee5\u4f60\u5011\u7576\u77e5\u9053\u3001\u3000\u795e\u9019\u6551\u6069\u3001\u5982\u4eca\u50b3\u7d66\u5916\u90a6\u4eba\u3001\u4ed6\u5011\u4e5f\u5fc5\u807d\u53d7\u3002\u3014\u6709\u53e4\u5377\u5728\u6b64\u6709\u3015"},{"id":29,"text":"\u3014\u4fdd\u7f85\u8aaa\u4e86\u9019\u8a71\u7336\u592a\u4eba\u8b70\u8ad6\u7d1b\u7d1b\u7684\u5c31\u8d70\u4e86\u3015"},{"id":30,"text":"\u4fdd\u7f85\u5728\u81ea\u5df1\u6240\u79df\u7684\u623f\u5b50\u88e1\u3001\u4f4f\u4e86\u8db3\u8db3\u5169\u5e74\u3002\u51e1\u4f86\u898b\u4ed6\u7684\u4eba\u3001\u4ed6\u5168\u90fd\u63a5\u5f85\u3001"},{"id":31,"text":"\u653e\u81bd\u50b3\u8b1b\u3000\u795e\u570b\u7684\u9053\u3001\u5c07\u4e3b\u8036\u7a4c\u57fa\u7763\u7684\u4e8b\u6559\u5c0e\u4eba\u3001\u4e26\u6c92\u6709\u4eba\u7981\u6b62\u3002"}]