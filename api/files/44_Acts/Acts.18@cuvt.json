[{"id":1,"text":"\u9019\u4e8b\u4ee5\u5f8c\u3001\u4fdd\u7f85\u96e2\u4e86\u96c5\u5178\u3001\u4f86\u5230\u54e5\u6797\u591a\u3002"},{"id":2,"text":"\u9047\u898b\u4e00\u500b\u7336\u592a\u4eba\u3001\u540d\u53eb\u4e9e\u5c45\u62c9\u3001\u4ed6\u751f\u5728\u672c\u90fd\uff0e\u56e0\u70ba\u9769\u8001\u4e1f\u547d\u7336\u592a\u4eba\u90fd\u96e2\u958b\u7f85\u99ac\u3001\u65b0\u8fd1\u5e36\u8457\u59bb\u767e\u57fa\u62c9\u3001\u5f9e\u7fa9\u5927\u5229\u4f86\uff0e\u4fdd\u7f85\u5c31\u6295\u5954\u4e86\u4ed6\u5011\u3002"},{"id":3,"text":"\u4ed6\u5011\u672c\u662f\u88fd\u9020\u5e33\u68da\u70ba\u696d\uff0e\u4fdd\u7f85\u56e0\u8207\u4ed6\u5011\u540c\u696d\u3001\u5c31\u548c\u4ed6\u4ed6\u5011\u540c\u4f4f\u4f5c\u5de5\u3002"},{"id":4,"text":"\u6bcf\u9022\u5b89\u606f\u65e5\u3001\u4fdd\u7f85\u5728\u6703\u5802\u88e1\u8faf\u8ad6\u3001\u52f8\u5316\u7336\u592a\u4eba\u548c\u5e0c\u5229\u5c3c\u4eba\u3002"},{"id":5,"text":"\u897f\u62c9\u548c\u63d0\u6469\u592a\u5f9e\u99ac\u5176\u9813\u4f86\u7684\u6642\u5019\u3001\u4fdd\u7f85\u70ba\u9053\u8feb\u5207\u3001\u5411\u7336\u592a\u4eba\u8b49\u660e\u8036\u7a4c\u662f\u57fa\u7763\u3002"},{"id":6,"text":"\u4ed6\u5011\u65e2\u6297\u62d2\u3001\u6bc0\u8b17\u3001\u4fdd\u7f85\u5c31\u6296\u8457\u8863\u88f3\u8aaa\u3001\u4f60\u5011\u7684\u7f6a\u6b78\u5230\u4f60\u5011\u81ea\u5df1\u982d\u4e0a\u3001\u3014\u7f6a\u539f\u6587\u4f5c\u8840\u3015\u8207\u6211\u7121\u5e72\u3001\u3014\u539f\u6587\u4f5c\u6211\u537b\u4e7e\u6de8\u3015\u5f9e\u4eca\u4ee5\u5f8c\u3001\u6211\u8981\u5f80\u5916\u90a6\u4eba\u90a3\u88e1\u53bb\u3001"},{"id":7,"text":"\u65bc\u662f\u96e2\u958b\u90a3\u88e1\u3001\u5230\u4e86\u4e00\u500b\u4eba\u7684\u5bb6\u4e2d\u3001\u9019\u4eba\u540d\u53eb\u63d0\u591a\u7336\u58eb\u90fd\u3001\u662f\u656c\u62dc\u3000\u795e\u7684\u3001\u4ed6\u7684\u5bb6\u9760\u8fd1\u6703\u5802\u3002"},{"id":8,"text":"\u7ba1\u6703\u5802\u7684\u57fa\u5229\u53f8\u5e03\u548c\u5168\u5bb6\u90fd\u4fe1\u4e86\u4e3b\uff0e\u9084\u6709\u8a31\u591a\u54e5\u6797\u591a\u4eba\u807d\u4e86\u3001\u5c31\u76f8\u4fe1\u53d7\u6d17\u3002"},{"id":9,"text":"\u591c\u9593\u4e3b\u5728\u7570\u8c61\u4e2d\u5c0d\u4fdd\u7f85\u8aaa\u3001\u4e0d\u8981\u6015\u3001\u53ea\u7ba1\u8b1b\u3001\u4e0d\u8981\u9589\u53e3\uff0e"},{"id":10,"text":"\u6709\u6211\u8207\u4f60\u540c\u5728\u3001\u5fc5\u6c92\u6709\u4eba\u4e0b\u624b\u5bb3\u4f60\uff0e\u56e0\u70ba\u5728\u9019\u57ce\u88e1\u6211\u6709\u8a31\u591a\u7684\u767e\u59d3\u3002"},{"id":11,"text":"\u4fdd\u7f85\u5728\u90a3\u88e1\u4f4f\u4e86\u4e00\u5e74\u96f6\u516d\u500b\u6708\u3001\u5c07\u3000\u795e\u7684\u9053\u6559\u8a13\u4ed6\u5011\u3002"},{"id":12,"text":"\u5230\u8fe6\u6d41\u4f5c\u4e9e\u8a72\u4e9e\u65b9\u4f2f\u7684\u6642\u5019\u3001\u7336\u592a\u4eba\u540c\u5fc3\u8d77\u4f86\u653b\u64ca\u4fdd\u7f85\u3001\u62c9\u4ed6\u5230\u516c\u5802\u3001"},{"id":13,"text":"\u8aaa\u3001\u9019\u500b\u4eba\u52f8\u4eba\u4e0d\u6309\u8457\u5f8b\u6cd5\u656c\u62dc\u3000\u795e\u3002"},{"id":14,"text":"\u4fdd\u7f85\u525b\u8981\u958b\u53e3\u3001\u8fe6\u6d41\u5c31\u5c0d\u7336\u592a\u4eba\u8aaa\u3001\u4f60\u5011\u9019\u4e9b\u7336\u592a\u4eba\u3001\u5982\u679c\u662f\u70ba\u51a4\u6789\u3001\u6216\u5978\u60e1\u7684\u4e8b\u3001\u6211\u7406\u7576\u8010\u6027\u807d\u4f60\u5011\uff0e"},{"id":15,"text":"\u4f46\u6240\u722d\u8ad6\u7684\u3001\u82e5\u662f\u95dc\u4e4e\u8a00\u8a9e\u3001\u540d\u76ee\u3001\u548c\u4f60\u5011\u7684\u5f8b\u6cd5\u3001\u4f60\u5011\u81ea\u5df1\u53bb\u8fa6\u7f77\uff0e\u9019\u6a23\u7684\u4e8b\u6211\u4e0d\u9858\u610f\u5be9\u554f\u3002"},{"id":16,"text":"\u5c31\u628a\u4ed6\u5011\u6506\u51fa\u516c\u5802\u3002"},{"id":17,"text":"\u773e\u4eba\u4fbf\u63ea\u4f4f\u7ba1\u6703\u5802\u7684\u6240\u63d0\u5c3c\u3001\u5728\u5802\u524d\u6253\u4ed6\uff0e\u9019\u4e9b\u4e8b\u8fe6\u6d41\u90fd\u4e0d\u7ba1\u3002"},{"id":18,"text":"\u4fdd\u7f85\u53c8\u4f4f\u4e86\u591a\u65e5\u3001\u5c31\u8fad\u5225\u4e86\u5f1f\u5144\u3001\u5750\u8239\u5f80\u6558\u5229\u4e9e\u53bb\u3001\u767e\u57fa\u62c9\u3001\u4e9e\u5c45\u62c9\u548c\u4ed6\u540c\u53bb\uff0e\u4ed6\u56e0\u70ba\u8a31\u904e\u9858\u3001\u5c31\u5728\u5805\u9769\u54e9\u526a\u4e86\u982d\u9aee\u3002"},{"id":19,"text":"\u5230\u4e86\u4ee5\u5f17\u6240\u3001\u4fdd\u7f85\u5c31\u628a\u4ed6\u5011\u7559\u5728\u90a3\u88e1\u3001\u81ea\u5df1\u9032\u4e86\u6703\u5802\u3001\u548c\u7336\u592a\u4eba\u8faf\u8ad6\u3002"},{"id":20,"text":"\u773e\u4eba\u8acb\u4ed6\u591a\u4f4f\u4e9b\u65e5\u5b50\u3001\u4ed6\u537b\u4e0d\u5141\uff0e"},{"id":21,"text":"\u5c31\u8fad\u5225\u4ed6\u5011\u8aaa\u3001\u3000\u795e\u82e5\u8a31\u6211\u3001\u6211\u9084\u8981\u56de\u5230\u4f60\u5011\u9019\u88e1\uff0e\u65bc\u662f\u958b\u8239\u96e2\u4e86\u4ee5\u5f17\u6240\u3002"},{"id":22,"text":"\u5728\u8a72\u6492\u5229\u4e9e\u4e0b\u4e86\u8239\u3001\u5c31\u4e0a\u8036\u8def\u6492\u51b7\u53bb\u554f\u6559\u6703\u5b89\u3001\u96a8\u5f8c\u4e0b\u5b89\u63d0\u963f\u53bb\u3002"},{"id":23,"text":"\u4f4f\u4e86\u4e9b\u65e5\u5b50\u3001\u53c8\u96e2\u958b\u90a3\u88e1\u3001\u6328\u6b21\u7d93\u904e\u52a0\u62c9\u592a\u548c\u5f17\u5442\u5bb6\u5730\u65b9\u3001\u5805\u56fa\u773e\u9580\u5f92\u3002"},{"id":24,"text":"\u6709\u4e00\u500b\u7336\u592a\u4eba\u3001\u540d\u53eb\u4e9e\u6ce2\u7f85\u3001\u4f86\u5230\u4ee5\u5f17\u6240\uff0e\u4ed6\u751f\u5728\u4e9e\u529b\u5c71\u592a\u3001\u662f\u6709\u5b78\u554f\u7684\u3001\u6700\u80fd\u8b1b\u89e3\u8056\u7d93\u3002\u3014\u5b78\u554f\u6216\u4f5c\u53e3\u624d\u3015"},{"id":25,"text":"\u9019\u4eba\u5df2\u7d93\u5728\u4e3b\u7684\u9053\u4e0a\u53d7\u4e86\u6559\u8a13\u3001\u5fc3\u88e1\u706b\u71b1\u3001\u5c07\u8036\u7a4c\u7684\u4e8b\u3001\u8a73\u7d30\u8b1b\u8ad6\u6559\u8a13\u4eba\uff0e\u53ea\u662f\u4ed6\u55ae\u66c9\u5f97\u7d04\u7ff0\u7684\u6d17\u79ae\u3002"},{"id":26,"text":"\u4ed6\u5728\u6703\u5802\u88e1\u653e\u81bd\u8b1b\u9053\u3001\u767e\u57fa\u62c9\u4e9e\u5c45\u62c9\u807d\u898b\u3001\u5c31\u63a5\u4ed6\u4f86\u3001\u5c07\u3000\u795e\u7684\u9053\u7d66\u4ed6\u8b1b\u89e3\u66f4\u52a0\u8a73\u7d30\u3002"},{"id":27,"text":"\u4ed6\u60f3\u8981\u5f80\u4e9e\u8a72\u4e9e\u53bb\uff0e\u5f1f\u5144\u5011\u5c31\u52c9\u52f5\u4ed6\u3001\u4e26\u5beb\u4fe1\u8acb\u9580\u5f92\u63a5\u5f85\u4ed6\uff0e\u3014\u6216\u4f5c\u5f1f\u5144\u5011\u5c31\u5beb\u4fe1\u52f8\u9580\u5f92\u63a5\u5f85\u4ed6\u3015\u4ed6\u5230\u4e86\u90a3\u88e1\u3001\u591a\u5e6b\u52a9\u90a3\u8499\u6069\u4fe1\u4e3b\u7684\u4eba\uff0e"},{"id":28,"text":"\u5728\u773e\u4eba\u9762\u524d\u6975\u6709\u80fd\u529b\u3001\u99c1\u5012\u7336\u592a\u4eba\u3001\u5f15\u8056\u7d93\u8b49\u660e\u8036\u7a4c\u662f\u57fa\u7763\u3002"}]