[{"id":1,"text":"\u7533\u521d\u79b1\u544a\u7684\u6642\u5019\u3001\u5f7c\u5f97\u3001\u7d04\u7ff0\u3001\u4e0a\u8056\u6bbf\u53bb\u3002"},{"id":2,"text":"\u6709\u4e00\u500b\u4eba\u3001\u751f\u4f86\u662f\u7638\u817f\u7684\u3001\u5929\u5929\u88ab\u4eba\u62ac\u4f86\u3001\u653e\u5728\u6bbf\u7684\u4e00\u500b\u9580\u53e3\u3001\u90a3\u9580\u540d\u53eb\u7f8e\u9580\u3001\u8981\u6c42\u9032\u6bbf\u7684\u4eba\u8cd9\u6fdf\u3002"},{"id":3,"text":"\u4ed6\u770b\u898b\u5f7c\u5f97\u7d04\u7ff0\u5c07\u8981\u9032\u6bbf\u3001\u5c31\u6c42\u4ed6\u5011\u8cd9\u6fdf\u3002"},{"id":4,"text":"\u5f7c\u5f97\u7d04\u7ff0\u5b9a\u775b\u770b\u4ed6\uff0e\u5f7c\u5f97\u8aaa\u3001\u4f60\u770b\u6211\u5011\u3002"},{"id":5,"text":"\u90a3\u4eba\u5c31\u7559\u610f\u770b\u4ed6\u5011\u3001\u6307\u671b\u5f97\u8457\u751a\u9ebc\u3002"},{"id":6,"text":"\u5f7c\u5f97\u8aaa\u3001\u91d1\u9280\u6211\u90fd\u6c92\u6709\u3001\u53ea\u628a\u6211\u6240\u6709\u7684\u7d66\u4f60\u3001\u6211\u5949\u62ff\u6492\u52d2\u4eba\u8036\u7a4c\u57fa\u7763\u7684\u540d\u3001\u53eb\u4f60\u8d77\u4f86\u884c\u8d70\u3002"},{"id":7,"text":"\u65bc\u662f\u62c9\u8457\u4ed6\u7684\u53f3\u624b\u3001\u6276\u4ed6\u8d77\u4f86\u3001\u4ed6\u7684\u8173\u548c\u8e1d\u5b50\u9aa8\u3001\u7acb\u523b\u5065\u58ef\u4e86\uff0e"},{"id":8,"text":"\u5c31\u8df3\u8d77\u4f86\u3001\u7ad9\u8457\u3001\u53c8\u884c\u8d70\uff0e\u540c\u4ed6\u5011\u9032\u4e86\u6bbf\u3001\u8d70\u8457\u3001\u8df3\u8457\u3001\u8b9a\u7f8e\u3000\u795e\u3002"},{"id":9,"text":"\u767e\u59d3\u90fd\u770b\u898b\u4ed6\u884c\u8d70\u3001\u8b9a\u7f8e\u3000\u795e\uff0e"},{"id":10,"text":"\u8a8d\u5f97\u4ed6\u662f\u90a3\u7d20\u5e38\u5750\u5728\u6bbf\u7684\u7f8e\u9580\u53e3\u6c42\u8cd9\u6fdf\u7684\uff0e\u5c31\u56e0\u4ed6\u6240\u9047\u8457\u7684\u4e8b\u3001\u6eff\u5fc3\u5e0c\u5947\u9a5a\u8a1d\u3002"},{"id":11,"text":"\u90a3\u4eba\u6b63\u5728\u7a31\u70ba\u6240\u7f85\u9580\u7684\u5eca\u4e0b\u3001\u62c9\u8457\u5f7c\u5f97\u3001\u7d04\u7ff0\uff0e\u773e\u767e\u59d3\u4e00\u9f4a\u8dd1\u5230\u4ed6\u5011\u90a3\u88e1\u3001\u5f88\u89ba\u5e0c\u5947\u3002"},{"id":12,"text":"\u5f7c\u5f97\u770b\u898b\u3001\u5c31\u5c0d\u767e\u59d3\u8aaa\u3001\u4ee5\u8272\u5217\u4eba\u54ea\u3001\u70ba\u751a\u9ebc\u628a\u9019\u4e8b\u7576\u4f5c\u5e0c\u5947\u5462\uff0e\u70ba\u751a\u9ebc\u5b9a\u775b\u770b\u6211\u5011\u3001\u4ee5\u70ba\u6211\u5011\u6191\u81ea\u5df1\u7684\u80fd\u529b\u548c\u8654\u8aa0\u3001\u4f7f\u9019\u4eba\u884c\u8d70\u5462\u3002"},{"id":13,"text":"\u4e9e\u4f2f\u62c9\u7f55\u3001\u4ee5\u6492\u3001\u96c5\u5404\u7684\u3000\u795e\u3001\u5c31\u662f\u6211\u5011\u5217\u7956\u7684\u3000\u795e\u3001\u5df2\u7d93\u69ae\u8000\u4e86\u4ed6\u7684\u50d5\u4eba\u8036\u7a4c\uff0e\u3014\u50d5\u4eba\u6216\u4f5c\u5152\u5b50\u3015\u4f60\u5011\u537b\u628a\u4ed6\u4ea4\u4ed8\u5f7c\u62c9\u591a\uff0e\u5f7c\u62c9\u591a\u5b9a\u610f\u8981\u91cb\u653e\u4ed6\u3001\u4f60\u5011\u7adf\u5728\u5f7c\u62c9\u591a\u9762\u524d\u68c4\u7d55\u4e86\u4ed6\uff0e"},{"id":14,"text":"\u4f60\u5011\u68c4\u7d55\u4e86\u90a3\u8056\u6f54\u516c\u7fa9\u8005\u3001\u53cd\u6c42\u8457\u91cb\u653e\u4e00\u500b\u5147\u624b\u7d66\u4f60\u5011\uff0e"},{"id":15,"text":"\u4f60\u5011\u6bba\u4e86\u90a3\u751f\u547d\u7684\u4e3b\u3001\u3000\u795e\u537b\u53eb\u4ed6\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u4e86\uff0e\u6211\u5011\u90fd\u662f\u70ba\u9019\u4e8b\u4f5c\u898b\u8b49\u3002"},{"id":16,"text":"\u6211\u5011\u56e0\u4fe1\u4ed6\u7684\u540d\u3001\u4ed6\u7684\u540d\u4fbf\u53eb\u4f60\u5011\u6240\u770b\u898b\u6240\u8a8d\u8b58\u7684\u9019\u4eba\u3001\u5065\u58ef\u4e86\uff0e\u6b63\u662f\u4ed6\u6240\u8cdc\u7684\u4fe1\u5fc3\u3001\u53eb\u9019\u4eba\u5728\u4f60\u5011\u773e\u4eba\u9762\u524d\u5168\u7136\u597d\u4e86\u3002"},{"id":17,"text":"\u5f1f\u5144\u5011\u3001\u6211\u66c9\u5f97\u4f60\u5011\u4f5c\u9019\u4e8b\u3001\u662f\u51fa\u65bc\u4e0d\u77e5\u3001\u4f60\u5011\u7684\u5b98\u9577\u4e5f\u662f\u5982\u6b64\u3002"},{"id":18,"text":"\u4f46\u3000\u795e\u66fe\u85c9\u773e\u5148\u77e5\u7684\u53e3\u3001\u8c6b\u8a00\u57fa\u7763\u5c07\u8981\u53d7\u5bb3\u3001\u5c31\u9019\u6a23\u61c9\u9a57\u4e86\u3002"},{"id":19,"text":"\u6240\u4ee5\u4f60\u5011\u7576\u6094\u6539\u6b78\u6b63\u3001\u4f7f\u4f60\u5011\u7684\u7f6a\u5f97\u4ee5\u5857\u62b9\u3001\u9019\u6a23\u3001\u90a3\u5b89\u8212\u7684\u65e5\u5b50\u3001\u5c31\u5fc5\u5f9e\u4e3b\u9762\u524d\u4f86\u5230\uff0e"},{"id":20,"text":"\u4e3b\u4e5f\u5fc5\u5dee\u9063\u6240\u8c6b\u5b9a\u7d66\u4f60\u5011\u7684\u57fa\u7763\u8036\u7a4c\u964d\u81e8\u3002"},{"id":21,"text":"\u5929\u5fc5\u7559\u4ed6\u3001\u7b49\u5230\u842c\u7269\u5fa9\u8208\u7684\u6642\u5019\u3001\u5c31\u662f\u3000\u795e\u5f9e\u5275\u4e16\u4ee5\u4f86\u3001\u85c9\u8457\u8056\u5148\u77e5\u7684\u53e3\u6240\u8aaa\u7684\u3002"},{"id":22,"text":"\u6469\u897f\u66fe\u8aaa\u3001\u300e\u4e3b\u3000\u795e\u8981\u5f9e\u4f60\u5011\u5f1f\u5144\u4e2d\u9593\u3001\u7d66\u4f60\u5011\u8208\u8d77\u4e00\u4f4d\u5148\u77e5\u50cf\u6211\u3001\u51e1\u4ed6\u5411\u4f60\u5011\u6240\u8aaa\u7684\u3001\u4f60\u5011\u90fd\u8981\u807d\u5f9e\u3002"},{"id":23,"text":"\u51e1\u4e0d\u807d\u5f9e\u90a3\u5148\u77e5\u7684\u3001\u5fc5\u8981\u5f9e\u6c11\u4e2d\u5168\u7136\u6ec5\u7d55\u3002\u300f"},{"id":24,"text":"\u5f9e\u6492\u6bcd\u8033\u4ee5\u4f86\u7684\u773e\u5148\u77e5\u3001\u51e1\u8aaa\u8c6b\u8a00\u7684\u3001\u4e5f\u90fd\u8aaa\u5230\u9019\u4e9b\u65e5\u5b50\u3002"},{"id":25,"text":"\u4f60\u5011\u662f\u5148\u77e5\u7684\u5b50\u5b6b\u3001\u4e5f\u627f\u53d7\u3000\u795e\u8207\u4f60\u5011\u7956\u5b97\u6240\u7acb\u7684\u7d04\u3001\u5c31\u662f\u5c0d\u4e9e\u4f2f\u62c9\u7f55\u8aaa\u3001\u5730\u4e0a\u842c\u65cf\u3001\u90fd\u8981\u56e0\u4f60\u7684\u5f8c\u88d4\u5f97\u798f\u3002"},{"id":26,"text":"\u795e\u65e2\u8208\u8d77\u4ed6\u7684\u50d5\u4eba\u3001\u3014\u6216\u4f5c\u5152\u5b50\u3015\u5c31\u5148\u5dee\u4ed6\u5230\u4f60\u5011\u9019\u88e1\u4f86\u3001\u8cdc\u798f\u7d66\u4f60\u5011\u3001\u53eb\u4f60\u5011\u5404\u4eba\u56de\u8f49\u3001\u96e2\u958b\u7f6a\u60e1\u3002"}]