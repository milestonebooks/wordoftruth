[{"id":1,"text":"\u5b89\u606f\u65e5\u3001\u8036\u7a4c\u5230\u4e00\u500b\u6cd5\u5229\u8cfd\u4eba\u7684\u9996\u9818\u5bb6\u88e1\u53bb\u55ab\u98ef\u3001\u4ed6\u5011\u5c31\u7aba\u63a2\u4ed6\u3002"},{"id":2,"text":"\u5728\u4ed6\u9762\u524d\u6709\u4e00\u500b\u60a3\u6c34\u81cc\u7684\u4eba\u3002"},{"id":3,"text":"\u8036\u7a4c\u5c0d\u5f8b\u6cd5\u5e2b\u548c\u6cd5\u5229\u8cfd\u4eba\u8aaa\u3001\u5b89\u606f\u65e5\u6cbb\u75c5\u3001\u53ef\u4ee5\u4e0d\u53ef\u4ee5\uff0e"},{"id":4,"text":"\u4ed6\u5011\u537b\u4e0d\u8a00\u8a9e\u3002\u8036\u7a4c\u5c31\u6cbb\u597d\u90a3\u4eba\u3001\u53eb\u4ed6\u8d70\u4e86\uff0e"},{"id":5,"text":"\u4fbf\u5c0d\u4ed6\u5011\u8aaa\u3001\u4f60\u5011\u4e2d\u9593\u8ab0\u6709\u9a62\u6216\u6709\u725b\u3001\u5728\u5b89\u606f\u65e5\u6389\u5728\u4e95\u88e1\u3001\u4e0d\u7acb\u6642\u62c9\u7260\u4e0a\u4f86\u5462\u3002"},{"id":6,"text":"\u4ed6\u5011\u4e0d\u80fd\u5c0d\u7b54\u9019\u8a71\u3002"},{"id":7,"text":"\u8036\u7a4c\u898b\u6240\u8acb\u7684\u5ba2\u63c0\u64c7\u9996\u4f4d\u3001\u5c31\u7528\u6bd4\u55bb\u5c0d\u4ed6\u5011\u8aaa\u3001"},{"id":8,"text":"\u4f60\u88ab\u4eba\u8acb\u53bb\u8d74\u5a5a\u59fb\u7684\u7b75\u5e2d\u3001\u4e0d\u8981\u5750\u5728\u9996\u4f4d\u4e0a\uff0e\u6050\u6015\u6709\u6bd4\u4f60\u5c0a\u8cb4\u7684\u5ba2\u3001\u88ab\u4ed6\u8acb\u4f86\uff0e"},{"id":9,"text":"\u90a3\u8acb\u4f60\u5011\u7684\u4eba\u524d\u4f86\u5c0d\u4f60\u8aaa\u3001\u8b93\u5ea7\u7d66\u9019\u4e00\u4f4d\u7f77\uff0e\u4f60\u5c31\u7f9e\u7f9e\u615a\u615a\u7684\u9000\u5230\u672b\u4f4d\u4e0a\u53bb\u4e86\u3002"},{"id":10,"text":"\u4f60\u88ab\u8acb\u7684\u6642\u5019\u3001\u5c31\u53bb\u5750\u5728\u672b\u4f4d\u4e0a\u3001\u597d\u53eb\u90a3\u8acb\u4f60\u7684\u4eba\u4f86\u3001\u5c0d\u4f60\u8aaa\u3001\u670b\u53cb\u3001\u8acb\u4e0a\u5750\uff0e\u90a3\u6642\u4f60\u5728\u540c\u5e2d\u7684\u4eba\u9762\u524d\u3001\u5c31\u6709\u5149\u5f69\u4e86\u3002"},{"id":11,"text":"\u56e0\u70ba\u51e1\u81ea\u9ad8\u7684\u5fc5\u964d\u70ba\u5351\uff0e\u81ea\u5351\u7684\u5fc5\u5347\u70ba\u9ad8\u3002"},{"id":12,"text":"\u8036\u7a4c\u53c8\u5c0d\u8acb\u4ed6\u7684\u4eba\u8aaa\u3001\u4f60\u64fa\u8a2d\u5348\u98ef\u3001\u6216\u665a\u98ef\u3001\u4e0d\u8981\u8acb\u4f60\u7684\u670b\u53cb\u3001\u5f1f\u5144\u3001\u89aa\u5c6c\u3001\u548c\u5bcc\u8db3\u7684\u9130\u820d\uff0e\u6050\u6015\u4ed6\u5011\u4e5f\u8acb\u4f60\u3001\u4f60\u5c31\u5f97\u4e86\u5831\u7b54\u3002"},{"id":13,"text":"\u4f60\u64fa\u8a2d\u7b75\u5e2d\u3001\u5012\u8981\u8acb\u90a3\u8ca7\u7aae\u7684\u3001\u6b98\u5ee2\u7684\u3001\u7638\u817f\u7684\u3001\u778e\u773c\u7684\u3001\u4f60\u5c31\u6709\u798f\u4e86\uff0e"},{"id":14,"text":"\u56e0\u70ba\u4ed6\u5011\u6c92\u6709\u751a\u9ebc\u53ef\u5831\u7b54\u4f60\uff0e\u5230\u7fa9\u4eba\u5fa9\u6d3b\u7684\u6642\u5019\u3001\u4f60\u8981\u5f97\u8457\u5831\u7b54\u3002"},{"id":15,"text":"\u540c\u5e2d\u7684\u6709\u4e00\u4eba\u807d\u898b\u9019\u8a71\u3001\u5c31\u5c0d\u8036\u7a4c\u8aaa\u3001\u5728\u3000\u795e\u570b\u88e1\u55ab\u98ef\u7684\u6709\u798f\u4e86\u3002"},{"id":16,"text":"\u8036\u7a4c\u5c0d\u4ed6\u8aaa\u3001\u6709\u4e00\u4eba\u64fa\u8a2d\u5927\u7b75\u5e2d\u3001\u8acb\u4e86\u8a31\u591a\u5ba2\uff0e"},{"id":17,"text":"\u5230\u4e86\u5750\u5e2d\u7684\u6642\u5019\u3001\u6253\u767c\u50d5\u4eba\u53bb\u5c0d\u6240\u8acb\u7684\u4eba\u8aaa\u3001\u8acb\u4f86\u7f77\uff0e\u6a23\u6a23\u90fd\u9f4a\u5099\u4e86\u3002"},{"id":18,"text":"\u773e\u4eba\u4e00\u53e3\u540c\u97f3\u7684\u63a8\u8fad\u3002\u982d\u4e00\u500b\u8aaa\u3001\u6211\u8cb7\u4e86\u4e00\u584a\u5730\u3001\u5fc5\u9808\u53bb\u770b\u770b\uff0e\u8acb\u4f60\u51c6\u6211\u8fad\u4e86\u3002"},{"id":19,"text":"\u53c8\u6709\u4e00\u500b\u8aaa\u3001\u6211\u8cb7\u4e86\u4e94\u5c0d\u725b\u3001\u8981\u53bb\u8a66\u4e00\u8a66\uff0e\u8acb\u4f60\u51c6\u6211\u8fad\u4e86\u3002"},{"id":20,"text":"\u53c8\u6709\u4e00\u500b\u8aaa\u3001\u6211\u7e94\u5a36\u4e86\u59bb\u3001\u6240\u4ee5\u4e0d\u80fd\u53bb\u3002"},{"id":21,"text":"\u90a3\u50d5\u4eba\u56de\u4f86\u3001\u628a\u9019\u4e8b\u90fd\u544a\u8a34\u4e86\u4e3b\u4eba\uff0e\u5bb6\u4e3b\u5c31\u52d5\u6012\u3001\u5c0d\u50d5\u4eba\u8aaa\u3001\u5feb\u51fa\u53bb\u5230\u57ce\u88e1\u5927\u8857\u5c0f\u5df7\u3001\u9818\u90a3\u8ca7\u7aae\u7684\u3001\u6b98\u5ee2\u7684\u3001\u778e\u773c\u7684\u3001\u7638\u817f\u7684\u4f86\u3002"},{"id":22,"text":"\u50d5\u4eba\u8aaa\u3001\u4e3b\u963f\u3001\u4f60\u6240\u5429\u5490\u7684\u5df2\u7d93\u8fa6\u4e86\u3001\u9084\u6709\u7a7a\u5ea7\u3002"},{"id":23,"text":"\u4e3b\u4eba\u5c0d\u50d5\u4eba\u8aaa\u3001\u4f60\u51fa\u53bb\u5230\u8def\u4e0a\u548c\u7c6c\u7b06\u90a3\u88e1\u3001\u52c9\u5f37\u4eba\u9032\u4f86\u3001\u5750\u6eff\u6211\u7684\u5c4b\u5b50\u3002"},{"id":24,"text":"\u6211\u544a\u8a34\u4f60\u5011\u3001\u5148\u524d\u6240\u8acb\u7684\u4eba\u3001\u6c92\u6709\u4e00\u500b\u5f97\u5617\u6211\u7684\u7b75\u5e2d\u3002"},{"id":25,"text":"\u6709\u6975\u591a\u7684\u4eba\u548c\u8036\u7a4c\u540c\u884c\uff0e\u4ed6\u8f49\u904e\u4f86\u5c0d\u4ed6\u5011\u8aaa\u3001"},{"id":26,"text":"\u4eba\u5230\u6211\u9019\u88e1\u4f86\u3001\u82e5\u4e0d\u611b\u6211\u52dd\u904e\u611b\u81ea\u5df1\u7684\u7236\u6bcd\u3001\u59bb\u5b50\u3001\u5152\u5973\u3001\u5f1f\u5144\u3001\u59d0\u59b9\u548c\u81ea\u5df1\u7684\u6027\u547d\u3001\u5c31\u4e0d\u80fd\u4f5c\u6211\u7684\u9580\u5f92\u3002\u3014\u611b\u6211\u52dd\u904e\u611b\u539f\u6587\u4f5c\u4f77\u3015"},{"id":27,"text":"\u51e1\u4e0d\u80cc\u8457\u81ea\u5df1\u5341\u5b57\u67b6\u8ddf\u5f9e\u6211\u7684\u3001\u4e5f\u4e0d\u80fd\u4f5c\u6211\u7684\u9580\u5f92\u3002"},{"id":28,"text":"\u4f60\u5011\u90a3\u4e00\u500b\u8981\u84cb\u4e00\u5ea7\u6a13\u3001\u4e0d\u5148\u5750\u4e0b\u7b97\u8a08\u82b1\u8cbb\u3001\u80fd\u84cb\u6210\u4e0d\u80fd\u5462\u3002"},{"id":29,"text":"\u6050\u6015\u5b89\u4e86\u5730\u57fa\u3001\u4e0d\u80fd\u6210\u529f\u3001\u770b\u898b\u7684\u4eba\u90fd\u7b11\u8a71\u4ed6\u3001"},{"id":30,"text":"\u8aaa\u3001\u9019\u500b\u4eba\u958b\u4e86\u5de5\u3001\u537b\u4e0d\u80fd\u5b8c\u5de5\u3002"},{"id":31,"text":"\u6216\u662f\u4e00\u500b\u738b\u3001\u51fa\u53bb\u548c\u5225\u7684\u738b\u6253\u4ed7\u3001\u8c48\u4e0d\u5148\u5750\u4e0b\u914c\u91cf\u3001\u80fd\u7528\u4e00\u842c\u5175\u3001\u53bb\u6575\u90a3\u9818\u4e8c\u842c\u5175\u4f86\u653b\u6253\u4ed6\u7684\u9ebc\u3002"},{"id":32,"text":"\u82e5\u662f\u4e0d\u80fd\u3001\u5c31\u8d81\u6575\u4eba\u9084\u9060\u7684\u6642\u5019\u3001\u6d3e\u4f7f\u8005\u53bb\u6c42\u548c\u606f\u7684\u689d\u6b3e\u3002"},{"id":33,"text":"\u9019\u6a23\u3001\u4f60\u5011\u7121\u8ad6\u751a\u9ebc\u4eba\u3001\u82e5\u4e0d\u6487\u4e0b\u4e00\u5207\u6240\u6709\u7684\u3001\u5c31\u4e0d\u80fd\u4f5c\u6211\u7684\u9580\u5f92\u3002"},{"id":34,"text":"\u9e7d\u672c\u662f\u597d\u7684\uff0e\u9e7d\u82e5\u5931\u4e86\u5473\u3001\u53ef\u7528\u751a\u9ebc\u53eb\u5b83\u518d\u9e79\u5462\u3002"},{"id":35,"text":"\u6216\u7528\u5728\u7530\u88e1\u3001\u6216\u5806\u5728\u7cde\u88e1\u3001\u90fd\u4e0d\u5408\u5f0f\uff0e\u53ea\u597d\u4e1f\u5728\u5916\u9762\u3002\u6709\u8033\u53ef\u807d\u7684\u3001\u5c31\u61c9\u7576\u807d\u3002"}]