[{"id":1,"text":"\u773e\u7a05\u540f\u548c\u7f6a\u4eba\u3001\u90fd\u6328\u8fd1\u8036\u7a4c\u8981\u807d\u4ed6\u8b1b\u9053\u3002"},{"id":2,"text":"\u6cd5\u5229\u8cfd\u4eba\u548c\u6587\u58eb\u3001\u79c1\u4e0b\u8b70\u8ad6\u8aaa\u3001\u9019\u500b\u4eba\u63a5\u5f85\u7f6a\u4eba\u3001\u53c8\u540c\u4ed6\u5011\u55ab\u98ef\u3002"},{"id":3,"text":"\u8036\u7a4c\u5c31\u7528\u6bd4\u55bb\u3001\u8aaa\u3001"},{"id":4,"text":"\u4f60\u5011\u4e2d\u9593\u8ab0\u6709\u4e00\u767e\u96bb\u7f8a\u3001\u5931\u53bb\u4e00\u96bb\u3001\u4e0d\u628a\u9019\u4e5d\u5341\u4e5d\u96bb\u6487\u5728\u66e0\u91ce\u3001\u53bb\u627e\u90a3\u5931\u53bb\u7684\u7f8a\u76f4\u5230\u627e\u8457\u5462\u3002"},{"id":5,"text":"\u627e\u8457\u4e86\u3001\u5c31\u6b61\u6b61\u559c\u559c\u7684\u625b\u5728\u80a9\u4e0a\u3001\u56de\u5230\u5bb6\u88e1\u3002"},{"id":6,"text":"\u5c31\u8acb\u670b\u53cb\u9130\u820d\u4f86\u3001\u5c0d\u4ed6\u5011\u8aaa\u3001\u6211\u5931\u53bb\u7684\u7f8a\u5df2\u7d93\u627e\u8457\u4e86\u3001\u4f60\u5011\u548c\u6211\u4e00\u540c\u6b61\u559c\u7f77\u3002"},{"id":7,"text":"\u6211\u544a\u8a34\u4f60\u5011\u3001\u4e00\u500b\u7f6a\u4eba\u6094\u6539\u3001\u5728\u5929\u4e0a\u4e5f\u8981\u9019\u6a23\u70ba\u4ed6\u6b61\u559c\u3001\u8f03\u6bd4\u70ba\u4e5d\u5341\u4e5d\u500b\u4e0d\u7528\u6094\u6539\u7684\u7fa9\u4eba\u3001\u6b61\u559c\u66f4\u5927\u3002"},{"id":8,"text":"\u6216\u662f\u4e00\u500b\u5a66\u4eba\u3001\u6709\u5341\u584a\u9322\u3001\u82e5\u5931\u843d\u4e00\u584a\u3001\u8c48\u4e0d\u9ede\u4e0a\u71c8\u3001\u6253\u6383\u5c4b\u5b50\u3001\u7d30\u7d30\u7684\u627e\u3001\u76f4\u5230\u627e\u8457\u9ebc\u3002"},{"id":9,"text":"\u627e\u8457\u4e86\u3001\u5c31\u8acb\u670b\u53cb\u9130\u820d\u4f86\u3001\u5c0d\u4ed6\u5011\u8aaa\u3001\u6211\u5931\u843d\u7684\u90a3\u584a\u9322\u5df2\u7d93\u627e\u8457\u4e86\u3001\u4f60\u5011\u548c\u6211\u4e00\u540c\u6b61\u559c\u7f77\u3002"},{"id":10,"text":"\u6211\u544a\u8a34\u4f60\u5011\u3001\u4e00\u500b\u7f6a\u4eba\u6094\u6539\u3001\u5728\u3000\u795e\u7684\u4f7f\u8005\u9762\u524d\u3001\u4e5f\u662f\u9019\u6a23\u70ba\u4ed6\u6b61\u559c\u3002"},{"id":11,"text":"\u8036\u7a4c\u53c8\u8aaa\u3001\u4e00\u500b\u4eba\u6709\u5169\u500b\u5152\u5b50\u3002"},{"id":12,"text":"\u5c0f\u5152\u5b50\u5c0d\u7236\u89aa\u8aaa\u3001\u7236\u89aa\u3001\u8acb\u4f60\u628a\u6211\u61c9\u5f97\u7684\u5bb6\u696d\u5206\u7d66\u6211\uff0e\u4ed6\u7236\u89aa\u5c31\u628a\u7522\u696d\u5206\u7d66\u4ed6\u5011\u3002"},{"id":13,"text":"\u904e\u4e86\u4e0d\u591a\u5e7e\u65e5\u3001\u5c0f\u5152\u5b50\u5c31\u628a\u4ed6\u4e00\u5207\u6240\u6709\u7684\u3001\u90fd\u6536\u62fe\u8d77\u4f86\u3001\u5f80\u9060\u65b9\u53bb\u4e86\uff0e\u5728\u90a3\u88e1\u4efb\u610f\u653e\u8569\u3001\u6d6a\u8cbb\u8cb2\u8ca1\u3002"},{"id":14,"text":"\u65e2\u8017\u76e1\u4e86\u4e00\u5207\u6240\u6709\u7684\u3001\u53c8\u9047\u8457\u90a3\u5730\u65b9\u5927\u906d\u9951\u8352\u3001\u5c31\u7aae\u82e6\u8d77\u4f86\u3002"},{"id":15,"text":"\u65bc\u662f\u53bb\u6295\u9760\u90a3\u5730\u65b9\u7684\u4e00\u500b\u4eba\uff0e\u90a3\u4eba\u6253\u767c\u4ed6\u5230\u7530\u88e1\u53bb\u653e\u8c6c\u3002"},{"id":16,"text":"\u4ed6\u6068\u4e0d\u5f97\u62ff\u8c6c\u6240\u55ab\u7684\u8c46\u83a2\u5145\u98e2\uff0e\u4e5f\u6c92\u6709\u4eba\u7d66\u4ed6\u3002"},{"id":17,"text":"\u4ed6\u9192\u609f\u904e\u4f86\u3001\u5c31\u8aaa\u3001\u6211\u7236\u89aa\u6709\u591a\u5c11\u7684\u96c7\u5de5\u3001\u53e3\u7ce7\u6709\u9918\u3001\u6211\u5012\u5728\u9019\u88e1\u9913\u6b7b\u9ebc\uff0e"},{"id":18,"text":"\u6211\u8981\u8d77\u4f86\u3001\u5230\u6211\u7236\u89aa\u90a3\u88e1\u53bb\u3001\u5411\u4ed6\u8aaa\u3001\u7236\u89aa\u3001\u6211\u5f97\u7f6a\u4e86\u5929\u3001\u53c8\u5f97\u7f6a\u4e86\u4f60\uff0e"},{"id":19,"text":"\u5f9e\u4eca\u4ee5\u5f8c\u3001\u6211\u4e0d\u914d\u7a31\u70ba\u4f60\u7684\u5152\u5b50\u3001\u628a\u6211\u7576\u4f5c\u4e00\u500b\u96c7\u5de5\u7f77\u3002"},{"id":20,"text":"\u65bc\u662f\u8d77\u4f86\u5f80\u4ed6\u7236\u89aa\u90a3\u88e1\u53bb\u3002\u76f8\u96e2\u9084\u9060\u3001\u4ed6\u7236\u89aa\u770b\u898b\u3001\u5c31\u52d5\u4e86\u6148\u5fc3\u3001\u8dd1\u53bb\u62b1\u8457\u4ed6\u7684\u9838\u9805\u3001\u9023\u9023\u8207\u4ed6\u89aa\u5634\u3002"},{"id":21,"text":"\u5152\u5b50\u8aaa\u3001\u7236\u89aa\u3001\u6211\u5f97\u7f6a\u4e86\u5929\u3001\u53c8\u5f97\u7f6a\u4e86\u4f60\uff0e\u5f9e\u4eca\u4ee5\u5f8c\u3001\u6211\u4e0d\u914d\u7a31\u70ba\u4f60\u7684\u5152\u5b50\u3002"},{"id":22,"text":"\u7236\u89aa\u537b\u5429\u5490\u50d5\u4eba\u8aaa\u3001\u628a\u90a3\u4e0a\u597d\u7684\u888d\u5b50\u5feb\u62ff\u51fa\u4f86\u7d66\u4ed6\u7a7f\uff0e\u628a\u6212\u6307\u6234\u5728\u4ed6\u6307\u982d\u4e0a\uff0e\u628a\u978b\u7a7f\u5728\u4ed6\u8173\u4e0a\uff0e"},{"id":23,"text":"\u628a\u90a3\u80a5\u725b\u72a2\u727d\u4f86\u5bb0\u4e86\u3001\u6211\u5011\u53ef\u4ee5\u55ab\u559d\u5feb\u6a02\uff0e"},{"id":24,"text":"\u56e0\u70ba\u6211\u9019\u500b\u5152\u5b50\u3001\u662f\u6b7b\u800c\u5fa9\u6d3b\u3001\u5931\u800c\u53c8\u5f97\u7684\u3002\u4ed6\u5011\u5c31\u5feb\u6a02\u8d77\u4f86\u3002"},{"id":25,"text":"\u90a3\u6642\u3001\u5927\u5152\u5b50\u6b63\u5728\u7530\u88e1\uff0e\u4ed6\u56de\u4f86\u96e2\u5bb6\u4e0d\u9060\u3001\u807d\u898b\u4f5c\u6a02\u8df3\u821e\u7684\u8072\u97f3\uff0e"},{"id":26,"text":"\u4fbf\u53eb\u904e\u4e00\u500b\u50d5\u4eba\u4f86\u3001\u554f\u662f\u751a\u9ebc\u4e8b\u3002"},{"id":27,"text":"\u50d5\u4eba\u8aaa\u3001\u4f60\u5144\u5f1f\u4f86\u4e86\uff0e\u4f60\u7236\u89aa\u3001\u56e0\u70ba\u5f97\u4ed6\u7121\u707d\u7121\u75c5\u7684\u56de\u4f86\u3001\u628a\u80a5\u725b\u72a2\u5bb0\u4e86\u3002"},{"id":28,"text":"\u5927\u5152\u5b50\u537b\u751f\u6c23\u3001\u4e0d\u80af\u9032\u53bb\uff0e\u4ed6\u7236\u89aa\u5c31\u51fa\u4f86\u52f8\u4ed6\u3002"},{"id":29,"text":"\u4ed6\u5c0d\u7236\u89aa\u8aaa\u3001\u6211\u670d\u4e8b\u4f60\u9019\u591a\u5e74\u3001\u5f9e\u4f86\u6c92\u6709\u9055\u80cc\u904e\u4f60\u7684\u547d\uff0e\u4f60\u4e26\u6c92\u6709\u7d66\u6211\u4e00\u96bb\u5c71\u7f8a\u7f94\u3001\u53eb\u6211\u548c\u670b\u53cb\u3001\u4e00\u540c\u5feb\u6a02\uff0e"},{"id":30,"text":"\u4f46\u4f60\u9019\u500b\u5152\u5b50\u3001\u548c\u5a3c\u5993\u541e\u76e1\u4e86\u4f60\u7684\u7522\u696d\u3001\u4ed6\u4e00\u4f86\u4e86\u3001\u4f60\u5012\u70ba\u4ed6\u5bb0\u4e86\u80a5\u725b\u72a2\u3002"},{"id":31,"text":"\u7236\u89aa\u5c0d\u4ed6\u8aaa\u3001\u5152\u963f\u3001\u4f60\u5e38\u548c\u6211\u540c\u5728\u3001\u6211\u4e00\u5207\u6240\u6709\u7684\u3001\u90fd\u662f\u4f60\u7684\u3002"},{"id":32,"text":"\u53ea\u662f\u4f60\u9019\u500b\u5144\u5f1f\u3001\u662f\u6b7b\u800c\u5fa9\u6d3b\u3001\u5931\u800c\u53c8\u5f97\u7684\u3001\u6240\u4ee5\u6211\u5011\u7406\u7576\u6b61\u559c\u5feb\u6a02\u3002"}]