[{"id":1,"text":"\u6b63\u7576\u90a3\u6642\u3001\u6709\u4eba\u5c07\u5f7c\u62c9\u591a\u4f7f\u52a0\u5229\u5229\u4eba\u7684\u8840\u6519\u96dc\u5728\u4ed6\u5011\u796d\u7269\u4e2d\u7684\u4e8b\u3001\u544a\u8a34\u8036\u7a4c\u3002"},{"id":2,"text":"\u8036\u7a4c\u8aaa\u3001\u4f60\u5011\u4ee5\u70ba\u9019\u4e9b\u52a0\u5229\u5229\u4eba\u6bd4\u773e\u52a0\u5229\u5229\u4eba\u66f4\u6709\u7f6a\u3001\u6240\u4ee5\u53d7\u9019\u5bb3\u9ebc\u3002"},{"id":3,"text":"\u6211\u544a\u8a34\u4f60\u5011\u3001\u4e0d\u662f\u7684\uff0e\u4f60\u5011\u82e5\u4e0d\u6094\u6539\u3001\u90fd\u8981\u5982\u6b64\u6ec5\u4ea1\u3002"},{"id":4,"text":"\u5f9e\u524d\u897f\u7f85\u4e9e\u6a13\u5012\u584c\u4e86\u3001\u58d3\u6b7b\u5341\u516b\u500b\u4eba\uff0e\u4f60\u5011\u4ee5\u70ba\u90a3\u4e9b\u4eba\u6bd4\u4e00\u5207\u4f4f\u5728\u8036\u8def\u6492\u51b7\u7684\u4eba\u66f4\u6709\u7f6a\u9ebc\u3002"},{"id":5,"text":"\u6211\u544a\u8a34\u4f60\u5011\u3001\u4e0d\u662f\u7684\uff0e\u4f60\u5011\u82e5\u4e0d\u6094\u6539\u3001\u90fd\u8981\u5982\u6b64\u6ec5\u4ea1\u3002"},{"id":6,"text":"\u65bc\u662f\u7528\u6bd4\u55bb\u8aaa\u3001\u4e00\u500b\u4eba\u6709\u4e00\u68f5\u7121\u82b1\u679c\u6a39\u3001\u683d\u5728\u8461\u8404\u5712\u88e1\uff0e\u4ed6\u4f86\u5230\u6a39\u524d\u627e\u679c\u5b50\u3001\u537b\u627e\u4e0d\u8457\u3002"},{"id":7,"text":"\u5c31\u5c0d\u7ba1\u5712\u7684\u8aaa\u3001\u770b\u54ea\u3001\u6211\u9019\u4e09\u5e74\u3001\u4f86\u5230\u9019\u7121\u82b1\u679c\u6a39\u524d\u627e\u679c\u5b50\u3001\u7adf\u627e\u4e0d\u8457\u3001\u628a\u4ed6\u780d\u4e86\u7f77\uff0e\u4f55\u5fc5\u767d\u4f54\u5730\u571f\u5462\u3002"},{"id":8,"text":"\u7ba1\u5712\u7684\u8aaa\u3001\u4e3b\u963f\u3001\u4eca\u5e74\u4e14\u7559\u8457\u3001\u7b49\u6211\u5468\u570d\u6398\u958b\u571f\u3001\u52a0\u4e0a\u7cde\uff0e"},{"id":9,"text":"\u4ee5\u5f8c\u82e5\u7d50\u679c\u5b50\u4fbf\u7f77\uff0e\u4e0d\u7136\u518d\u628a\u4ed6\u780d\u4e86\u3002"},{"id":10,"text":"\u5b89\u606f\u65e5\u3001\u8036\u7a4c\u5728\u6703\u5802\u88e1\u6559\u8a13\u4eba\u3002"},{"id":11,"text":"\u6709\u4e00\u500b\u5973\u4eba\u3001\u88ab\u9b3c\u9644\u8457\u75c5\u4e86\u5341\u516b\u5e74\uff0e\u8170\u5f4e\u5f97\u4e00\u9ede\u76f4\u4e0d\u8d77\u4f86\u3002"},{"id":12,"text":"\u8036\u7a4c\u770b\u898b\u3001\u4fbf\u53eb\u904e\u4ed6\u4f86\u3001\u5c0d\u4ed6\u8aaa\u3001\u5973\u4eba\u3001\u4f60\u812b\u96e2\u9019\u75c5\u4e86\u3002"},{"id":13,"text":"\u65bc\u662f\u7528\u5169\u96bb\u624b\u6309\u8457\u4ed6\uff0e\u4ed6\u7acb\u523b\u76f4\u8d77\u8170\u4f86\u3001\u5c31\u6b78\u69ae\u8000\u8207\u3000\u795e\u3002"},{"id":14,"text":"\u7ba1\u6703\u5802\u7684\u3001\u56e0\u70ba\u8036\u7a4c\u5728\u5b89\u606f\u65e5\u6cbb\u75c5\u3001\u5c31\u6c23\u5fff\u5fff\u7684\u5c0d\u773e\u4eba\u8aaa\u3001\u6709\u516d\u65e5\u61c9\u7576\u4f5c\u5de5\uff0e\u90a3\u516d\u65e5\u4e4b\u5167\u3001\u53ef\u4ee5\u4f86\u6c42\u91ab\u3001\u5728\u5b89\u606f\u65e5\u537b\u4e0d\u53ef\u3002"},{"id":15,"text":"\u4e3b\u8aaa\u3001\u5047\u5192\u70ba\u5584\u7684\u4eba\u54ea\u3001\u96e3\u9053\u4f60\u5011\u5404\u4eba\u5728\u5b89\u606f\u65e5\u4e0d\u89e3\u958b\u69fd\u4e0a\u7684\u725b\u9a62\u3001\u727d\u53bb\u98f2\u9ebc\u3002"},{"id":16,"text":"\u6cc1\u4e14\u9019\u5973\u4eba\u672c\u662f\u4e9e\u4f2f\u62c9\u7f55\u7684\u5f8c\u88d4\u3001\u88ab\u6492\u4f46\u6346\u7d81\u4e86\u9019\u5341\u516b\u5e74\u3001\u4e0d\u7576\u5728\u5b89\u606f\u65e5\u89e3\u958b\u4ed6\u7684\u7d81\u9ebc\u3002"},{"id":17,"text":"\u8036\u7a4c\u8aaa\u9019\u8a71\u3001\u4ed6\u7684\u6575\u4eba\u90fd\u615a\u6127\u4e86\uff0e\u773e\u4eba\u56e0\u4ed6\u6240\u884c\u4e00\u5207\u69ae\u8000\u7684\u4e8b\u3001\u5c31\u90fd\u6b61\u559c\u4e86\u3002"},{"id":18,"text":"\u8036\u7a4c\u8aaa\u3001\u3000\u795e\u7684\u570b\u3001\u597d\u50cf\u751a\u9ebc\uff0e\u6211\u62ff\u751a\u9ebc\u4f86\u6bd4\u8f03\u5462\uff0e"},{"id":19,"text":"\u597d\u50cf\u4e00\u7c92\u82a5\u83dc\u7a2e\u3001\u6709\u4eba\u62ff\u53bb\u7a2e\u5728\u5712\u5b50\u88e1\uff0e\u9577\u5927\u6210\u6a39\u3001\u5929\u4e0a\u7684\u98db\u9ce5\u3001\u5bbf\u5728\u4ed6\u7684\u679d\u4e0a\u3002"},{"id":20,"text":"\u53c8\u8aaa\u3001\u6211\u62ff\u751a\u9ebc\u4f86\u6bd4\u3000\u795e\u7684\u570b\u5462\uff0e"},{"id":21,"text":"\u597d\u6bd4\u9eb5\u9175\u3001\u6709\u5a66\u4eba\u62ff\u4f86\u85cf\u5728\u4e09\u6597\u9eb5\u88e1\u3001\u76f4\u7b49\u5168\u7cf0\u90fd\u767c\u8d77\u4f86\u3002"},{"id":22,"text":"\u8036\u7a4c\u5f80\u8036\u8def\u6492\u51b7\u53bb\u3001\u5728\u6240\u7d93\u904e\u7684\u5404\u57ce\u5404\u9109\u6559\u8a13\u4eba\u3002"},{"id":23,"text":"\u6709\u4e00\u500b\u4eba\u554f\u4ed6\u8aaa\u3001\u4e3b\u963f\u3001\u5f97\u6551\u7684\u4eba\u5c11\u9ebc\u3002"},{"id":24,"text":"\u8036\u7a4c\u5c0d\u773e\u4eba\u8aaa\u3001\u4f60\u5011\u8981\u52aa\u529b\u9032\u7a84\u9580\uff0e\u6211\u544a\u8a34\u4f60\u5011\u3001\u5c07\u4f86\u6709\u8a31\u591a\u4eba\u60f3\u8981\u9032\u53bb\u3001\u537b\u662f\u4e0d\u80fd\u3002"},{"id":25,"text":"\u53ca\u81f3\u5bb6\u4e3b\u8d77\u4f86\u95dc\u4e86\u9580\u3001\u4f60\u5011\u7ad9\u5728\u5916\u9762\u53e9\u9580\u3001\u8aaa\u3001\u4e3b\u963f\u3001\u7d66\u6211\u5011\u958b\u9580\u3001\u4ed6\u5c31\u56de\u7b54\u8aaa\u3001\u6211\u4e0d\u8a8d\u8b58\u4f60\u5011\u3001\u4e0d\u66c9\u5f97\u4f60\u5011\u662f\u90a3\u88e1\u4f86\u7684\u3002"},{"id":26,"text":"\u90a3\u6642\u3001\u4f60\u5011\u8981\u8aaa\u3001\u6211\u5011\u5728\u4f60\u9762\u524d\u55ab\u904e\u559d\u904e\u3001\u4f60\u4e5f\u5728\u6211\u5011\u7684\u8857\u4e0a\u6559\u8a13\u904e\u4eba\u3002"},{"id":27,"text":"\u4ed6\u8981\u8aaa\u3001\u6211\u544a\u8a34\u4f60\u5011\u3001\u6211\u4e0d\u66c9\u5f97\u4f60\u5011\u662f\u90a3\u88e1\u4f86\u7684\uff0e\u4f60\u5011\u9019\u4e00\u5207\u4f5c\u60e1\u7684\u4eba\u3001\u96e2\u958b\u6211\u53bb\u7f77\u3002"},{"id":28,"text":"\u4f60\u5011\u8981\u770b\u898b\u4e9e\u4f2f\u62c9\u7f55\u3001\u4ee5\u6492\u3001\u96c5\u5404\u3001\u548c\u773e\u5148\u77e5\u3001\u90fd\u5728\u3000\u795e\u7684\u570b\u88e1\u3001\u4f60\u5011\u537b\u88ab\u8d95\u5230\u5916\u9762\uff0e\u5728\u90a3\u88e1\u5fc5\u8981\u54c0\u54ed\u5207\u9f52\u4e86\u3002"},{"id":29,"text":"\u5f9e\u6771\u3001\u5f9e\u897f\u3001\u5f9e\u5357\u3001\u5f9e\u5317\u3001\u5c07\u6709\u4eba\u4f86\u3001\u5728\u3000\u795e\u7684\u570b\u88e1\u5750\u5e2d\u3002"},{"id":30,"text":"\u53ea\u662f\u6709\u5728\u5f8c\u7684\u5c07\u8981\u5728\u524d\uff0e\u6709\u5728\u524d\u7684\u5c07\u8981\u5728\u5f8c\u3002"},{"id":31,"text":"\u6b63\u7576\u90a3\u6642\u3001\u6709\u5e7e\u500b\u6cd5\u5229\u8cfd\u4eba\u4f86\u5c0d\u8036\u7a4c\u8aaa\u3001\u96e2\u958b\u9019\u88e1\u53bb\u7f77\uff0e\u56e0\u70ba\u5e0c\u5f8b\u60f3\u8981\u6bba\u4f60\u3002"},{"id":32,"text":"\u8036\u7a4c\u8aaa\u3001\u4f60\u5011\u53bb\u544a\u8a34\u90a3\u500b\u72d0\u72f8\u8aaa\u3001\u4eca\u5929\u660e\u5929\u6211\u8d95\u9b3c\u6cbb\u75c5\u3001\u7b2c\u4e09\u5929\u6211\u7684\u4e8b\u5c31\u6210\u5168\u4e86\u3002"},{"id":33,"text":"\u96d6\u7136\u9019\u6a23\u3001\u4eca\u5929\u660e\u5929\u5f8c\u5929\u6211\u5fc5\u9808\u524d\u884c\uff0e\u56e0\u70ba\u5148\u77e5\u5728\u8036\u8def\u6492\u51b7\u4e4b\u5916\u55aa\u547d\u662f\u4e0d\u80fd\u7684\u3002"},{"id":34,"text":"\u8036\u8def\u6492\u51b7\u963f\u3001\u8036\u8def\u6492\u51b7\u963f\u3001\u4f60\u5e38\u6bba\u5bb3\u5148\u77e5\u3001\u53c8\u7528\u77f3\u982d\u6253\u6b7b\u90a3\u5949\u5dee\u9063\u5230\u4f60\u9019\u88e1\u4f86\u7684\u4eba\uff0e\u6211\u591a\u6b21\u9858\u610f\u805a\u96c6\u4f60\u7684\u5152\u5973\u3001\u597d\u50cf\u6bcd\u96de\u628a\u5c0f\u96de\u805a\u96c6\u5728\u7fc5\u8180\u5e95\u4e0b\u3001\u53ea\u662f\u4f60\u5011\u4e0d\u9858\u610f\u3002"},{"id":35,"text":"\u770b\u54ea\u3001\u4f60\u5011\u7684\u5bb6\u6210\u70ba\u8352\u5834\u7559\u7d66\u4f60\u5011\uff0e\u6211\u544a\u8a34\u4f60\u5011\u3001\u5f9e\u4eca\u4ee5\u5f8c\u4f60\u5011\u4e0d\u5f97\u518d\u898b\u6211\u3001\u76f4\u7b49\u5230\u4f60\u5011\u8aaa\u3001\u5949\u4e3b\u540d\u4f86\u7684\u662f\u61c9\u7576\u7a31\u980c\u7684\u3002"}]