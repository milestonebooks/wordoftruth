[{"id":1,"text":"\u4f60\u5011\u5fc3\u88e1\u4e0d\u8981\u6182\u6101\uff0e\u4f60\u5011\u4fe1\u3000\u795e\u3001\u4e5f\u7576\u4fe1\u6211\u3002"},{"id":2,"text":"\u5728\u6211\u7236\u7684\u5bb6\u88e1\u3001\u6709\u8a31\u591a\u4f4f\u8655\uff0e\u82e5\u662f\u6c92\u6709\u3001\u6211\u5c31\u65e9\u5df2\u544a\u8a34\u4f60\u5011\u4e86\uff0e\u6211\u53bb\u539f\u662f\u70ba\u4f60\u5011\u8c6b\u5099\u5730\u65b9\u53bb\u3002"},{"id":3,"text":"\u6211\u82e5\u53bb\u70ba\u4f60\u5011\u8c6b\u5099\u4e86\u5730\u65b9\u3001\u5c31\u5fc5\u518d\u4f86\u63a5\u4f60\u5011\u5230\u6211\u90a3\u88e1\u53bb\uff0e\u6211\u5728\u90a3\u88e1\u3001\u53eb\u4f60\u5011\u4e5f\u5728\u90a3\u88e1\u3002"},{"id":4,"text":"\u6211\u5f80\u90a3\u88e1\u53bb\u3001\u4f60\u5011\u77e5\u9053\uff0e\u90a3\u689d\u8def\u3001\u4f60\u5011\u4e5f\u77e5\u9053\u3002\u3014\u6709\u53e4\u5377\u4f5c\u6211\u5f80\u90a3\u88e1\u53bb\u4f60\u5011\u77e5\u9053\u90a3\u689d\u8def\u3015"},{"id":5,"text":"\u591a\u99ac\u5c0d\u4ed6\u8aaa\u3001\u4e3b\u963f\u3001\u6211\u5011\u4e0d\u77e5\u9053\u4f60\u5f80\u90a3\u88e1\u53bb\u3001\u600e\u9ebc\u77e5\u9053\u90a3\u689d\u8def\u5462\u3002"},{"id":6,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u5c31\u662f\u9053\u8def\u3001\u771f\u7406\u3001\u751f\u547d\uff0e\u82e5\u4e0d\u85c9\u8457\u6211\u3001\u6c92\u6709\u4eba\u80fd\u5230\u7236\u90a3\u88e1\u53bb\u3002"},{"id":7,"text":"\u4f60\u5011\u82e5\u8a8d\u8b58\u6211\u3001\u4e5f\u5c31\u8a8d\u8b58\u6211\u7684\u7236\uff0e\u5f9e\u4eca\u4ee5\u5f8c\u3001\u4f60\u5011\u8a8d\u8b58\u4ed6\u3001\u4e26\u4e14\u5df2\u7d93\u770b\u898b\u4ed6\u3002"},{"id":8,"text":"\u8153\u529b\u5c0d\u4ed6\u8aaa\u3001\u6c42\u4e3b\u5c07\u7236\u986f\u7d66\u6211\u5011\u770b\u3001\u6211\u5011\u5c31\u77e5\u8db3\u4e86\u3002"},{"id":9,"text":"\u8036\u7a4c\u5c0d\u4ed6\u8aaa\u3001\u8153\u529b\u3001\u6211\u8207\u4f60\u5011\u540c\u5728\u9019\u9ebc\u9577\u4e45\u3001\u4f60\u9084\u4e0d\u8a8d\u8b58\u6211\u9ebc\u3002\u4eba\u770b\u898b\u4e86\u6211\u3001\u5c31\u662f\u770b\u898b\u4e86\u7236\uff0e\u4f60\u600e\u9ebc\u8aaa\u3001\u5c07\u7236\u986f\u7d66\u6211\u5011\u770b\u5462\u3002"},{"id":10,"text":"\u6211\u5728\u7236\u88e1\u9762\u3001\u7236\u5728\u6211\u88e1\u9762\u3001\u4f60\u4e0d\u4fe1\u9ebc\u3002\u6211\u5c0d\u4f60\u5011\u6240\u8aaa\u7684\u8a71\u3001\u4e0d\u662f\u6191\u8457\u81ea\u5df1\u8aaa\u7684\u3001\u4e43\u662f\u4f4f\u5728\u6211\u88e1\u9762\u7684\u7236\u4f5c\u4ed6\u81ea\u5df1\u7684\u4e8b\u3002"},{"id":11,"text":"\u4f60\u5011\u7576\u4fe1\u6211\u3001\u6211\u5728\u7236\u88e1\u9762\u3001\u7236\u5728\u6211\u88e1\u9762\uff0e\u5373\u6216\u4e0d\u4fe1\u3001\u4e5f\u7576\u56e0\u6211\u6240\u4f5c\u7684\u4e8b\u4fe1\u6211\u3002"},{"id":12,"text":"\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u5011\u3001\u6211\u6240\u4f5c\u7684\u4e8b\u3001\u4fe1\u6211\u7684\u4eba\u4e5f\u8981\u4f5c\uff0e\u4e26\u4e14\u8981\u4f5c\u6bd4\u9019\u66f4\u5927\u7684\u4e8b\uff0e\u56e0\u70ba\u6211\u5f80\u7236\u90a3\u88e1\u53bb\u3002"},{"id":13,"text":"\u4f60\u5011\u5949\u6211\u7684\u540d\u3001\u7121\u8ad6\u6c42\u751a\u9ebc\u3001\u6211\u5fc5\u6210\u5c31\u3001\u53eb\u7236\u56e0\u5152\u5b50\u5f97\u69ae\u8000\u3002"},{"id":14,"text":"\u4f60\u5011\u82e5\u5949\u6211\u540d\u6c42\u751a\u9ebc\u3001\u6211\u5fc5\u6210\u5c31\u3002"},{"id":15,"text":"\u4f60\u5011\u82e5\u611b\u6211\u3001\u5c31\u5fc5\u9075\u5b88\u6211\u7684\u547d\u4ee4\u3002"},{"id":16,"text":"\u6211\u8981\u6c42\u7236\u3001\u7236\u5c31\u53e6\u5916\u8cdc\u7d66\u4f60\u5011\u4e00\u4f4d\u4fdd\u60e0\u5e2b\u3001\u3014\u6216\u4f5c\u8a13\u6170\u5e2b\u4e0b\u540c\u3015\u53eb\u4ed6\u6c38\u9060\u8207\u4f60\u5011\u540c\u5728\u3001"},{"id":17,"text":"\u5c31\u662f\u771f\u7406\u7684\u8056\u9748\u3001\u4e43\u4e16\u4eba\u4e0d\u80fd\u63a5\u53d7\u7684\uff0e\u56e0\u70ba\u4e0d\u898b\u4ed6\u3001\u4e5f\u4e0d\u8a8d\u8b58\u4ed6\uff0e\u4f60\u5011\u537b\u8a8d\u8b58\u4ed6\uff0e\u56e0\u4ed6\u5e38\u8207\u4f60\u5011\u540c\u5728\u3001\u4e5f\u8981\u5728\u4f60\u5011\u88e1\u9762\u3002"},{"id":18,"text":"\u6211\u4e0d\u6487\u4e0b\u4f60\u5011\u70ba\u5b64\u5152\u3001\u6211\u5fc5\u5230\u4f60\u5011\u9019\u88e1\u4f86\u3002"},{"id":19,"text":"\u9084\u6709\u4e0d\u591a\u7684\u6642\u5019\u3001\u4e16\u4eba\u4e0d\u518d\u770b\u898b\u6211\uff0e\u4f60\u5011\u537b\u770b\u898b\u6211\uff0e\u56e0\u70ba\u6211\u6d3b\u8457\u3001\u4f60\u5011\u4e5f\u8981\u6d3b\u8457\u3002"},{"id":20,"text":"\u5230\u90a3\u65e5\u4f60\u5011\u5c31\u77e5\u9053\u6211\u5728\u7236\u88e1\u9762\u3001\u4f60\u5011\u5728\u6211\u88e1\u9762\u3001\u6211\u4e5f\u5728\u4f60\u5011\u88e1\u9762\u3002"},{"id":21,"text":"\u6709\u4e86\u6211\u7684\u547d\u4ee4\u53c8\u9075\u5b88\u7684\u3001\u9019\u4eba\u5c31\u662f\u611b\u6211\u7684\uff0e\u611b\u6211\u7684\u5fc5\u8499\u6211\u7236\u611b\u4ed6\u3001\u6211\u4e5f\u8981\u611b\u4ed6\u3001\u4e26\u4e14\u8981\u5411\u4ed6\u986f\u73fe\u3002"},{"id":22,"text":"\u7336\u5927\uff08\u4e0d\u662f\u52a0\u7565\u4eba\u7336\u5927\uff09\u554f\u8036\u7a4c\u8aaa\u3001\u4e3b\u963f\u3001\u70ba\u751a\u9ebc\u8981\u5411\u6211\u5011\u986f\u73fe\u3001\u4e0d\u5411\u4e16\u4eba\u986f\u73fe\u5462\u3002"},{"id":23,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u4eba\u82e5\u611b\u6211\u3001\u5c31\u5fc5\u9075\u5b88\u6211\u7684\u9053\uff0e\u6211\u7236\u4e5f\u5fc5\u611b\u4ed6\u3001\u4e26\u4e14\u6211\u5011\u8981\u5230\u4ed6\u90a3\u88e1\u53bb\u3001\u8207\u4ed6\u540c\u4f4f\u3002"},{"id":24,"text":"\u4e0d\u611b\u6211\u7684\u4eba\u5c31\u4e0d\u9075\u5b88\u6211\u7684\u9053\uff0e\u4f60\u5011\u6240\u807d\u898b\u7684\u9053\u4e0d\u662f\u6211\u7684\u3001\u4e43\u662f\u5dee\u6211\u4f86\u4e4b\u7236\u7684\u9053\u3002"},{"id":25,"text":"\u6211\u9084\u8207\u4f60\u5011\u540c\u4f4f\u7684\u6642\u5019\u3001\u5df2\u5c07\u9019\u4e9b\u8a71\u5c0d\u4f60\u5011\u8aaa\u4e86\u3002"},{"id":26,"text":"\u4f46\u4fdd\u60e0\u5e2b\u3001\u5c31\u662f\u7236\u56e0\u6211\u7684\u540d\u6240\u8981\u5dee\u4f86\u7684\u8056\u9748\u3001\u4ed6\u8981\u5c07\u4e00\u5207\u7684\u4e8b\u3001\u6307\u6559\u4f60\u5011\u3001\u4e26\u4e14\u8981\u53eb\u4f60\u5011\u60f3\u8d77\u6211\u5c0d\u4f60\u5011\u6240\u8aaa\u7684\u4e00\u5207\u8a71\u3002"},{"id":27,"text":"\u6211\u7559\u4e0b\u5e73\u5b89\u7d66\u4f60\u5011\u3001\u6211\u5c07\u6211\u7684\u5e73\u5b89\u8cdc\u7d66\u4f60\u5011\uff0e\u6211\u6240\u8cdc\u7684\u3001\u4e0d\u50cf\u4e16\u4eba\u6240\u8cdc\u7684\uff0e\u4f60\u5011\u5fc3\u88e1\u4e0d\u8981\u6182\u6101\u3001\u4e5f\u4e0d\u8981\u81bd\u602f\u3002"},{"id":28,"text":"\u4f60\u5011\u807d\u898b\u6211\u5c0d\u4f60\u5011\u8aaa\u4e86\u3001\u6211\u53bb\u9084\u8981\u5230\u4f60\u5011\u9019\u88e1\u4f86\u3002\u4f60\u5011\u82e5\u611b\u6211\u3001\u56e0\u6211\u5230\u7236\u90a3\u88e1\u53bb\u3001\u5c31\u5fc5\u559c\u6a02\u3001\u56e0\u70ba\u7236\u662f\u6bd4\u6211\u5927\u7684\u3002"},{"id":29,"text":"\u73fe\u5728\u4e8b\u60c5\u9084\u6c92\u6709\u6210\u5c31\u3001\u6211\u8c6b\u5148\u544a\u8a34\u4f60\u5011\u3001\u53eb\u4f60\u5011\u5230\u4e8b\u60c5\u6210\u5c31\u7684\u6642\u5019\u3001\u5c31\u53ef\u4ee5\u4fe1\u3002"},{"id":30,"text":"\u4ee5\u5f8c\u6211\u4e0d\u518d\u548c\u4f60\u5011\u591a\u8aaa\u8a71\u3001\u56e0\u70ba\u9019\u4e16\u754c\u7684\u738b\u5c07\u5230\uff0e\u4ed6\u5728\u6211\u88e1\u9762\u662f\u6beb\u7121\u6240\u6709\uff0e"},{"id":31,"text":"\u4f46\u8981\u53eb\u4e16\u4eba\u77e5\u9053\u6211\u611b\u7236\u3001\u4e26\u4e14\u7236\u600e\u6a23\u5429\u5490\u6211\u3001\u6211\u5c31\u600e\u6a23\u884c\u3002\u8d77\u4f86\u3001\u6211\u5011\u8d70\u7f77\u3002"}]