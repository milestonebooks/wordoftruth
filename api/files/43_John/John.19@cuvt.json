[{"id":1,"text":"\u7576\u4e0b\u5f7c\u62c9\u591a\u5c07\u8036\u7a4c\u97ad\u6253\u4e86\u3002"},{"id":2,"text":"\u5175\u4e01\u7528\u834a\u68d8\u7de8\u4f5c\u51a0\u5195\u3001\u6234\u5728\u4ed6\u982d\u4e0a\u3001\u7d66\u4ed6\u7a7f\u4e0a\u7d2b\u888d\uff0e"},{"id":3,"text":"\u53c8\u6328\u8fd1\u4ed6\u8aaa\u3001\u606d\u559c\u7336\u592a\u4eba\u7684\u738b\u963f\uff0e\u4ed6\u5011\u5c31\u7528\u624b\u638c\u6253\u4ed6\u3002"},{"id":4,"text":"\u5f7c\u62c9\u591a\u53c8\u51fa\u4f86\u5c0d\u773e\u4eba\u8aaa\u3001\u6211\u5e36\u4ed6\u51fa\u4f86\u898b\u4f60\u5011\u3001\u53eb\u4f60\u5011\u77e5\u9053\u6211\u67e5\u4e0d\u51fa\u4ed6\u6709\u751a\u9ebc\u7f6a\u4f86\u3002"},{"id":5,"text":"\u8036\u7a4c\u51fa\u4f86\u3001\u6234\u8457\u834a\u68d8\u51a0\u5195\u3001\u7a7f\u8457\u7d2b\u888d\u3002\u5f7c\u62c9\u591a\u5c0d\u4ed6\u5011\u8aaa\u3001\u4f60\u5011\u770b\u9019\u500b\u4eba\u3002"},{"id":6,"text":"\u796d\u53f8\u9577\u548c\u5dee\u5f79\u770b\u898b\u4ed6\u3001\u5c31\u558a\u8457\u8aaa\u3001\u91d8\u4ed6\u5341\u5b57\u67b6\u3001\u91d8\u4ed6\u5341\u5b57\u67b6\u3002\u5f7c\u62c9\u591a\u8aaa\u3001\u4f60\u5011\u81ea\u5df1\u628a\u4ed6\u91d8\u5341\u5b57\u67b6\u7f77\uff0e\u6211\u67e5\u4e0d\u51fa\u4ed6\u6709\u751a\u9ebc\u7f6a\u4f86\u3002"},{"id":7,"text":"\u7336\u592a\u4eba\u56de\u7b54\u8aaa\u3001\u6211\u5011\u6709\u5f8b\u6cd5\u3001\u6309\u90a3\u5f8b\u6cd5\u3001\u4ed6\u662f\u8a72\u6b7b\u7684\u3001\u56e0\u4ed6\u4ee5\u81ea\u5df1\u70ba\u3000\u795e\u7684\u5152\u5b50\u3002"},{"id":8,"text":"\u5f7c\u62c9\u591a\u807d\u898b\u9019\u8a71\u3001\u8d8a\u767c\u5bb3\u6015\u3002"},{"id":9,"text":"\u53c8\u9032\u8859\u9580\u3001\u5c0d\u8036\u7a4c\u8aaa\u3001\u4f60\u662f\u90a3\u88e1\u4f86\u7684\uff0e\u8036\u7a4c\u537b\u4e0d\u56de\u7b54\u3002"},{"id":10,"text":"\u5f7c\u62c9\u591a\u8aaa\u3001\u4f60\u4e0d\u5c0d\u6211\u8aaa\u8a71\u9ebc\uff0e\u4f60\u8c48\u4e0d\u77e5\u6211\u6709\u6b0a\u67c4\u91cb\u653e\u4f60\u3001\u4e5f\u6709\u6b0a\u67c4\u628a\u4f60\u91d8\u5341\u5b57\u67b6\u9ebc\u3002"},{"id":11,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u82e5\u4e0d\u662f\u5f9e\u4e0a\u982d\u8cdc\u7d66\u4f60\u7684\u3001\u4f60\u5c31\u4eb3\u7121\u6b0a\u67c4\u8fa6\u6211\uff0e\u6240\u4ee5\u628a\u6211\u4ea4\u7d66\u4f60\u7684\u90a3\u4eba\u3001\u7f6a\u66f4\u91cd\u4e86\u3002"},{"id":12,"text":"\u5f9e\u6b64\u5f7c\u62c9\u591a\u60f3\u8981\u91cb\u653e\u8036\u7a4c\uff0e\u7121\u5948\u7336\u592a\u4eba\u558a\u8457\u8aaa\u3001\u4f60\u82e5\u91cb\u653e\u9019\u500b\u4eba\u3001\u5c31\u4e0d\u662f\u8a72\u6492\u7684\u5fe0\u81e3\uff0e\u3014\u539f\u6587\u4f5c\u670b\u53cb\u3015\u51e1\u4ee5\u81ea\u5df1\u70ba\u738b\u7684\u3001\u5c31\u662f\u80cc\u53db\u8a72\u6492\u4e86\u3002"},{"id":13,"text":"\u5f7c\u62c9\u591a\u807d\u898b\u9019\u8a71\u3001\u5c31\u5e36\u8036\u7a4c\u51fa\u4f86\u3001\u5230\u4e86\u4e00\u500b\u5730\u65b9\u3001\u540d\u53eb\u92ea\u83ef\u77f3\u8655\u3001\u5e0c\u4f2f\u4f86\u8a71\u53eb\u5384\u5df4\u5927\u3001\u5c31\u5728\u90a3\u88e1\u5750\u5802\u3002"},{"id":14,"text":"\u90a3\u65e5\u662f\u8c6b\u5099\u903e\u8d8a\u7bc0\u7684\u65e5\u5b50\u3001\u7d04\u6709\u5348\u6b63\u3002\u5f7c\u62c9\u591a\u5c0d\u7336\u592a\u4eba\u8aaa\u3001\u770b\u54ea\u3001\u9019\u662f\u4f60\u5011\u7684\u738b\u3002"},{"id":15,"text":"\u4ed6\u5011\u558a\u8457\u8aaa\u3001\u9664\u6389\u4ed6\u3001\u9664\u6389\u4ed6\u3001\u91d8\u4ed6\u5728\u5341\u5b57\u67b6\u4e0a\u3002\u5f7c\u62c9\u591a\u8aaa\u3001\u6211\u53ef\u4ee5\u628a\u4f60\u5011\u7684\u738b\u91d8\u5341\u5b57\u67b6\u9ebc\u3002\u796d\u53f8\u9577\u56de\u7b54\u8aaa\u3001\u9664\u4e86\u8a72\u6492\u3001\u6211\u5011\u6c92\u6709\u738b\u3002"},{"id":16,"text":"\u65bc\u662f\u5f7c\u62c9\u591a\u5c07\u8036\u7a4c\u4ea4\u7d66\u4ed6\u5011\u53bb\u91d8\u5341\u5b57\u67b6\u3002"},{"id":17,"text":"\u4ed6\u5011\u5c31\u628a\u8036\u7a4c\u5e36\u4e86\u53bb\uff0e\u8036\u7a4c\u80cc\u8457\u81ea\u5df1\u7684\u5341\u5b57\u67b6\u51fa\u4f86\u3001\u5230\u4e86\u4e00\u500b\u5730\u65b9\u3001\u540d\u53eb\u9ad1\u9acf\u5730\u3001\u5e0c\u4f2f\u4f86\u8a71\u53eb\u5404\u5404\u4ed6\u3002"},{"id":18,"text":"\u4ed6\u5011\u5c31\u5728\u90a3\u88e1\u91d8\u4ed6\u5728\u5341\u5b57\u67b6\u4e0a\u3001\u9084\u6709\u5169\u500b\u4eba\u548c\u4ed6\u4e00\u540c\u91d8\u8457\u3001\u4e00\u908a\u4e00\u500b\u3001\u8036\u7a4c\u5728\u4e2d\u9593\u3002"},{"id":19,"text":"\u5f7c\u62c9\u591a\u53c8\u7528\u724c\u5b50\u5beb\u4e86\u4e00\u500b\u540d\u865f\u3001\u5b89\u5728\u5341\u5b57\u67b6\u4e0a\uff0e\u5beb\u7684\u662f\u7336\u592a\u4eba\u7684\u738b\u3001\u62ff\u6492\u52d2\u4eba\u8036\u7a4c\u3002"},{"id":20,"text":"\u6709\u8a31\u591a\u7336\u592a\u4eba\u5ff5\u9019\u540d\u865f\uff0e\u56e0\u70ba\u8036\u7a4c\u88ab\u91d8\u5341\u5b57\u67b6\u7684\u5730\u65b9\u3001\u8207\u57ce\u76f8\u8fd1\u3001\u4e26\u4e14\u662f\u7528\u5e0c\u4f2f\u4f86\u3001\u7f85\u99ac\u3001\u5e0c\u5229\u5c3c\u3001\u4e09\u6a23\u6587\u5b57\u5beb\u7684\u3002"},{"id":21,"text":"\u7336\u592a\u4eba\u7684\u796d\u53f8\u9577\u3001\u5c31\u5c0d\u5f7c\u62c9\u591a\u8aaa\u3001\u4e0d\u8981\u5beb\u7336\u592a\u4eba\u7684\u738b\uff0e\u8981\u5beb\u4ed6\u81ea\u5df1\u8aaa\u6211\u662f\u7336\u592a\u4eba\u7684\u738b\u3002"},{"id":22,"text":"\u5f7c\u62c9\u591a\u8aaa\u3001\u6211\u6240\u5beb\u7684\u3001\u6211\u5df2\u7d93\u5beb\u4e0a\u4e86\u3002"},{"id":23,"text":"\u5175\u4e01\u65e2\u7136\u5c07\u8036\u7a4c\u91d8\u5728\u5341\u5b57\u67b6\u4e0a\u3001\u5c31\u62ff\u4ed6\u7684\u8863\u670d\u5206\u70ba\u56db\u5206\u3001\u6bcf\u5175\u4e00\u5206\uff0e\u53c8\u62ff\u4ed6\u7684\u88e1\u8863\uff0e\u9019\u4ef6\u88e1\u8863\u3001\u539f\u4f86\u6c92\u6709\u7e2b\u5152\u3001\u662f\u4e0a\u4e0b\u4e00\u7247\u7e54\u6210\u7684\u3002"},{"id":24,"text":"\u4ed6\u5011\u5c31\u5f7c\u6b64\u8aaa\u3001\u6211\u5011\u4e0d\u8981\u6495\u958b\u3001\u53ea\u8981\u62c8\u9b2e\u3001\u770b\u8ab0\u5f97\u8457\uff0e\u9019\u8981\u61c9\u9a57\u7d93\u4e0a\u7684\u8a71\u8aaa\u3001\u300e\u4ed6\u5011\u5206\u4e86\u6211\u7684\u5916\u8863\u3001\u70ba\u6211\u7684\u88e1\u8863\u62c8\u9b2e\u3002\u300f\u5175\u4e01\u679c\u7136\u4f5c\u4e86\u9019\u4e8b\u3002"},{"id":25,"text":"\u7ad9\u5728\u8036\u7a4c\u5341\u5b57\u67b6\u65c1\u908a\u7684\u3001\u6709\u4ed6\u6bcd\u89aa\u3001\u8207\u4ed6\u6bcd\u89aa\u7684\u59ca\u59b9\u3001\u4e26\u9769\u7f85\u7f77\u7684\u59bb\u5b50\u99ac\u5229\u4e9e\u3001\u548c\u62b9\u5927\u62c9\u7684\u99ac\u5229\u4e9e\u3002"},{"id":26,"text":"\u8036\u7a4c\u898b\u6bcd\u89aa\u548c\u4ed6\u6240\u611b\u7684\u90a3\u9580\u5f92\u7ad9\u5728\u65c1\u908a\u3001\u5c31\u5c0d\u4ed6\u6bcd\u89aa\u8aaa\u3001\u6bcd\u89aa\u3001\u3014\u539f\u6587\u4f5c\u5a66\u4eba\u3015\u770b\u4f60\u7684\u5152\u5b50\u3002"},{"id":27,"text":"\u53c8\u5c0d\u90a3\u9580\u5f92\u8aaa\u3001\u770b\u4f60\u7684\u6bcd\u89aa\u3002\u5f9e\u6b64\u90a3\u9580\u5f92\u5c31\u63a5\u4ed6\u5230\u81ea\u5df1\u5bb6\u88e1\u53bb\u4e86\u3002"},{"id":28,"text":"\u9019\u4e8b\u4ee5\u5f8c\u3001\u8036\u7a4c\u77e5\u9053\u5404\u6a23\u7684\u4e8b\u5df2\u7d93\u6210\u4e86\u3001\u70ba\u8981\u4f7f\u7d93\u4e0a\u7684\u8a71\u61c9\u9a57\u3001\u5c31\u8aaa\u3001\u6211\u6e34\u4e86\u3002"},{"id":29,"text":"\u6709\u4e00\u500b\u5668\u76bf\u76db\u6eff\u4e86\u918b\u3001\u653e\u5728\u90a3\u88e1\uff0e\u4ed6\u5011\u5c31\u62ff\u6d77\u7d68\u8638\u6eff\u4e86\u918b\u3001\u7d81\u5728\u725b\u819d\u8349\u4e0a\u3001\u9001\u5230\u4ed6\u53e3\u3002"},{"id":30,"text":"\u8036\u7a4c\u5617\u3014\u539f\u6587\u4f5c\u53d7\u3015\u4e86\u90a3\u918b\u3001\u5c31\u8aaa\u3001\u6210\u4e86\uff0e\u4fbf\u4f4e\u4e0b\u982d\u3001\u5c07\u9748\u9b42\u4ea4\u4ed8\u3000\u795e\u4e86\u3002"},{"id":31,"text":"\u7336\u592a\u4eba\u56e0\u9019\u65e5\u662f\u8c6b\u5099\u65e5\u3001\u53c8\u56e0\u90a3\u5b89\u606f\u65e5\u662f\u500b\u5927\u65e5\u3001\u5c31\u6c42\u5f7c\u62c9\u591a\u53eb\u4eba\u6253\u65b7\u4ed6\u5011\u7684\u817f\u3001\u628a\u4ed6\u5011\u62ff\u53bb\u3001\u514d\u5f97\u5c4d\u9996\u7576\u5b89\u606f\u65e5\u7559\u5728\u5341\u5b57\u67b6\u4e0a\u3002"},{"id":32,"text":"\u65bc\u662f\u5175\u4e01\u4f86\u4e86\u3001\u628a\u982d\u4e00\u500b\u4eba\u7684\u817f\u3001\u4e26\u8207\u8036\u7a4c\u540c\u91d8\u7b2c\u4e8c\u500b\u4eba\u7684\u817f\u3001\u90fd\u6253\u65b7\u4e86\u3002"},{"id":33,"text":"\u53ea\u662f\u4f86\u5230\u8036\u7a4c\u90a3\u88e1\u3001\u898b\u4ed6\u5df2\u7d93\u6b7b\u4e86\u3001\u5c31\u4e0d\u6253\u65b7\u4ed6\u7684\u817f\u3002"},{"id":34,"text":"\u60df\u6709\u4e00\u500b\u5175\u62ff\u69cd\u624e\u4ed6\u7684\u808b\u65c1\u3001\u96a8\u5373\u6709\u8840\u548c\u6c34\u6d41\u51fa\u4f86\u3002"},{"id":35,"text":"\u770b\u898b\u9019\u4e8b\u7684\u90a3\u4eba\u5c31\u4f5c\u898b\u8b49\u3001\u4ed6\u7684\u898b\u8b49\u4e5f\u662f\u771f\u7684\u3001\u4e26\u4e14\u4ed6\u77e5\u9053\u81ea\u5df1\u6240\u8aaa\u7684\u662f\u771f\u7684\u3001\u53eb\u4f60\u5011\u4e5f\u53ef\u4ee5\u4fe1\u3002"},{"id":36,"text":"\u9019\u4e9b\u4e8b\u6210\u4e86\u3001\u70ba\u8981\u61c9\u9a57\u7d93\u4e0a\u7684\u8a71\u8aaa\u3001\u300e\u4ed6\u7684\u9aa8\u982d\u3001\u4e00\u6839\u4e5f\u4e0d\u6298\u65b7\u3002\u300f"},{"id":37,"text":"\u7d93\u4e0a\u53c8\u6709\u4e00\u53e5\u8aaa\u3001\u300e\u4ed6\u5011\u8981\u4ef0\u671b\u81ea\u5df1\u6240\u624e\u7684\u4eba\u3002\u300f"},{"id":38,"text":"\u9019\u4e9b\u4e8b\u4ee5\u5f8c\u3001\u6709\u4e9e\u5229\u99ac\u592a\u4eba\u7d04\u745f\u3001\u662f\u8036\u7a4c\u7684\u9580\u5f92\u3001\u53ea\u56e0\u6015\u7336\u592a\u4eba\u3001\u5c31\u6697\u6697\u7684\u4f5c\u9580\u5f92\u3001\u4ed6\u4f86\u6c42\u5f7c\u62c9\u591a\u3001\u8981\u628a\u8036\u7a4c\u7684\u8eab\u9ad4\u9818\u53bb\uff0e\u5f7c\u62c9\u591a\u5141\u51c6\u3001\u4ed6\u5c31\u628a\u8036\u7a4c\u7684\u8eab\u9ad4\u9818\u53bb\u4e86\u3002"},{"id":39,"text":"\u53c8\u6709\u5c3c\u54e5\u5e95\u6bcd\u3001\u5c31\u662f\u5148\u524d\u591c\u88e1\u53bb\u898b\u8036\u7a4c\u7684\u3001\u5e36\u8457\u6c92\u85e5\u3001\u548c\u6c89\u9999\u3001\u7d04\u6709\u4e00\u767e\u65a4\u524d\u4f86\u3002"},{"id":40,"text":"\u4ed6\u5011\u5c31\u7167\u7336\u592a\u4eba\u6baf\u846c\u7684\u898f\u77e9\u3001\u628a\u8036\u7a4c\u7684\u8eab\u9ad4\u7528\u3001\u7d30\u9ebb\u5e03\u52a0\u4e0a\u9999\u6599\u88f9\u597d\u4e86\u3002"},{"id":41,"text":"\u5728\u8036\u7a4c\u91d8\u5341\u5b57\u67b6\u7684\u5730\u65b9\u3001\u6709\u4e00\u500b\u5712\u5b50\uff0e\u5712\u5b50\u88e1\u6709\u4e00\u5ea7\u65b0\u58b3\u5893\u3001\u662f\u5f9e\u4f86\u6c92\u6709\u846c\u904e\u4eba\u7684\u3002"},{"id":42,"text":"\u53ea\u56e0\u662f\u7336\u592a\u4eba\u7684\u8c6b\u5099\u65e5\u3001\u53c8\u56e0\u90a3\u58b3\u5893\u8fd1\u3001\u4ed6\u5011\u5c31\u628a\u8036\u7a4c\u5b89\u653e\u5728\u90a3\u88e1\u3002"}]