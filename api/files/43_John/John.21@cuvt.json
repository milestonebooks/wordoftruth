[{"id":1,"text":"\u9019\u4e9b\u4e8b\u4ee5\u5f8c\u3001\u8036\u7a4c\u5728\u63d0\u6bd4\u54e9\u4e9e\u6d77\u908a\u3001\u53c8\u5411\u9580\u5f92\u986f\u73fe\uff0e\u4ed6\u600e\u6a23\u986f\u73fe\u8a18\u5728\u4e0b\u9762\u3002"},{"id":2,"text":"\u6709\u897f\u9580\u5f7c\u5f97\u548c\u7a31\u70ba\u4f4e\u571f\u99ac\u7684\u591a\u99ac\u3001\u4e26\u52a0\u5229\u5229\u7684\u8fe6\u62ff\u4eba\u62ff\u4f46\u696d\u3001\u9084\u6709\u897f\u5e87\u592a\u7684\u5169\u500b\u5152\u5b50\u3001\u53c8\u6709\u5169\u500b\u9580\u5f92\u3001\u90fd\u5728\u4e00\u8655\u3002"},{"id":3,"text":"\u897f\u9580\u5f7c\u5f97\u5c0d\u4ed6\u5011\u8aaa\u3001\u6211\u6253\u9b5a\u53bb\uff0e\u4ed6\u5011\u8aaa\u3001\u6211\u5011\u4e5f\u548c\u4f60\u540c\u53bb\u3002\u4ed6\u5011\u5c31\u51fa\u53bb\u3001\u4e0a\u4e86\u8239\u3001\u90a3\u4e00\u591c\u4e26\u6c92\u6709\u6253\u8457\u751a\u9ebc\u3002"},{"id":4,"text":"\u5929\u5c07\u4eae\u7684\u6642\u5019\u3001\u8036\u7a4c\u7ad9\u5728\u5cb8\u4e0a\uff0e\u9580\u5f92\u537b\u4e0d\u77e5\u9053\u662f\u8036\u7a4c\u3002"},{"id":5,"text":"\u8036\u7a4c\u5c31\u5c0d\u4ed6\u5011\u8aaa\u3001\u5c0f\u5b50\u3001\u4f60\u5011\u6709\u55ab\u7684\u6c92\u6709\u3002\u4ed6\u5011\u56de\u7b54\u8aaa\u3001\u6c92\u6709\u3002"},{"id":6,"text":"\u8036\u7a4c\u8aaa\u3001\u4f60\u5011\u628a\u7db2\u6492\u5728\u8239\u7684\u53f3\u908a\u3001\u5c31\u5fc5\u5f97\u8457\u3002\u4ed6\u5011\u4fbf\u6492\u4e0b\u7db2\u53bb\u3001\u7adf\u62c9\u4e0d\u4e0a\u4f86\u4e86\u3001\u56e0\u70ba\u9b5a\u751a\u591a\u3002"},{"id":7,"text":"\u8036\u7a4c\u6240\u611b\u7684\u90a3\u9580\u5f92\u5c0d\u5f7c\u5f97\u8aaa\u3001\u662f\u4e3b\u3002\u90a3\u6642\u897f\u9580\u5f7c\u5f97\u8d64\u8457\u8eab\u5b50\u3001\u4e00\u807d\u898b\u662f\u4e3b\u3001\u5c31\u675f\u4e0a\u4e00\u4ef6\u5916\u8863\u3001\u8df3\u5728\u6d77\u88e1\u3002"},{"id":8,"text":"\u5176\u9918\u7684\u9580\u5f92\u3001\uff08\u96e2\u5cb8\u4e0d\u9060\u3001\u7d04\u6709\u4e8c\u767e\u8098\u3014\u53e4\u6642\u4ee5\u8098\u70ba\u5c3a\u4e00\u8098\u7d04\u6709\u4eca\u6642\u5c3a\u534a\u3015\uff09\u5c31\u5728\u5c0f\u8239\u4e0a\u628a\u90a3\u7db2\u9b5a\u62c9\u904e\u4f86\u3002"},{"id":9,"text":"\u4ed6\u5011\u4e0a\u4e86\u5cb8\u3001\u5c31\u770b\u898b\u90a3\u88e1\u6709\u70ad\u706b\u3001\u4e0a\u9762\u6709\u9b5a\u3001\u53c8\u6709\u9905\u3002"},{"id":10,"text":"\u8036\u7a4c\u5c0d\u4ed6\u5011\u8aaa\u3001\u628a\u525b\u7e94\u6253\u7684\u9b5a\u62ff\u5e7e\u689d\u4f86\u3002"},{"id":11,"text":"\u897f\u9580\u5f7c\u5f97\u5c31\u53bb\u3001\u3014\u6216\u4f5c\u4e0a\u8239\u3015\u628a\u7db2\u62c9\u5230\u5cb8\u4e0a\u3001\u90a3\u7db2\u6eff\u4e86\u5927\u9b5a\u3001\u5171\u4e00\u767e\u4e94\u5341\u4e09\u689d\uff0e\u9b5a\u96d6\u9019\u6a23\u591a\u3001\u7db2\u537b\u6c92\u6709\u7834\u3002"},{"id":12,"text":"\u8036\u7a4c\u8aaa\u3001\u4f60\u5011\u4f86\u55ab\u65e9\u98ef\uff0e\u9580\u5f92\u4e2d\u6c92\u6709\u4e00\u500b\u6562\u554f\u4ed6\u3001\u4f60\u662f\u8ab0\u3001\u56e0\u70ba\u77e5\u9053\u662f\u4e3b\u3002"},{"id":13,"text":"\u8036\u7a4c\u5c31\u4f86\u62ff\u9905\u548c\u9b5a\u7d66\u4ed6\u5011\u3002"},{"id":14,"text":"\u8036\u7a4c\u5f9e\u6b7b\u88e1\u5fa9\u6d3b\u4ee5\u5f8c\u3001\u5411\u9580\u5f92\u986f\u73fe\u3001\u9019\u662f\u7b2c\u4e09\u6b21\u3002"},{"id":15,"text":"\u4ed6\u5011\u55ab\u5b8c\u4e86\u65e9\u98ef\u3001\u8036\u7a4c\u5c0d\u897f\u9580\u5f7c\u5f97\u8aaa\u3001\u7d04\u7ff0\u7684\u5152\u5b50\u897f\u9580\u3001\u3014\u7d04\u7ff0\u99ac\u592a\u5341\u516d\u7ae0\u5341\u4e03\u7bc0\u7a31\u7d04\u62ff\u3015\u4f60\u611b\u6211\u6bd4\u9019\u4e9b\u66f4\u6df1\u9ebc\u3002\u5f7c\u5f97\u8aaa\u3001\u4e3b\u963f\u3001\u662f\u7684\uff0e\u4f60\u77e5\u9053\u6211\u611b\u4f60\u3002\u8036\u7a4c\u5c0d\u4ed6\u8aaa\u3001\u4f60\u9927\u990a\u6211\u7684\u5c0f\u7f8a\u3002"},{"id":16,"text":"\u8036\u7a4c\u7b2c\u4e8c\u6b21\u53c8\u5c0d\u4ed6\u8aaa\u3001\u7d04\u7ff0\u7684\u5152\u5b50\u897f\u9580\u3001\u4f60\u611b\u6211\u9ebc\u3002\u5f7c\u5f97\u8aaa\u3001\u4e3b\u963f\u3001\u662f\u7684\uff0e\u4f60\u77e5\u9053\u6211\u611b\u4f60\uff0e\u8036\u7a4c\u8aaa\u3001\u4f60\u7267\u990a\u6211\u7684\u7f8a\u3002"},{"id":17,"text":"\u7b2c\u4e09\u6b21\u5c0d\u4ed6\u8aaa\u3001\u7d04\u7ff0\u7684\u5152\u5b50\u897f\u9580\u3001\u4f60\u611b\u6211\u9ebc\u3002\u5f7c\u5f97\u56e0\u70ba\u8036\u7a4c\u7b2c\u4e09\u6b21\u5c0d\u4ed6\u8aaa\u3001\u4f60\u611b\u6211\u9ebc\u3001\u5c31\u6182\u6101\u3001\u5c0d\u8036\u7a4c\u8aaa\u3001\u4e3b\u963f\u3001\u4f60\u662f\u7121\u6240\u4e0d\u77e5\u7684\u3001\u4f60\u77e5\u9053\u6211\u611b\u4f60\u3002\u8036\u7a4c\u8aaa\u3001\u4f60\u9927\u990a\u6211\u7684\u7f8a\u3002"},{"id":18,"text":"\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u3001\u4f60\u5e74\u5c11\u7684\u6642\u5019\u3001\u81ea\u5df1\u675f\u4e0a\u5e36\u5b50\u3001\u96a8\u610f\u5f80\u4f86\u3001\u4f46\u5e74\u8001\u7684\u6642\u5019\u3001\u4f60\u8981\u4f38\u51fa\u624b\u4f86\u3001\u5225\u4eba\u8981\u628a\u4f60\u675f\u4e0a\u3001\u5e36\u4f60\u5230\u4e0d\u9858\u610f\u53bb\u7684\u5730\u65b9\u3002"},{"id":19,"text":"\u8036\u7a4c\u8aaa\u9019\u8a71\u3001\u662f\u6307\u8457\u5f7c\u5f97\u8981\u600e\u6a23\u6b7b\u69ae\u8000\u3000\u795e\u3002\u8aaa\u4e86\u9019\u8a71\u3001\u5c31\u5c0d\u4ed6\u8aaa\u3001\u4f60\u8ddf\u5f9e\u6211\u7f77\u3002"},{"id":20,"text":"\u5f7c\u5f97\u8f49\u904e\u4f86\u3001\u770b\u898b\u8036\u7a4c\u6240\u611b\u7684\u90a3\u9580\u5f92\u8ddf\u8457\u3001\u5c31\u662f\u5728\u665a\u98ef\u7684\u6642\u5019\u3001\u9760\u8457\u8036\u7a4c\u80f8\u819b\u3001\u8aaa\u3001\u4e3b\u963f\u3001\u8ce3\u4f60\u7684\u662f\u8ab0\u7684\u90a3\u9580\u5f92\u3002"},{"id":21,"text":"\u5f7c\u5f97\u770b\u898b\u4ed6\u3001\u5c31\u554f\u8036\u7a4c\u8aaa\u3001\u4e3b\u963f\u3001\u9019\u4eba\u5c07\u4f86\u5982\u4f55\u3002"},{"id":22,"text":"\u8036\u7a4c\u5c0d\u4ed6\u8aaa\u3001\u6211\u82e5\u8981\u4ed6\u7b49\u5230\u6211\u4f86\u7684\u6642\u5019\u3001\u8207\u4f60\u4f55\u5e72\uff0e\u4f60\u8ddf\u5f9e\u6211\u7f77\u3002"},{"id":23,"text":"\u65bc\u662f\u9019\u8a71\u50b3\u5728\u5f1f\u5144\u4e2d\u9593\u3001\u8aaa\u90a3\u9580\u5f92\u4e0d\u6b7b\uff0e\u5176\u5be6\u8036\u7a4c\u4e0d\u662f\u8aaa\u4ed6\u4e0d\u6b7b\uff0e\u4e43\u662f\u8aaa\u6211\u82e5\u8981\u4ed6\u7b49\u5230\u6211\u4f86\u7684\u6642\u5019\u3001\u8207\u4f60\u4f55\u5e72\u3002"},{"id":24,"text":"\u70ba\u9019\u4e9b\u4e8b\u4f5c\u898b\u8b49\u3001\u4e26\u4e14\u8a18\u8f09\u9019\u4e9b\u4e8b\u7684\u3001\u5c31\u662f\u9019\u9580\u5f92\uff0e\u6211\u5011\u4e5f\u77e5\u9053\u4ed6\u7684\u898b\u8b49\u662f\u771f\u7684\u3002"},{"id":25,"text":"\u8036\u7a4c\u6240\u884c\u7684\u4e8b\u3001\u9084\u6709\u8a31\u591a\u3001\u82e5\u662f\u4e00\u4e00\u7684\u90fd\u5beb\u51fa\u4f86\u3001\u6211\u60f3\u6240\u5beb\u7684\u66f8\u3001\u5c31\u662f\u4e16\u754c\u4e5f\u5bb9\u4e0d\u4e0b\u4e86\u3002"}]