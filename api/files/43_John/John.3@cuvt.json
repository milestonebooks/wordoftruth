[{"id":1,"text":"\u6709\u4e00\u500b\u6cd5\u5229\u8cfd\u4eba\u3001\u540d\u53eb\u5c3c\u54e5\u5e95\u6bcd\u3001\u662f\u7336\u592a\u4eba\u7684\u5b98\u3002"},{"id":2,"text":"\u9019\u4eba\u591c\u88e1\u4f86\u898b\u8036\u7a4c\u3001\u8aaa\u3001\u62c9\u6bd4\u3001\u6211\u5011\u77e5\u9053\u4f60\u662f\u7531\u3000\u795e\u90a3\u88e1\u4f86\u4f5c\u5e2b\u5085\u7684\uff0e\u56e0\u70ba\u4f60\u6240\u884c\u7684\u795e\u8e5f\u3001\u82e5\u6c92\u6709\u3000\u795e\u540c\u5728\u3001\u7121\u4eba\u80fd\u884c\u3002"},{"id":3,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u3001\u4eba\u82e5\u4e0d\u91cd\u751f\u3001\u5c31\u4e0d\u80fd\u898b\u3000\u795e\u7684\u570b\u3002"},{"id":4,"text":"\u5c3c\u54e5\u5e95\u6bcd\u8aaa\u3001\u4eba\u5df2\u7d93\u8001\u4e86\u3001\u5982\u4f55\u80fd\u91cd\u751f\u5462\uff0e\u8c48\u80fd\u518d\u9032\u6bcd\u8179\u751f\u51fa\u4f86\u9ebc\u3002"},{"id":5,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u3001\u4eba\u82e5\u4e0d\u662f\u5f9e\u6c34\u548c\u8056\u9748\u751f\u7684\u3001\u5c31\u4e0d\u80fd\u9032\u3000\u795e\u7684\u570b\u3002"},{"id":6,"text":"\u5f9e\u8089\u8eab\u751f\u7684\u3001\u5c31\u662f\u8089\u8eab\uff0e\u5f9e\u9748\u751f\u7684\u3001\u5c31\u662f\u9748\u3002"},{"id":7,"text":"\u6211\u8aaa\u3001\u4f60\u5011\u5fc5\u9808\u91cd\u751f\u3001\u4f60\u4e0d\u8981\u4ee5\u70ba\u5e0c\u5947\u3002"},{"id":8,"text":"\u98a8\u96a8\u8457\u610f\u601d\u5439\u3001\u4f60\u807d\u898b\u98a8\u7684\u97ff\u8072\u3001\u537b\u4e0d\u66c9\u5f97\u5f9e\u90a3\u88e1\u4f86\u3001\u5f80\u90a3\u88e1\u53bb\uff0e\u51e1\u5f9e\u8056\u9748\u751f\u7684\u3001\u4e5f\u662f\u5982\u6b64\u3002"},{"id":9,"text":"\u5c3c\u54e5\u5e95\u6bcd\u554f\u4ed6\u8aaa\u3001\u600e\u80fd\u6709\u9019\u4e8b\u5462\u3002"},{"id":10,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u4f60\u662f\u4ee5\u8272\u5217\u4eba\u7684\u5148\u751f\u3001\u9084\u4e0d\u660e\u767d\u9019\u4e8b\u9ebc\u3002"},{"id":11,"text":"\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u3001\u6211\u5011\u6240\u8aaa\u7684\u3001\u662f\u6211\u5011\u77e5\u9053\u7684\u3001\u6211\u5011\u6240\u898b\u8b49\u7684\u3001\u662f\u6211\u5011\u898b\u904e\u7684\uff0e\u4f60\u5011\u537b\u4e0d\u9818\u53d7\u6211\u5011\u7684\u898b\u8b49\u3002"},{"id":12,"text":"\u6211\u5c0d\u4f60\u5011\u8aaa\u5730\u4e0a\u7684\u4e8b\u3001\u4f60\u5011\u5c1a\u4e14\u4e0d\u4fe1\u3001\u82e5\u8aaa\u5929\u4e0a\u7684\u4e8b\u3001\u5982\u4f55\u80fd\u4fe1\u5462\u3002"},{"id":13,"text":"\u9664\u4e86\u5f9e\u5929\u964d\u4e0b\u4ecd\u820a\u5728\u5929\u7684\u4eba\u5b50\u3001\u6c92\u6709\u4eba\u5347\u904e\u5929\u3002"},{"id":14,"text":"\u6469\u897f\u5728\u66e0\u91ce\u600e\u6a23\u8209\u86c7\u3001\u4eba\u5b50\u4e5f\u5fc5\u7167\u6a23\u88ab\u8209\u8d77\u4f86\uff0e"},{"id":15,"text":"\u53eb\u4e00\u5207\u4fe1\u4ed6\u7684\u90fd\u5f97\u6c38\u751f\u3002\u3014\u6216\u4f5c\u53eb\u4e00\u5207\u4fe1\u7684\u4eba\u5728\u4ed6\u88e1\u9762\u5f97\u6c38\u751f\u3015"},{"id":16,"text":"\u795e\u611b\u4e16\u4eba\u3001\u751a\u81f3\u5c07\u4ed6\u7684\u7368\u751f\u5b50\u8cdc\u7d66\u4ed6\u5011\u3001\u53eb\u4e00\u5207\u4fe1\u4ed6\u7684\u3001\u4e0d\u81f3\u6ec5\u4ea1\u3001\u53cd\u5f97\u6c38\u751f\u3002"},{"id":17,"text":"\u56e0\u70ba\u3000\u795e\u5dee\u4ed6\u7684\u8eab\u5b50\u964d\u4e16\u3001\u4e0d\u662f\u8981\u5b9a\u4e16\u4eba\u7684\u7f6a\u3001\u3014\u6216\u4f5c\u5be9\u5224\u4e16\u4eba\u4e0b\u540c\u3015\u4e43\u662f\u8981\u53eb\u4e16\u4eba\u56e0\u4ed6\u5f97\u6551\u3002"},{"id":18,"text":"\u4fe1\u4ed6\u7684\u4eba\u3001\u4e0d\u88ab\u5b9a\u7f6a\uff0e\u4e0d\u4fe1\u7684\u4eba\u3001\u7f6a\u5df2\u7d93\u5b9a\u4e86\u3001\u56e0\u70ba\u4ed6\u4e0d\u4fe1\u3000\u795e\u7368\u751f\u5b50\u7684\u540d\u3002"},{"id":19,"text":"\u5149\u4f86\u5230\u4e16\u9593\u3001\u4e16\u4eba\u56e0\u81ea\u5df1\u7684\u884c\u70ba\u662f\u60e1\u7684\u3001\u4e0d\u611b\u5149\u5012\u611b\u9ed1\u6697\u3001\u5b9a\u4ed6\u5011\u7684\u7f6a\u5c31\u662f\u5728\u6b64\u3002"},{"id":20,"text":"\u51e1\u4f5c\u60e1\u7684\u4fbf\u6068\u5149\u3001\u4e26\u4e0d\u4f86\u5c31\u5149\u3001\u6050\u6015\u4ed6\u7684\u884c\u70ba\u53d7\u8cac\u5099\u3002"},{"id":21,"text":"\u4f46\u884c\u771f\u7406\u7684\u5fc5\u4f86\u5c31\u5149\u3001\u8981\u986f\u660e\u4ed6\u6240\u884c\u7684\u662f\u9760\u3000\u795e\u800c\u884c\u3002"},{"id":22,"text":"\u9019\u4e8b\u4ee5\u5f8c\u3001\u8036\u7a4c\u548c\u9580\u5f92\u5230\u4e86\u7336\u592a\u5730\u3001\u5728\u90a3\u88e1\u5c45\u4f4f\u65bd\u6d17\u3002"},{"id":23,"text":"\u7d04\u7ff0\u5728\u9760\u8fd1\u6492\u51b7\u7684\u54c0\u5ae9\u4e5f\u65bd\u6d17\u3001\u56e0\u70ba\u90a3\u88e1\u6c34\u591a\uff0e\u773e\u4eba\u90fd\u53bb\u53d7\u6d17\u3002"},{"id":24,"text":"\u90a3\u6642\u7d04\u7ff0\u9084\u6c92\u6709\u4e0b\u5728\u76e3\u88e1\u3002"},{"id":25,"text":"\u7d04\u7ff0\u7684\u9580\u5f92\u3001\u548c\u4e00\u500b\u7336\u592a\u4eba\u8faf\u8ad6\u6f54\u6de8\u7684\u79ae\u3002"},{"id":26,"text":"\u5c31\u4f86\u898b\u7d04\u7ff0\u8aaa\u3001\u62c9\u6bd4\u3001\u5f9e\u524d\u540c\u4f60\u5728\u7d04\u4f46\u6cb3\u5916\u3001\u4f60\u6240\u898b\u8b49\u7684\u90a3\u4f4d\u3001\u73fe\u5728\u65bd\u6d17\u3001\u773e\u4eba\u90fd\u5f80\u4ed6\u90a3\u88e1\u53bb\u4e86\u3002"},{"id":27,"text":"\u7d04\u7ff0\u8aaa\u3001\u82e5\u4e0d\u662f\u5f9e\u5929\u4e0a\u8cdc\u7684\u3001\u4eba\u5c31\u4e0d\u80fd\u5f97\u751a\u9ebc\u3002"},{"id":28,"text":"\u6211\u66fe\u8aaa\u3001\u6211\u4e0d\u662f\u57fa\u7763\u3001\u662f\u5949\u5dee\u9063\u5728\u4ed6\u524d\u9762\u7684\u3001\u4f60\u5011\u81ea\u5df1\u53ef\u4ee5\u7d66\u6211\u4f5c\u898b\u8b49\u3002"},{"id":29,"text":"\u5a36\u65b0\u5a66\u7684\u3001\u5c31\u662f\u65b0\u90ce\uff0e\u65b0\u90ce\u7684\u670b\u53cb\u7ad9\u8457\u807d\u898b\u65b0\u90ce\u7684\u8072\u97f3\u5c31\u751a\u559c\u6a02\uff0e\u6545\u6b64\u6211\u9019\u559c\u6a02\u6eff\u8db3\u4e86\u3002"},{"id":30,"text":"\u4ed6\u5fc5\u8208\u65fa\u3001\u6211\u5fc5\u8870\u5fae\u3002"},{"id":31,"text":"\u5f9e\u5929\u4e0b\u4f86\u7684\u3001\u662f\u5728\u842c\u6709\u4e4b\u4e0a\uff0e\u5f9e\u5730\u4e0a\u4f86\u7684\u3001\u662f\u5c6c\u4e4e\u5730\u3001\u4ed6\u6240\u8aaa\u7684\u3001\u4e5f\u662f\u5c6c\u4e4e\u5730\uff0e\u5f9e\u5929\u4e0a\u4f86\u7684\u3001\u662f\u5728\u842c\u6709\u4e4b\u4e0a\u3002"},{"id":32,"text":"\u4ed6\u5c07\u6240\u898b\u6240\u805e\u7684\u898b\u8b49\u51fa\u4f86\u3001\u53ea\u662f\u6c92\u6709\u4eba\u9818\u53d7\u4ed6\u7684\u898b\u8b49\u3002"},{"id":33,"text":"\u90a3\u9818\u53d7\u4ed6\u898b\u8b49\u7684\u3001\u5c31\u5370\u4e0a\u5370\u3001\u8b49\u660e\u3000\u795e\u662f\u771f\u7684\u3002"},{"id":34,"text":"\u795e\u6240\u5dee\u4f86\u7684\u3001\u5c31\u8aaa\u3000\u795e\u7684\u8a71\uff0e\u56e0\u70ba\u3000\u795e\u8cdc\u8056\u9748\u7d66\u4ed6\u3001\u662f\u6c92\u6709\u9650\u91cf\u7684\u3002"},{"id":35,"text":"\u7236\u611b\u5b50\u3001\u5df2\u5c07\u842c\u6709\u4ea4\u5728\u4ed6\u624b\u88e1\u3002"},{"id":36,"text":"\u4fe1\u5b50\u7684\u4eba\u6709\u6c38\u751f\uff0e\u4e0d\u4fe1\u5b50\u7684\u4eba\u5f97\u4e0d\u8457\u6c38\u751f\u3001\u3014\u539f\u6587\u4f5c\u4e0d\u5f97\u898b\u6c38\u751f\u3015\u3000\u795e\u7684\u9707\u6012\u5e38\u5728\u4ed6\u8eab\u4e0a\u3002"}]