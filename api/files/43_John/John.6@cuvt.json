[{"id":1,"text":"\u9019\u4e8b\u4ee5\u5f8c\u3001\u8036\u7a4c\u6e21\u904e\u52a0\u5229\u5229\u6d77\u3001\u5c31\u662f\u63d0\u6bd4\u54e9\u4e9e\u6d77\u3002"},{"id":2,"text":"\u6709\u8a31\u591a\u4eba\u3001\u56e0\u70ba\u770b\u898b\u4ed6\u5728\u75c5\u4eba\u8eab\u4e0a\u6240\u884c\u7684\u795e\u8e5f\u3001\u5c31\u8ddf\u96a8\u4ed6\u3002"},{"id":3,"text":"\u8036\u7a4c\u4e0a\u4e86\u5c71\u3001\u548c\u9580\u5f92\u4e00\u540c\u5750\u5728\u90a3\u88e1\u3002"},{"id":4,"text":"\u90a3\u6642\u7336\u592a\u4eba\u7684\u903e\u8d8a\u7bc0\u8fd1\u4e86\u3002"},{"id":5,"text":"\u8036\u7a4c\u8209\u76ee\u770b\u898b\u8a31\u591a\u4eba\u4f86\u3001\u5c31\u5c0d\u8153\u529b\u8aaa\u3001\u6211\u5011\u5f9e\u90a3\u88e1\u8cb7\u9905\u53eb\u9019\u4e9b\u4eba\u55ab\u5462\u3002"},{"id":6,"text":"\u4ed6\u8aaa\u9019\u8a71\u3001\u662f\u8981\u8a66\u9a57\u8153\u529b\uff0e\u4ed6\u81ea\u5df1\u539f\u77e5\u9053\u8981\u600e\u6a23\u884c\u3002"},{"id":7,"text":"\u8153\u529b\u56de\u7b54\u8aaa\u3001\u5c31\u662f\u4e8c\u5341\u5169\u9280\u5b50\u7684\u9905\u3001\u53eb\u4ed6\u5011\u5404\u4eba\u55ab\u4e00\u9ede\u3001\u4e5f\u662f\u4e0d\u5f40\u7684\u3002"},{"id":8,"text":"\u6709\u4e00\u500b\u9580\u5f92\u3001\u5c31\u662f\u897f\u9580\u5f7c\u5f97\u7684\u5144\u5f1f\u5b89\u5f97\u70c8\u3001\u5c0d\u8036\u7a4c\u8aaa\u3001"},{"id":9,"text":"\u5728\u9019\u88e1\u6709\u4e00\u500b\u5b69\u7ae5\u3001\u5e36\u8457\u4e94\u500b\u5927\u9ea5\u9905\u3001\u5169\u689d\u9b5a\uff0e\u53ea\u662f\u5206\u7d66\u9019\u8a31\u591a\u4eba\u3001\u9084\u7b97\u751a\u9ebc\u5462\u3002"},{"id":10,"text":"\u8036\u7a4c\u8aaa\u3001\u4f60\u5011\u53eb\u773e\u4eba\u5750\u4e0b\u3002\u539f\u4f86\u90a3\u5730\u65b9\u7684\u8349\u591a\u3001\u773e\u4eba\u5c31\u5750\u4e0b\u3001\u6578\u76ee\u7d04\u6709\u4e94\u5343\u3002"},{"id":11,"text":"\u8036\u7a4c\u62ff\u8d77\u9905\u4f86\u3001\u795d\u8b1d\u4e86\u3001\u5c31\u5206\u7d66\u90a3\u5750\u8457\u7684\u4eba\uff0e\u5206\u9b5a\u4e5f\u662f\u9019\u6a23\u3001\u90fd\u96a8\u8457\u4ed6\u5011\u6240\u8981\u7684\u3002"},{"id":12,"text":"\u4ed6\u5011\u55ab\u98fd\u4e86\u3001\u8036\u7a4c\u5c0d\u9580\u5f92\u8aaa\u3001\u628a\u5269\u4e0b\u7684\u96f6\u788e\u3001\u6536\u62fe\u8d77\u4f86\u3001\u514d\u5f97\u6709\u8e67\u8e4b\u7684\u3002"},{"id":13,"text":"\u4ed6\u5011\u4fbf\u5c07\u90a3\u4e94\u500b\u5927\u9ea5\u9905\u7684\u96f6\u788e\u3001\u5c31\u662f\u773e\u4eba\u55ab\u4e86\u5269\u4e0b\u7684\u3001\u6536\u62fe\u8d77\u4f86\u3001\u88dd\u6eff\u4e86\u5341\u4e8c\u500b\u7c43\u5b50\u3002"},{"id":14,"text":"\u773e\u4eba\u770b\u898b\u8036\u7a4c\u6240\u884c\u7684\u795e\u8e5f\u3001\u5c31\u8aaa\u3001\u9019\u771f\u662f\u90a3\u8981\u5230\u4e16\u9593\u4f86\u7684\u5148\u77e5\u3002"},{"id":15,"text":"\u8036\u7a4c\u65e2\u77e5\u9053\u773e\u4eba\u8981\u4f86\u5f37\u903c\u4ed6\u4f5c\u738b\u3001\u5c31\u7368\u81ea\u53c8\u9000\u5230\u5c71\u4e0a\u53bb\u4e86\u3002"},{"id":16,"text":"\u5230\u4e86\u665a\u4e0a\u3001\u4ed6\u7684\u9580\u5f92\u4e0b\u6d77\u908a\u53bb\u3001"},{"id":17,"text":"\u4e0a\u4e86\u8239\u3001\u8981\u904e\u6d77\u5f80\u8fe6\u767e\u8fb2\u53bb\u3002\u5929\u5df2\u7d93\u9ed1\u4e86\u3001\u8036\u7a4c\u9084\u6c92\u6709\u4f86\u5230\u4ed6\u5011\u90a3\u88e1\u3002"},{"id":18,"text":"\u5ffd\u7136\u72c2\u98a8\u5927\u4f5c\u3001\u6d77\u5c31\u7ffb\u9a30\u8d77\u4f86\u3002"},{"id":19,"text":"\u9580\u5f92\u6416\u6ad3\u7d04\u884c\u4e86\u5341\u91cc\u591a\u8def\u3001\u770b\u898b\u8036\u7a4c\u5728\u6d77\u9762\u4e0a\u8d70\u3001\u6f38\u6f38\u8fd1\u4e86\u8239\u3001\u4ed6\u5011\u5c31\u5bb3\u6015\u3002"},{"id":20,"text":"\u8036\u7a4c\u5c0d\u4ed6\u5011\u8aaa\u3001\u662f\u6211\uff0e\u4e0d\u8981\u6015\u3002"},{"id":21,"text":"\u9580\u5f92\u5c31\u559c\u6b61\u63a5\u4ed6\u4e0a\u8239\u3001\u8239\u7acb\u6642\u5230\u4e86\u4ed6\u5011\u6240\u8981\u53bb\u7684\u5730\u65b9\u3002"},{"id":22,"text":"\u7b2c\u4e8c\u65e5\u3001\u7ad9\u5728\u6d77\u90a3\u908a\u7684\u773e\u4eba\u3001\u77e5\u9053\u90a3\u88e1\u6c92\u6709\u5225\u7684\u8239\u3001\u53ea\u6709\u4e00\u96bb\u5c0f\u8239\u3001\u53c8\u77e5\u9053\u8036\u7a4c\u6c92\u6709\u540c\u4ed6\u7684\u9580\u5f92\u4e0a\u8239\u3001\u4e43\u662f\u9580\u5f92\u81ea\u5df1\u53bb\u7684\uff0e"},{"id":23,"text":"\u7136\u800c\u6709\u5e7e\u96bb\u5c0f\u8239\u5f9e\u63d0\u6bd4\u54e9\u4e9e\u4f86\u3001\u9760\u8fd1\u4e3b\u795d\u8b1d\u5f8c\u5206\u9905\u7d66\u4eba\u55ab\u7684\u5730\u65b9\u3002"},{"id":24,"text":"\u773e\u4eba\u898b\u8036\u7a4c\u548c\u9580\u5f92\u90fd\u4e0d\u5728\u90a3\u88e1\u3001\u5c31\u4e0a\u4e86\u8239\u3001\u5f80\u8fe6\u767e\u8fb2\u53bb\u627e\u8036\u7a4c\u3002"},{"id":25,"text":"\u65e2\u5728\u6d77\u90a3\u908a\u627e\u8457\u4e86\u3001\u5c31\u5c0d\u4ed6\u8aaa\u3001\u62c9\u6bd4\u3001\u662f\u5e7e\u6642\u5230\u9019\u88e1\u4f86\u7684\u3002"},{"id":26,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u5011\u3001\u4f60\u5011\u627e\u6211\u3001\u4e26\u4e0d\u662f\u56e0\u898b\u4e86\u795e\u8e5f\u3001\u4e43\u662f\u56e0\u55ab\u9905\u5f97\u98fd\u3002"},{"id":27,"text":"\u4e0d\u8981\u70ba\u90a3\u5fc5\u58de\u7684\u98df\u7269\u52de\u529b\u3001\u8981\u70ba\u90a3\u5b58\u5230\u6c38\u751f\u7684\u98df\u7269\u52de\u529b\u3001\u5c31\u662f\u4eba\u5b50\u8981\u8cdc\u7d66\u4f60\u5011\u7684\uff0e\u56e0\u70ba\u4eba\u5b50\u662f\u7236\u3000\u795e\u6240\u5370\u8b49\u7684\u3002"},{"id":28,"text":"\u773e\u4eba\u554f\u4ed6\u8aaa\u3001\u6211\u5011\u7576\u884c\u751a\u9ebc\u3001\u7e94\u7b97\u4f5c\u3000\u795e\u7684\u5de5\u5462\u3002"},{"id":29,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u4fe1\u3000\u795e\u6240\u5dee\u4f86\u7684\u3001\u9019\u5c31\u662f\u4f5c\u3000\u795e\u7684\u5de5\u3002"},{"id":30,"text":"\u4ed6\u5011\u53c8\u8aaa\u3001\u4f60\u884c\u751a\u9ebc\u795e\u8e5f\u3001\u53eb\u6211\u5011\u770b\u898b\u5c31\u4fe1\u4f60\uff0e\u4f60\u5230\u5e95\u4f5c\u751a\u9ebc\u4e8b\u5462\u3002"},{"id":31,"text":"\u6211\u5011\u7684\u7956\u5b97\u5728\u66e0\u91ce\u55ab\u904e\u55ce\u54ea\u3001\u5982\u7d93\u4e0a\u5beb\u8457\u8aaa\u3001\u300e\u4ed6\u5f9e\u5929\u4e0a\u8cdc\u4e0b\u7ce7\u4f86\u7d66\u4ed6\u5011\u55ab\u3002\u300f"},{"id":32,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u5011\u3001\u90a3\u5f9e\u5929\u4e0a\u4f86\u7684\u7ce7\u3001\u4e0d\u662f\u6469\u897f\u8cdc\u7d66\u4f60\u5011\u7684\u3001\u4e43\u662f\u6211\u7236\u5c07\u5929\u4e0a\u4f86\u7684\u771f\u7ce7\u8cdc\u7d66\u4f60\u5011\u3002"},{"id":33,"text":"\u56e0\u70ba\u3000\u795e\u7684\u7ce7\u3001\u5c31\u662f\u90a3\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u8cdc\u751f\u547d\u7d66\u4e16\u754c\u7684\u3002"},{"id":34,"text":"\u4ed6\u5011\u8aaa\u3001\u4e3b\u963f\u3001\u5e38\u5c07\u9019\u7ce7\u8cdc\u7d66\u6211\u5011\u3002"},{"id":35,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u5c31\u662f\u751f\u547d\u7684\u7ce7\uff0e\u5230\u6211\u9019\u88e1\u4f86\u7684\u3001\u5fc5\u5b9a\u4e0d\u9913\uff0e\u4fe1\u6211\u7684\u3001\u6c38\u9060\u4e0d\u6e34\u3002"},{"id":36,"text":"\u53ea\u662f\u6211\u5c0d\u4f60\u5011\u8aaa\u904e\u3001\u4f60\u5011\u5df2\u7d93\u770b\u898b\u6211\u3001\u9084\u662f\u4e0d\u4fe1\u3002"},{"id":37,"text":"\u51e1\u7236\u6240\u8cdc\u7d66\u6211\u7684\u4eba\u3001\u5fc5\u5230\u6211\u9019\u88e1\u4f86\uff0e\u5230\u6211\u9019\u88e1\u4f86\u7684\u3001\u6211\u7e3d\u4e0d\u4e1f\u68c4\u4ed6\u3002"},{"id":38,"text":"\u56e0\u70ba\u6211\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u3001\u4e0d\u662f\u8981\u6309\u81ea\u5df1\u7684\u610f\u601d\u884c\u3001\u4e43\u662f\u8981\u6309\u90a3\u5dee\u6211\u4f86\u8005\u7684\u610f\u601d\u884c\u3002"},{"id":39,"text":"\u5dee\u6211\u4f86\u8005\u7684\u610f\u601d\u3001\u5c31\u662f\u4ed6\u6240\u8cdc\u7d66\u6211\u7684\u3001\u53eb\u6211\u4e00\u500b\u4e5f\u4e0d\u5931\u843d\u3001\u5728\u672b\u65e5\u537b\u53eb\u4ed6\u5fa9\u6d3b\u3002"},{"id":40,"text":"\u56e0\u70ba\u6211\u7236\u7684\u610f\u601d\u3001\u662f\u53eb\u4e00\u5207\u898b\u5b50\u800c\u4fe1\u7684\u4eba\u5f97\u6c38\u751f\uff0e\u4e26\u4e14\u5728\u672b\u65e5\u6211\u8981\u53eb\u4ed6\u5fa9\u6d3b\u3002"},{"id":41,"text":"\u7336\u592a\u4eba\u56e0\u70ba\u8036\u7a4c\u8aaa\u3001\u6211\u662f\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u7684\u7ce7\u3001\u5c31\u79c1\u4e0b\u8b70\u8ad6\u4ed6\uff0e"},{"id":42,"text":"\u8aaa\u3001\u9019\u4e0d\u662f\u7d04\u745f\u7684\u5152\u5b50\u8036\u7a4c\u9ebc\uff0e\u4ed6\u7684\u7236\u6bcd\u6211\u5011\u8c48\u4e0d\u8a8d\u5f97\u9ebc\uff0e\u4ed6\u5982\u4eca\u600e\u9ebc\u8aaa\u3001\u6211\u662f\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u7684\u5462\u3002"},{"id":43,"text":"\u8036\u7a4c\u56de\u7b54\u8aaa\u3001\u4f60\u5011\u4e0d\u8981\u5927\u5bb6\u8b70\u8ad6\u3002"},{"id":44,"text":"\u82e5\u4e0d\u662f\u5dee\u6211\u4f86\u7684\u7236\u5438\u5f15\u4eba\u3001\u5c31\u6c92\u6709\u80fd\u5230\u6211\u9019\u88e1\u4f86\u7684\uff0e\u5230\u6211\u9019\u88e1\u4f86\u7684\u3001\u5728\u672b\u65e5\u6211\u8981\u53eb\u4ed6\u5fa9\u6d3b\u3002"},{"id":45,"text":"\u5728\u5148\u77e5\u66f8\u4e0a\u5beb\u8457\u8aaa\u3001\u300e\u4ed6\u5011\u90fd\u8981\u8499\u3000\u795e\u7684\u6559\u8a13\u3002\u300f\u51e1\u807d\u898b\u7236\u4e4b\u6559\u8a13\u53c8\u5b78\u7fd2\u7684\u3001\u5c31\u5230\u6211\u9019\u88e1\u4f86\u3002"},{"id":46,"text":"\u9019\u4e0d\u662f\u8aaa\u3001\u6709\u4eba\u770b\u898b\u904e\u7236\u3001\u60df\u7368\u5f9e\u3000\u795e\u4f86\u7684\u3001\u4ed6\u770b\u898b\u904e\u7236\u3002"},{"id":47,"text":"\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u5011\u3001\u4fe1\u7684\u4eba\u6709\u6c38\u751f\u3002"},{"id":48,"text":"\u6211\u5c31\u662f\u751f\u547d\u7684\u7ce7\u3002"},{"id":49,"text":"\u4f60\u5011\u7684\u7956\u5b97\u5728\u66e0\u91ce\u55ab\u904e\u55ce\u54ea\u3001\u9084\u662f\u6b7b\u4e86\u3002"},{"id":50,"text":"\u9019\u662f\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u7684\u7ce7\u3001\u53eb\u4eba\u55ab\u4e86\u5c31\u4e0d\u6b7b\u3002"},{"id":51,"text":"\u6211\u662f\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u751f\u547d\u7684\u7ce7\uff0e\u4eba\u82e5\u55ab\u9019\u7ce7\u3001\u5c31\u5fc5\u6c38\u9060\u6d3b\u8457\uff0e\u6211\u6240\u8981\u8cdc\u7684\u7ce7\u3001\u5c31\u662f\u6211\u7684\u8089\u3001\u70ba\u4e16\u4eba\u4e4b\u751f\u547d\u6240\u8cdc\u7684\u3002"},{"id":52,"text":"\u56e0\u6b64\u3001\u7336\u592a\u4eba\u5f7c\u6b64\u722d\u8ad6\u8aaa\u3001\u9019\u500b\u4eba\u600e\u80fd\u628a\u4ed6\u7684\u8089\u3001\u7d66\u6211\u5011\u55ab\u5462\u3002"},{"id":53,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u5be6\u5be6\u5728\u5728\u7684\u544a\u8a34\u4f60\u5011\u3001\u4f60\u5011\u82e5\u4e0d\u55ab\u4eba\u5b50\u7684\u8089\u3001\u4e0d\u559d\u4eba\u5b50\u7684\u8840\u3001\u5c31\u6c92\u6709\u751f\u547d\u5728\u4f60\u5011\u88e1\u9762\u3002"},{"id":54,"text":"\u55ab\u6211\u8089\u559d\u6211\u8840\u7684\u4eba\u5c31\u6709\u6c38\u751f\uff0e\u5728\u672b\u65e5\u6211\u8981\u53eb\u4ed6\u5fa9\u6d3b\u3002"},{"id":55,"text":"\u6211\u7684\u8089\u771f\u662f\u53ef\u55ab\u7684\u3001\u6211\u7684\u8840\u771f\u662f\u53ef\u559d\u7684\u3002"},{"id":56,"text":"\u55ab\u6211\u8089\u559d\u6211\u8840\u7684\u4eba\u3001\u5e38\u5728\u6211\u88e1\u9762\u3001\u6211\u4e5f\u5e38\u5728\u4ed6\u88e1\u9762\u3002"},{"id":57,"text":"\u6c38\u6d3b\u7684\u7236\u600e\u6a23\u5dee\u6211\u4f86\u3001\u6211\u53c8\u56e0\u7236\u6d3b\u8457\u3001\u7167\u6a23\u3001\u55ab\u6211\u8089\u7684\u4eba\u3001\u4e5f\u8981\u56e0\u6211\u6d3b\u8457\u3002"},{"id":58,"text":"\u9019\u5c31\u662f\u5f9e\u5929\u4e0a\u964d\u4e0b\u4f86\u7684\u7ce7\uff0e\u55ab\u9019\u7ce7\u7684\u4eba\u3001\u5c31\u6c38\u9060\u6d3b\u8457\u3001\u4e0d\u50cf\u4f60\u5011\u7684\u7956\u5b97\u55ab\u904e\u55ce\u54ea\u3001\u9084\u662f\u6b7b\u4e86\u3002"},{"id":59,"text":"\u9019\u4e9b\u8a71\u662f\u8036\u7a4c\u5728\u8fe6\u767e\u8fb2\u6703\u5802\u88e1\u6559\u8a13\u4eba\u8aaa\u7684\u3002"},{"id":60,"text":"\u4ed6\u7684\u9580\u5f92\u4e2d\u6709\u597d\u4e9b\u4eba\u807d\u898b\u4e86\u3001\u5c31\u8aaa\u3001\u9019\u8a71\u751a\u96e3\u3001\u8ab0\u80fd\u807d\u5462\u3002"},{"id":61,"text":"\u8036\u7a4c\u5fc3\u88e1\u77e5\u9053\u9580\u5f92\u70ba\u9019\u8a71\u8b70\u8ad6\u3001\u5c31\u5c0d\u4ed6\u5011\u8aaa\u3001\u9019\u8a71\u662f\u53eb\u4f60\u5011\u53ad\u68c4\u9ebc\u3002\u3014\u53ad\u68c4\u539f\u6587\u4f5c\u8dcc\u5012\u3015"},{"id":62,"text":"\u5018\u6216\u4f60\u5011\u770b\u898b\u4eba\u5b50\u5347\u5230\u4ed6\u539f\u4f86\u6240\u5728\u4e4b\u8655\u3001\u600e\u9ebc\u6a23\u5462\u3002"},{"id":63,"text":"\u53eb\u4eba\u6d3b\u8457\u7684\u4e43\u662f\u9748\u3001\u8089\u9ad4\u662f\u7121\u76ca\u7684\uff0e\u6211\u5c0d\u4f60\u5011\u6240\u8aaa\u7684\u8a71\u3001\u5c31\u662f\u9748\u3001\u5c31\u662f\u751f\u547d\u3002"},{"id":64,"text":"\u53ea\u662f\u4f60\u5011\u4e2d\u9593\u6709\u4e0d\u4fe1\u7684\u4eba\u3002\u8036\u7a4c\u5f9e\u8d77\u982d\u5c31\u77e5\u9053\u3001\u8ab0\u4e0d\u4fe1\u4ed6\u3001\u8ab0\u8981\u8ce3\u4ed6\u3002"},{"id":65,"text":"\u8036\u7a4c\u53c8\u8aaa\u3001\u6240\u4ee5\u6211\u5c0d\u4f60\u5011\u8aaa\u904e\u3001\u82e5\u4e0d\u662f\u8499\u6211\u7236\u7684\u6069\u8cdc\u3001\u6c92\u6709\u4eba\u80fd\u5230\u6211\u9019\u88e1\u4f86\u3002"},{"id":66,"text":"\u5f9e\u6b64\u4ed6\u9580\u5f92\u4e2d\u591a\u6709\u9000\u53bb\u7684\u3001\u4e0d\u518d\u548c\u4ed6\u540c\u884c\u3002"},{"id":67,"text":"\u8036\u7a4c\u5c31\u5c0d\u90a3\u5341\u4e8c\u500b\u9580\u5f92\u8aaa\u3001\u4f60\u5011\u4e5f\u8981\u53bb\u9ebc\u3002"},{"id":68,"text":"\u897f\u9580\u5f7c\u5f97\u56de\u7b54\u8aaa\u3001\u4e3b\u963f\u3001\u4f60\u6709\u6c38\u751f\u4e4b\u9053\u3001\u6211\u5011\u9084\u6b78\u5f9e\u8ab0\u5462\u3002"},{"id":69,"text":"\u6211\u5011\u5df2\u7d93\u4fe1\u4e86\u3001\u53c8\u77e5\u9053\u4f60\u662f\u3000\u795e\u7684\u8056\u8005\u3002"},{"id":70,"text":"\u8036\u7a4c\u8aaa\u3001\u6211\u4e0d\u662f\u63c0\u9078\u4e86\u4f60\u5011\u5341\u4e8c\u500b\u9580\u5f92\u9ebc\uff0e\u4f46\u4f60\u5011\u4e2d\u9593\u6709\u4e00\u500b\u662f\u9b54\u9b3c\u3002"},{"id":71,"text":"\u8036\u7a4c\u9019\u8a71\u662f\u6307\u8457\u52a0\u7565\u4eba\u897f\u9580\u7684\u5152\u5b50\u7336\u5927\u8aaa\u7684\uff0e\u4ed6\u672c\u662f\u5341\u4e8c\u500b\u9580\u5f92\u88e1\u7684\u4e00\u500b\u3001\u5f8c\u4f86\u8981\u8ce3\u8036\u7a4c\u7684\u3002"}]